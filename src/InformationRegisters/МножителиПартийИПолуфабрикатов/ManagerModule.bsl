		ПроизводствоСервер.ВыполнитьПодстановкуАлгоритмаРасчетаДолейСтоимости(ТекстЗапроса, ПараметрыПодстановки);
		
		ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "&ТекстЗапросаКоэффициентУпаковки1",
			Справочники.УпаковкиЕдиницыИзмерения.ТекстЗапросаКоэффициентаУпаковки(
				"Изделия.Упаковка",
				"Изделия.Номенклатура"));
		
		ТекстЗапроса = СтрЗаменить(ТекстЗапроса, "&ТекстЗапросаКоэффициентУпаковки2",
			Справочники.УпаковкиЕдиницыИзмерения.ТекстЗапросаКоэффициентаУпаковки(
				"Изделия.Упаковка",
				"ПФПроизводимыеВПроцессе.Номенклатура"));
			
		Запрос.Текст = ТекстЗапроса;
		
		ВидЦены = Справочники.ВидыЦен.ВидЦеныПлановойСтоимостиТМЦ();
		Запрос.УстановитьПараметр("ВидЦены", ВидЦены);
		Запрос.УстановитьПараметр("ДатаКалькуляции", НачалоМесяца(Калькуляция.Дата));
		Запрос.УстановитьПараметр("Дата", НачалоМесяца(Калькуляция.Дата));
		Запрос.УстановитьПараметр("Валюта", ОбщегоНазначения.ЗначениеРеквизитаОбъекта(ВидЦены, "ВалютаЦены"));
		Запрос.УстановитьПараметр("БазоваяВалюта", ЗначениеНастроекПовтИсп.БазоваяВалютаПоУмолчанию());
		
		МассивРезультатов = Запрос.ВыполнитьПакет();
		
