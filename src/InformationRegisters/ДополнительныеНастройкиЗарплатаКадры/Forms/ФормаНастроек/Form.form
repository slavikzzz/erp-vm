      <id>4</id>
      <visible>true</visible>
      <enabled>true</enabled>
      <userVisible>
        <common>true</common>
      </userVisible>
      <dataPath xsi:type="form:DataPath">
        <segments>Запись.УдалятьПрефиксыОрганизацииИИБИзНомеровКадровыхПриказов</segments>
      </dataPath>
      <titleLocation>Right</titleLocation>
