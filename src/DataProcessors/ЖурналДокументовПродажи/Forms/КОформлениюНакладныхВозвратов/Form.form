    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокРаспоряженияНаОформление.СостояниеОрдера</segments>
    </notDefaultUseAlwaysAttributes>
    <notDefaultUseAlwaysAttributes xsi:type="form:DataPath">
      <segments>СписокРаспоряженияНаОформление.Ссылка</segments>
