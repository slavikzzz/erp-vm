	ИмяПКО = ТипОбъектаИмяПКО[ТипОбъекта];
	ПКО = ПравилоКонвертацииОбъекта(ИмяПКО);
	Если Не ЗначениеЗаполнено(ПКО) Тогда
		Возврат ИменаСвойств;
	КонецЕсли;
	
	Для каждого ПКС Из ПКО.ПравилаКонвертацииСвойств Цикл
		Если ПКС.ИмяПКО = "Файл" Тогда
			ИменаСвойств.Добавить(ПКС.ИмяСвойстваКонфигурации);
		КонецЕсли;
	КонецЦикла;
	
	Возврат ИменаСвойств;

КонецФункции

Процедура ЗаполнитьИдентификаторыОбъектовИзКоллекции(КоллекцияОбъектов, СвойстваПубличнаяСсылка)

	ТипОбъектаСсылки = СсылкиОбъектовКоллекции(КоллекцияОбъектов, СвойстваПубличнаяСсылка);
	Для каждого КлючИзначение Из ТипОбъектаСсылки Цикл
		ТипОбъекта = КлючИЗначение.Ключ;
		МассивСсылок = ОбщегоНазначенияБЗККлиентСервер.КлючиСоответствия(КлючИЗначение.Значение);
		ЗаполнитьКэшСсылкаИдентификатор(МассивСсылок, ТипОбъекта);
	КонецЦикла;

КонецПроцедуры

Функция СсылкиОбъектовКоллекции(КоллекцияОбъектов, СвойстваПубличнаяСсылка)
	
	ТипОбъектаСсылки = Новый Соответствие;
	
	Для каждого КлючИЗначение Из СвойстваПубличнаяСсылка Цикл
		
		ИмяСвойства = КлючИЗначение.Ключ;
		
		Если ТипЗнч(КлючИЗначение.Значение) = Тип("Соответствие") Тогда
			ВложеннаяКолекция = Неопределено;
			Для каждого ОбъектКоллекции Из КоллекцияОбъектов Цикл
				ЗначениеСвойства = ОбъектКоллекции[ИмяСвойства];
				Если Не ЗначениеЗаполнено(ЗначениеСвойства) Тогда
					Продолжить;
				КонецЕсли;
				Если ВложеннаяКолекция = Неопределено Тогда
					Если ТипЗнч(ЗначениеСвойства) = Тип("ТаблицаЗначений") Тогда
						ВложеннаяКолекция = ЗначениеСвойства.СкопироватьКолонки();
					Иначе
						ВложеннаяКолекция = Новый Массив;
					КонецЕсли;
				КонецЕсли;
				Если ТипЗнч(ЗначениеСвойства) = Тип("ТаблицаЗначений") Тогда
					ОбщегоНазначенияКлиентСервер.ДополнитьТаблицу(ЗначениеСвойства, ВложеннаяКолекция);
				ИначеЕсли ТипЗнч(ЗначениеСвойства) = Тип("Массив") Тогда
					ОбщегоНазначенияКлиентСервер.ДополнитьМассив(ВложеннаяКолекция, ЗначениеСвойства);
				Иначе
					ВложеннаяКолекция.Добавить(ЗначениеСвойства);
				КонецЕсли;
			КонецЦикла;
			Если ЗначениеЗаполнено(ВложеннаяКолекция) Тогда
				ВложенныеТипОбъектаСсылки = СсылкиОбъектовКоллекции(ВложеннаяКолекция, КлючИЗначение.Значение);
				Для каждого ЭлементКоллекции Из ВложенныеТипОбъектаСсылки Цикл
					ТипОбъекта 			= ЭлементКоллекции.Ключ;
					СсылкиТипаОбъекта 	= ЭлементКоллекции.Значение;
					Ссылки = ТипОбъектаСсылки[ЭлементКоллекции.Ключ];
					Если Не ЗначениеЗаполнено(Ссылки) Тогда
						Ссылки = Новый Соответствие;
					КонецЕсли;
					ОбщегоНазначенияКлиентСервер.ДополнитьСоответствие(Ссылки, СсылкиТипаОбъекта, Ложь);
				КонецЦикла;
			КонецЕсли;
			
		Иначе
			
			ТипОбъекта = КлючИЗначение.Значение;
			Ссылки = ТипОбъектаСсылки[ТипОбъекта];
			Если Не ЗначениеЗаполнено(Ссылки) Тогда
				Ссылки = Новый Соответствие;
			КонецЕсли;
			Для каждого ОбъектКоллекции Из КоллекцияОбъектов Цикл
				Значение = ОбъектКоллекции[ИмяСвойства];
				Если ТипЗнч(Значение) = Тип("Массив") Тогда
					Для каждого Ссылка Из Значение Цикл
						Ссылки.Вставить(Ссылка);
					КонецЦикла;
				Иначе
					Ссылки.Вставить(Значение);
				КонецЕсли;
			КонецЦикла;
			
		КонецЕсли;
		
		Если ЗначениеЗаполнено(Ссылки) Тогда
			ТипОбъектаСсылки.Вставить(ТипОбъекта, Ссылки);
		КонецЕсли;
		
	КонецЦикла;
	
	Возврат ТипОбъектаСсылки;
	
КонецФункции

Функция СвойстваПКЗПубличнаяСсылкаВыгрузка(ИмяПКО)
	
	СвойстваПубличнаяСсылка = Новый Соответствие;
	
	ПКО = ПравилоКонвертацииОбъекта(ИмяПКО);
	Если Не ЗначениеЗаполнено(ПКО) Тогда
		Возврат СвойстваПубличнаяСсылка;
	КонецЕсли;
	
	НеЗаполнятьСвойства = ПКО.НеЗаполнятьСвойства[Приложение];
	Для каждого ПКС Из ПКО.ПравилаКонвертацииСвойств Цикл
		Если НеЗаполнятьСвойства[ПКС.ИмяСвойстваDTO] = Истина Тогда
			Продолжить;
		КонецЕсли;
		Если ЗначениеЗаполнено(ПКС.ИмяПКЗ) Тогда
			ПКЗ = ПравилоКонвертацииЗначения(ПКС.ИмяПКЗ);
			Если Не ЗначениеЗаполнено(ПКЗ) Тогда
				Продолжить;
			КонецЕсли;
			Если ПКЗ.Выгрузка.ПубличныйИдентификатор Тогда
				СвойстваПубличнаяСсылка.Вставить(ПКС.ИмяСвойстваКонфигурации, ПКЗ.Выгрузка.ТипОбъекта);
			КонецЕсли;
		ИначеЕсли ЗначениеЗаполнено(ПКС.ИмяПКО) Тогда
			ВложенныеСвойстваПубличнаяСсылка = СвойстваПКЗПубличнаяСсылкаВыгрузка(ПКС.ИмяПКО);
			Если ЗначениеЗаполнено(ВложенныеСвойстваПубличнаяСсылка) Тогда
				СвойстваПубличнаяСсылка.Вставить(ПКС.ИмяСвойстваКонфигурации, ВложенныеСвойстваПубличнаяСсылка);
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	
	Возврат СвойстваПубличнаяСсылка;
	
КонецФункции

Процедура ЗаполнитьТипЗначенияТипОбъекта()
	
	ПеречислениеМенеджер = Перечисления.ТипыОбъектовИнтеграцияУправлениеПерсоналом;
	
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.ФизическиеЛица"), 					ПеречислениеМенеджер.ФизическоеЛицо);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.Организации"), 					ПеречислениеМенеджер.Организация);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.ПодразделенияОрганизаций"), 		ПеречислениеМенеджер.Подразделение);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.СтруктураПредприятия"), 			ПеречислениеМенеджер.Подразделение);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.Должности"), 						ПеречислениеМенеджер.Должность);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.ШтатноеРасписание"), 				ПеречислениеМенеджер.ДолжностьПоШтатномуРасписанию);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.Сотрудники"), 						ПеречислениеМенеджер.Сотрудник);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.МашиночитаемыеДоверенности"),	 	ПеречислениеМенеджер.МашиночитаемаяДоверенность);
	ТипЗначенияТипОбъекта.Вставить(Тип("СправочникСсылка.ШаблоныДокументов"), 				ПеречислениеМенеджер.ШаблонДокумента);
	
	ТипЗначения = ИнтеграцияУправлениеПерсоналом.ТипВидПредоставляемойСотрудникамСправкиСсылка();
	Если ЗначениеЗаполнено(ТипЗначения) Тогда
		ТипЗначенияТипОбъекта.Вставить(ТипЗначения, ПеречислениеМенеджер.ВидПредоставляемойСотрудникамСправки);
	КонецЕсли;
	ТипЗначения = ИнтеграцияУправлениеПерсоналом.ТипГрафикРаботыСсылка();
	Если ЗначениеЗаполнено(ТипЗначения) Тогда
		ТипЗначенияТипОбъекта.Вставить(ТипЗначения, ПеречислениеМенеджер.ГрафикРаботы);
	КонецЕсли;
	ТипЗначения = ИнтеграцияУправлениеПерсоналом.ТипСборГрафиковОтпусковСсылка();
	Если ЗначениеЗаполнено(ТипЗначения) Тогда
		ТипЗначенияТипОбъекта.Вставить(ТипЗначения, ПеречислениеМенеджер.ГрафикОтпусковПредприятия);
	КонецЕсли;

КонецПроцедуры

