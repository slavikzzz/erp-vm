      <common>true</common>
    </edit>
    <functionalOptions>FunctionalOption.ИспользоватьУчетПоОбъектамВозникновенияЗатрат</functionalOptions>
    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ РАЗЛИЧНЫЕ РАЗРЕШЕННЫЕ
	ЕСТЬNULL(ЗначенияПриИзменении.Период, &amp;НачалоМесяца) КАК Период,
	ВЫРАЗИТЬ(ОбъектыВозникновенияЗатрат.Ссылка КАК Справочник.ОбъектыВозникновенияЗатрат) КАК ОВЗ,
	ЕСТЬNULL(ЗначенияЕжемесячно.Показатель, ЕСТЬNULL(ЗначенияПриИзменении.Показатель,
		Значение(Справочник.ПравилаРаспределенияРасходов.ПустаяСсылка))) КАК Правило,
	ЕСТЬNULL(ЗначенияЕжемесячно.ЗначениеПоказателя, ЕСТЬNULL(ЗначенияПриИзменении.ЗначениеПоказателя, 0)) КАК Показатель,
	ВЫБОР
		КОГДА ЕСТЬNULL(ЗначенияЕжемесячно.ЗначениеПоказателя, ЕСТЬNULL(ЗначенияПриИзменении.ЗначениеПоказателя, 0)) = 0
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК НеПустоеЗначение,
	ВЫРАЗИТЬ(ВЫБОР
		КОГДА ЕСТЬNULL(ЗначенияЕжемесячноПрошлыхПериодов.ЗначениеПоказателя, 0) = 0
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК БУЛЕВО) КАК ВводилсяВПрошломПериоде,
	ЕСТЬNULL(ЗначенияЕжемесячноПрошлыхПериодов.ЗначениеПоказателя, 0) КАК ЗначениеПрошлогоПериода
ИЗ
	Справочник.ОбъектыВозникновенияЗатрат КАК ОбъектыВозникновенияЗатрат
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЗначенияПоказателейДляРаспределенияРасходовНаОВЗ КАК ЗначенияЕжемесячно
		ПО ЗначенияЕжемесячно.ОВЗ = ОбъектыВозникновенияЗатрат.Ссылка
		И
			ЗначенияЕжемесячно.Показатель.БазаРаспределения = ЗНАЧЕНИЕ(Перечисление.ТипыБазыРаспределенияРасходов.ВводитсяЕжемесячно)
		И НАЧАЛОПЕРИОДА(ЗначенияЕжемесячно.Период, Месяц) = &amp;НачалоМесяца
		И ЗначенияЕжемесячно.Показатель = &amp;Показатель
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЗначенияПоказателейДляРаспределенияРасходовНаОВЗ КАК
			ЗначенияЕжемесячноПрошлыхПериодов
		ПО ЗначенияЕжемесячноПрошлыхПериодов.ОВЗ = ОбъектыВозникновенияЗатрат.Ссылка
		И
			ЗначенияЕжемесячноПрошлыхПериодов.Показатель.БазаРаспределения = ЗНАЧЕНИЕ(Перечисление.ТипыБазыРаспределенияРасходов.ВводитсяЕжемесячно)
		И ЗначенияЕжемесячноПрошлыхПериодов.Показатель = &amp;Показатель
		И НАЧАЛОПЕРИОДА(ЗначенияЕжемесячноПрошлыхПериодов.Период, Месяц) = ДобавитьКДате(&amp;НачалоМесяца, МЕСЯЦ, -1)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЗначенияПоказателейДляРаспределенияРасходовНаОВЗ.СрезПоследних(&amp;НачалоМесяца) КАК
			ЗначенияПриИзменении
		ПО ЗначенияПриИзменении.ОВЗ = ОбъектыВозникновенияЗатрат.Ссылка
		И
			ЗначенияПриИзменении.Показатель.БазаРаспределения = ЗНАЧЕНИЕ(Перечисление.ТипыБазыРаспределенияРасходов.ВводитсяПриИзменении)
		И ЗначенияПриИзменении.Показатель = &amp;Показатель
ГДЕ
	НЕ ОбъектыВозникновенияЗатрат.НеИспользуется
	И &amp;Показатель &lt;> Значение(Справочник.ПравилаРаспределенияРасходов.ПустаяСсылка)</queryText>
      <autoFillAvailableFields>true</autoFillAvailableFields>
      <customQuery>true</customQuery>
      <autoSaveUserSettings>true</autoSaveUserSettings>
