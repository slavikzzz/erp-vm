    <title>
      <key>ru</key>
      <value>Значения к заполнению на ОВЗ</value>
    </title>
    <title>
      <key>en</key>
      <value>Values for population on cost centers</value>
    </title>
    <id>11</id>
    <valueType>
      <types>DynamicList</types>
    </valueType>
