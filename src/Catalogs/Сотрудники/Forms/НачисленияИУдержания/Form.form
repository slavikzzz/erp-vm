  <attributes>
    <name>НачислениеЗарплаты</name>
    <title>
      <key>ru</key>
      <value>Начисление зарплаты</value>
    </title>
    <title>
      <key>en</key>
      <value>Payroll accrual</value>
    </title>
    <id>1</id>
