	Элемент = Элементы.Добавить();
	Элемент.Наименование              = НСтр("ru = 'Общие условия налогообложения';
											|en = 'General taxation terms'");
	Элемент.ИмяПредопределенныхДанных = "ОбщиеУсловияНалогообложения";
	Элемент.КодМестаПредставления     = "214";
	Элемент.ЛистДекларации            = "02";
	Элемент.ПризнакНалогоплательщика  = "01";
	
КонецПроцедуры

