		<dataSource>ИсточникДанных1</dataSource>
		<query>// ВНИМАНИЕ! ИМЕЮТСЯ КОММЕНТАРИИ, КРИТИЧНЫЕ ДЛЯ РАБОТЫ

ВЫБРАТЬ
    &amp;НачалоПериода КАК НачалоПериодаПлан,
    &amp;КонецПериода КАК КонецПериодаПлан,
    
    ТаблицаПлана.СтатьяБюджетов КАК СтатьяБюджетовПлан,
    
    &amp;ПоСценариям КАК ПоСценариямПлан,
    &amp;ПоОрганизациям КАК ПоОрганизациямПлан,
    &amp;ПоПодразделениям КАК ПоПодразделениямПлан,
    
    &amp;ВидАналитики1 КАК ВидАналитики1План,
    &amp;ВидАналитики2 КАК ВидАналитики2План,
    &amp;ВидАналитики3 КАК ВидАналитики3План,
    &amp;ВидАналитики4 КАК ВидАналитики4План,
    &amp;ВидАналитики5 КАК ВидАналитики5План,
    &amp;ВидАналитики6 КАК ВидАналитики6План,
    
    // Если в правиле контроля лимитов не установлен конкретный сценарий, то используются данные по всем сценариям 
    ВЫБОР
        КОГДА НЕ &amp;ПоСценариям
            ТОГДА ЗНАЧЕНИЕ(Справочник.Сценарии.ПустаяСсылка)
        ИНАЧЕ ТаблицаПлана.Сценарий
    КОНЕЦ КАК СценарийПлан,
    
    ВЫБОР
        КОГДА НЕ &amp;ПоОрганизациям
            ТОГДА ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
        ИНАЧЕ ТаблицаПлана.Организация
    КОНЕЦ КАК ОрганизацияПлан,
    
    ВЫБОР
        КОГДА НЕ &amp;ПоПодразделениям
            ТОГДА ЗНАЧЕНИЕ(Справочник.СтруктураПредприятия.ПустаяСсылка)
        ИНАЧЕ ТаблицаПлана.Подразделение
    КОНЕЦ КАК ПодразделениеПлан,
    
    ВЫБОР
        КОГДА &amp;ВидАналитики1 = НЕОПРЕДЕЛЕНО
            ТОГДА НЕОПРЕДЕЛЕНО
        ИНАЧЕ ТаблицаПлана.Аналитика1
    КОНЕЦ КАК Аналитика1План,
    
    ВЫБОР
        КОГДА &amp;ВидАналитики2 = НЕОПРЕДЕЛЕНО
            ТОГДА НЕОПРЕДЕЛЕНО
        ИНАЧЕ ТаблицаПлана.Аналитика2
    КОНЕЦ КАК Аналитика2План,
    
    ВЫБОР
        КОГДА &amp;ВидАналитики3 = НЕОПРЕДЕЛЕНО
            ТОГДА НЕОПРЕДЕЛЕНО
        ИНАЧЕ ТаблицаПлана.Аналитика3
    КОНЕЦ КАК Аналитика3План,
    
    ВЫБОР
        КОГДА &amp;ВидАналитики4 = НЕОПРЕДЕЛЕНО
            ТОГДА НЕОПРЕДЕЛЕНО
        ИНАЧЕ ТаблицаПлана.Аналитика4
    КОНЕЦ КАК Аналитика4План,
    
    ВЫБОР
        КОГДА &amp;ВидАналитики5 = НЕОПРЕДЕЛЕНО
            ТОГДА НЕОПРЕДЕЛЕНО
        ИНАЧЕ ТаблицаПлана.Аналитика5
    КОНЕЦ КАК Аналитика5План,
    
    ВЫБОР
        КОГДА &amp;ВидАналитики6 = НЕОПРЕДЕЛЕНО
            ТОГДА НЕОПРЕДЕЛЕНО
        ИНАЧЕ ТаблицаПлана.Аналитика6
    КОНЕЦ КАК Аналитика6План,
    
    ТаблицаПлана.Валюта КАК ВалютаПлан,
    
    //ВыражениеПоляРегистраторДляРасшифровки
    
    ТаблицаПлана.СуммаВВалютеОборот КАК СуммаПлан
    
{ВЫБРАТЬ
    НачалоПериодаПлан,
    КонецПериодаПлан,
    
    СтатьяБюджетовПлан,
    
    ПоСценариямПлан,
    ПоОрганизациямПлан,
    ПоПодразделениямПлан,
    
    ВидАналитики1План,
    ВидАналитики2План,
    ВидАналитики3План,
    ВидАналитики4План,
    ВидАналитики5План,
    ВидАналитики6План,
    
    СценарийПлан.*,
    ОрганизацияПлан.*,
    ПодразделениеПлан.*,
    
    Аналитика1План.*,
    Аналитика2План.*,
    Аналитика3План.*,
    Аналитика4План.*,
    Аналитика5План.*,
    Аналитика6План.*,
    
    ВалютаПлан,
    
    //ПолеРегистраторДляРасшифровки
    
    СуммаПлан}
ИЗ
    РегистрНакопления.ОборотыБюджетов.Обороты(
            {(&amp;ПустаяДата)},
            {(&amp;ПустаяДата)},
            //ПериодичностьРегистраторДляРасшифровки
            ,
            ПериодПланирования МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
                И Статус = &amp;Статус
                И СтатьяБюджетов = &amp;СтатьяБюджетов
                И (Сценарий = &amp;Сценарий ИЛИ НЕ &amp;ПоСценариям)
            ) КАК ТаблицаПлана
</query>
		<autoFillFields>false</autoFillFields>
