		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	&amp;ЭлементОтчета КАК Показатель,
	&amp;ДатаНачала КАК ДатаНачала,
	&amp;ДатаОкончания КАК ДатаОкончания,
	&amp;ВидСубконтоОборота КАК ВидСубконто,
	&amp;Субконто1,
	&amp;Субконто2,
	&amp;Субконто3,
	&amp;Организация,
	&amp;Подразделение,
	&amp;НаправлениеДеятельности,
	ЕСТЬNULL(ТаблицаРегистра.СуммаНачальныйОстаток, 0) * ВЫБОР
		КОГДА &amp;ОбратныйЗнак = ИСТИНА
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК ПолеОстатка,
	ЕСТЬNULL(ТаблицаРегистра.СуммаКонечныйОстаток, 0) * ВЫБОР
		КОГДА &amp;ОбратныйЗнак = ИСТИНА
			ТОГДА -1
		ИНАЧЕ 1
	КОНЕЦ КАК Значение,
	&amp;КодСтроки КАК КодСтроки,
	&amp;Примечание КАК Примечание
ИЗ
	РегистрБухгалтерии.Международный.ОстаткиИОбороты(&amp;ДатаНачала, &amp;ДатаОкончания, Авто,,, {(&amp;ВидСубконтоОборота)},
		{(Организация).* КАК Организация} {(Подразделение).* КАК Подразделение} {(НаправлениеДеятельности).* КАК
		НаправлениеДеятельности} {(Субконто1).* КАК Субконто1} {(Субконто2).* КАК Субконто2} {(Субконто3).* КАК
		Субконто3}) КАК ТаблицаРегистра</query>
	</dataSet>
