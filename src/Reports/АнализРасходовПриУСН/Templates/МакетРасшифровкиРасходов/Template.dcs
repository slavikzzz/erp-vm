				<dataPath>НДСНеПризнано</dataPath>
				<field>НДСНеПризнано</field>
				<title xsi:type="v8:LocalStringType">
					<v8:item>
						<v8:lang>ru</v8:lang>
						<v8:content>НДСНе признано</v8:content>
					</v8:item>
					<v8:item>
