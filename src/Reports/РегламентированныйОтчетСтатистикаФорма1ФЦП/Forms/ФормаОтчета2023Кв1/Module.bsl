					КонецЦикла;
					ПоказательИмя = "П0010010301" + Графа;
					Обл = ТДО.Найти(ПоказательИмя);
					Если Обл <> Неопределено Тогда
						ТДО[ПоказательИмя].Значение = СуммаПоГрафе;
					КонецЕсли;
					
					//стр. 204 = стр. 205 + стр. 206 + стр. 207 по соответствующим графам;
					СуммаПоГрафе = 0;
					Для Стр = 205 По 207 Цикл
						Если Гр = 3 Или Гр = 4 Тогда
