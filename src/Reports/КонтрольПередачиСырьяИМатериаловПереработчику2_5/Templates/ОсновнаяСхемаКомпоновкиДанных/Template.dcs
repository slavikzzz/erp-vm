		<dataSource>ИсточникДанных1</dataSource>
		<query>// ИСПОЛЬЗУЮТСЯ ТЕГИ!!!
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Заказы.ПериодСекунда КАК ПериодСекунда,
	Заказы.ПериодМинута КАК ПериодМинута,
	Заказы.ПериодЧас КАК ПериодЧас,
	Заказы.ПериодДень КАК ПериодДень,
	Заказы.ПериодНеделя КАК ПериодНеделя,
	Заказы.ПериодДекада КАК ПериодДекада,
	Заказы.ПериодМесяц КАК ПериодМесяц,
	Заказы.ПериодКвартал КАК ПериодКвартал,
	Заказы.ПериодПолугодие КАК ПериодПолугодие,
	Заказы.ПериодГод КАК ПериодГод,
	
	ВЫБОР
		КОГДА Заказы.Регистратор = НЕОПРЕДЕЛЕНО
			ТОГДА NULL
		ИНАЧЕ Заказы.Регистратор
	КОНЕЦ КАК Регистратор,
	
	РеестрДокументов.Организация КАК Организация,
	РеестрДокументов.Партнер КАК Переработчик,
	РеестрДокументов.Договор КАК Договор,
	Заказы.ЗаказКлиента КАК ЗаказКлиента,
	РеестрДокументов.ДатаДокументаИБ КАК ДатаДокументаИБ,
	РеестрДокументов.НомерДокументаИБ КАК НомерДокументаИБ,
	Заказы.Номенклатура КАК Номенклатура,
	Заказы.Номенклатура.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	Заказы.Характеристика КАК Характеристика,
	Заказы.КодСтроки КАК КодСтроки,
	Заказы.Склад КАК Склад,
	Заказы.Серия КАК Серия,
	Заказы.КодСтроки = 0 КАК СверхЗаказа,
	Заказы.ЗаказаноНачальныйОстаток КАК КОформлениюНачальныйОстаток,
	Заказы.ЗаказаноПриход КАК КОформлениюПриход,
	Заказы.КОформлениюРасход КАК КОформлениюРасход,
	Заказы.ЗаказаноКонечныйОстаток КАК КОформлениюКонечныйОстаток
ПОМЕСТИТЬ ДанныеРегистра
ИЗ
	РегистрНакопления.ЗаказыКлиентов.ОстаткиИОбороты(
		{(&amp;ДатаНачала)},
		{(&amp;ДатаОкончания)},
		Авто,,
		ТИПЗНАЧЕНИЯ(ЗаказКлиента) = ТИП(Документ.ЗаказПереработчику2_5)
			{ЗаказКлиента.* КАК ЗаказКлиента,
			(КодСтроки = 0) КАК СверхЗаказа,
			Номенклатура.* КАК Номенклатура,
			Характеристика.* КАК Характеристика,
			КодСтроки КАК КодСтроки,
			Склад.* КАК Склад
			}
		) КАК Заказы
	
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.РеестрДокументов КАК РеестрДокументов
	ПО РеестрДокументов.Ссылка = Заказы.ЗаказКлиента
	И НЕ РеестрДокументов.ДополнительнаяЗапись

{ГДЕ
	Заказы.Регистратор.*               КАК Регистратор,
	РеестрДокументов.Организация.*     КАК Организация,
	РеестрДокументов.Партнер.*         КАК Переработчик,
	РеестрДокументов.Договор.*         КАК Договор
}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ДанныеРегистра.ЗаказКлиента КАК ЗаказКлиента,
	ДанныеРегистра.КодСтроки    КАК КодСтроки
ПОМЕСТИТЬ ВтЗаказы
ИЗ
	ДанныеРегистра КАК ДанныеРегистра

СГРУППИРОВАТЬ ПО
	ДанныеРегистра.ЗаказКлиента,
	ДанныеРегистра.КодСтроки
	
ИНДЕКСИРОВАТЬ ПО
	ЗаказКлиента,
	КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Заказы.ЗаказКлиента КАК ЗаказКлиента,
	Заказы.КодСтроки КАК КодСтроки,
	МАКСИМУМ(ЕСТЬNULL(ТабЧасть.ДатаОтгрузки, ДАТАВРЕМЯ(1, 1, 1))) КАК ДатаОтгрузкиПлан
ПОМЕСТИТЬ ВтСрокиПоставки
ИЗ
	ВтЗаказы КАК Заказы

	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику2_5.ОбеспечениеМатериаламиИРаботами КАК ТабЧасть
	ПО Заказы.ЗаказКлиента = ТабЧасть.Ссылка
	И Заказы.КодСтроки = ТабЧасть.КодСтроки

ГДЕ
	НЕ ТабЧасть.Отменено

СГРУППИРОВАТЬ ПО
	Заказы.ЗаказКлиента,
	Заказы.КодСтроки

//++ НЕ УТКА
ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Заказы.ЗаказКлиента КАК ЗаказКлиента,
	Заказы.КодСтроки КАК КодСтроки,
	МАКСИМУМ(ЕСТЬNULL(ТабЧасть.ДатаОтгрузки, ДАТАВРЕМЯ(1, 1, 1))) КАК ДатаОтгрузкиПлан
ИЗ
	ВтЗаказы КАК Заказы

	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику2_5 КАК ДокЗаказ
	ПО Заказы.ЗаказКлиента = ДокЗаказ.Ссылка

	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЭтапПроизводства2_2 КАК ДокЭтап
	ПО ДокЗаказ.Ссылка = ДокЭтап.ЗаказПереработчику

	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЭтапПроизводства2_2.ОбеспечениеМатериаламиИРаботами КАК ТабЧасть
	ПО ДокЭтап.Ссылка = ТабЧасть.Ссылка
	И Заказы.КодСтроки = ТабЧасть.КодСтроки
	
ГДЕ
	ДокЗаказ.ГруппировкаЗатрат = ЗНАЧЕНИЕ(Перечисление.ГруппировкиЗатратВЗаказеПереработчику.ПоЭтапамПроизводства)
	И НЕ ТабЧасть.Отменено
	
СГРУППИРОВАТЬ ПО
	Заказы.ЗаказКлиента,
	Заказы.КодСтроки
//-- НЕ УТКА

ИНДЕКСИРОВАТЬ ПО
	ЗаказКлиента,
	КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДанныеРегистра.ПериодСекунда КАК ПериодСекунда,
	ДанныеРегистра.ПериодМинута КАК ПериодМинута,
	ДанныеРегистра.ПериодЧас КАК ПериодЧас,
	ДанныеРегистра.ПериодДень КАК ПериодДень,
	ДанныеРегистра.ПериодНеделя КАК ПериодНеделя,
	ДанныеРегистра.ПериодДекада КАК ПериодДекада,
	ДанныеРегистра.ПериодМесяц КАК ПериодМесяц,
	ДанныеРегистра.ПериодКвартал КАК ПериодКвартал,
	ДанныеРегистра.ПериодПолугодие КАК ПериодПолугодие,
	ДанныеРегистра.ПериодГод КАК ПериодГод,
	ДанныеРегистра.Регистратор КАК Регистратор,
	ДанныеРегистра.Организация КАК Организация,
	ДанныеРегистра.Переработчик КАК Переработчик,
	ДанныеРегистра.Договор КАК Договор,
	ДанныеРегистра.ЗаказКлиента КАК ЗаказКлиента,
	ДанныеРегистра.ДатаДокументаИБ КАК ДатаДокументаИБ,
	ДанныеРегистра.НомерДокументаИБ КАК НомерДокументаИБ,
	ДанныеРегистра.Номенклатура КАК Номенклатура,
	ДанныеРегистра.Номенклатура.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	ДанныеРегистра.Характеристика КАК Характеристика,
	ДанныеРегистра.КодСтроки КАК КодСтроки,
	СрокиПоставки.ДатаОтгрузкиПлан КАК ДатаОтгрузкиПлан,
	ДанныеРегистра.Склад КАК Склад,
	ДанныеРегистра.Серия КАК Серия,
