				СтруктураИнформацииКодРФ.Свойство("ТекущееКоличествоПредупреждений", ТекущееКоличествоПредупреждений);
				
				// Проверка на дублирование кода субъекта РФ.
				Если ЗначениеЗаполнено(КодРФ) Тогда
					
					СтрНомерСтрокиПовтора = СписокОтгрузкиКодовРФ.Получить(КодРФ);
					Если НЕ СтрНомерСтрокиПовтора = Неопределено Тогда
											
						ТекстОписанияОшибки = СтрокаГдеТаблицыПоРФ + " дублирование кода субъекта РФ
							|ранее обнаруженного в строке " + СтрНомерСтрокиПовтора
							+ " этой же подтаблицы отгрузок по РФ.";

						СтруктураОшибки = Новый Структура;
						СтруктураОшибки.Вставить("ТекстОписанияОшибки", ТекстОписанияОшибки);
						СтруктураОшибки.Вставить("Раздел", ИмяРаздела);
						СтруктураОшибки.Вставить("Показатель", ИмяПоказателя);
						СтруктураОшибки.Вставить("ЛистРаздела", НомерЛиста);
						СтруктураОшибки.Вставить("ТипОшибки", "ДублированиеКодаРФ");

						СообщитьОбОшибкеЗаполнения(ДокументСсылкаАлкоОтчет, ТаблицаСообщений, СтатусПроверки, СтруктураОшибки, 
														МаксимальноеКоличествоОшибок, ТекущееКоличествоОшибок);

					Иначе
						СписокОтгрузкиКодовРФ.Вставить(КодРФ, СтрНомерСтрокиТаблицыПоРФ);
					КонецЕсли; 
									
				КонецЕсли; 
				
				Если СтрокаОтгрузки.Розница < 0 Тогда

					Показатель = ИмяРозницаРФ + "_" + НомерСтрокиСтроковый;
					
					ТекстОписанияОшибки = СтрокаГдеТаблицыПоРФ + " отрицательное значение
						|в колонке  (""Отгружено потребителям, по Российской Федерации, организациям розничной торговли"").";

					СтруктураОшибки = Новый Структура;
