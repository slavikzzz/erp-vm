	НомерМесяца = Формат(ДатаОкончания, "ДФ=М");
	
	Если НомерМесяца = "12" Тогда
		НомерМесяца = "0";
	КонецЕсли;
	
	РазделыОтчета.Титульный.Данные.Период = НомерМесяца;
	РазделыОтчета.Титульный.Данные.ОтчетГод = Формат(ДатаОкончания, "ДФ=гггг");
