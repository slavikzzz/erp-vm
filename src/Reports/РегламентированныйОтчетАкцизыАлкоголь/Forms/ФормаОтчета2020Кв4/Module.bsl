&НаСервере
Процедура ПроверкаКС_2_06_код(Настройки)
	
	Перем Показатели;
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = ДанныеКС.ВременныеТаблицы;
	Запрос.УстановитьПараметр("Отчет", ДанныеКС.Отчет);
	Запрос.УстановитьПараметр("МассивКодовСроков", СтрРазделить(",03,04", ","));
	Запрос.УстановитьПараметр("МассивКодовПоказателей", СтрРазделить(",40001", ","));
	Запрос.Текст = 
	
	"ВЫБРАТЬ
	|	Раздел24.КБК КАК КБК,
	|	Раздел24.КодПоказателя КАК КодПоказателя,
	|	Раздел24.Р24Г002 КАК Р24Г002,
	|	Раздел24.Отчет КАК Отчет,
	|	Раздел24.НомерСтраницы КАК НомерСтраницы
	|ИЗ
	|	ВТ_Р24 КАК Раздел24
	|ГДЕ
	|	Раздел24.Отчет = &Отчет
	|	И Раздел24.КодПоказателя В(&МассивКодовПоказателей)
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы
	|ИТОГИ
	|	СУММА(Р24Г002) 
	|ПО
	|	КБК
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Раздел1.КБК КАК КБК,
	|	Раздел1.Р01С040 КАК Р01С040,
	|	Раздел1.Отчет КАК Отчет,
	|	Раздел1.НомерСтраницы КАК НомерСтраницы,
	|	Раздел1.НомерСтроки КАК НомерСтроки,
	|	Раздел1.НомерСтроки2 КАК НомерСтроки2
	|ИЗ
	|	ВТ_Р1а КАК Раздел1
	|ГДЕ
	|	Раздел1.Отчет = &Отчет
	|	И Раздел1.КодСрока В(&МассивКодовСроков)
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки,
	|	НомерСтроки2
	|ИТОГИ
	|	СУММА(Р01С040)
	|ПО
	|	КБК";	
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("Р01С040"		, "П00001М204000_");
	ИменаПоказателей.Вставить("Р24Г002"		, "П000240000002");
	
	СформироватьПоказатели(Показатели, ИменаПоказателей);
	
	РезультатЗапроса = Запрос.ВыполнитьПакет();
	ВыборкаР1 = РезультатЗапроса[1].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	ВыборкаР2 = РезультатЗапроса[0].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	
	ПоказательР1 = "Р01С040";
	ПоказательР2 = "Р24Г002";
	
	Пока ВыборкаР1.Следующий() Цикл
		
		ЗарегистрироватьФормулу(Настройки);
		Показатели[ПоказательР1].Итог = 0;
		Показатели[ПоказательР2].Итог = 0;
		
		Выборка = ВыборкаР1.Выбрать();
		Пока Выборка.Следующий() Цикл
			ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательР1], Выборка);
		КонецЦикла; 
		
		Если ВыборкаР2.НайтиСледующий(Новый Структура("КБК", ВыборкаР1.КБК)) Тогда
			Выборка	= ВыборкаР2.Выбрать();
			Пока Выборка.Следующий() Цикл
				ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательР2], Выборка);
			КонецЦикла;
			ВыборкаР2.Сбросить();
		КонецЕсли;
		
		Настройки.ЭтоОшибка = НЕ Показатели[ПоказательР1].Итог = Показатели[ПоказательР2].Итог;
		Настройки.Расшифровка = ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки,
			Показатели[ПоказательР1].Итог, Показатели[ПоказательР2].Итог, ЗначениеСтр(ВыборкаР1.КБК));
		ДополнитьСтруктуруОшибок(Настройки);
		
	КонецЦикла;
	
КонецПроцедуры

