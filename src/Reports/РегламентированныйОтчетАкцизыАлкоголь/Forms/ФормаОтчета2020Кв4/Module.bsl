&НаСервере
Процедура ПроверкаКС_3_08_код(Настройки)
	
	Перем Показатели;
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = ДанныеКС.ВременныеТаблицы;
	Запрос.УстановитьПараметр("Отчет", ДанныеКС.Отчет);
	Запрос.УстановитьПараметр("МассивКодовТоваров", СтрРазделить(",294", ","));
	Запрос.УстановитьПараметр("МассивКодовПоказателей", СтрРазделить(",30014,30015,30016", ","));
	Запрос.Текст = 
	
	"ВЫБРАТЬ
	|	Раздел23.КодТовара + Раздел23.КодПоказателя КАК Ключ,
	|	Раздел23.КодТовара КАК КодТовара,
	|	Раздел23.КодПоказателя КАК КодПоказателя,
	|	Раздел23.Р23Г003 КАК Р23Г003,
	|	Раздел23.Отчет КАК Отчет,
	|	Раздел23.НомерСтраницы КАК НомерСтраницы,
	|	Раздел23.НомерСтроки КАК НомерСтроки
	|ИЗ
	|	ВТ_Р23 КАК Раздел23
	|ГДЕ
	|	Раздел23.Отчет = &Отчет
	|	И Раздел23.КодТовара В(&МассивКодовТоваров)
	|	И Раздел23.КодПоказателя В(&МассивКодовПоказателей)
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки
	|ИТОГИ
	|	МИНИМУМ(КодТовара),
	|	МИНИМУМ(КодПоказателя),
	|	СУММА(Р23Г003) 
	|ПО
	|	Ключ
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Раздел31.КодТовара + Раздел31.КодПоказателя КАК Ключ,
	|	Раздел31.КодТовара КАК КодТовара,
	|	Раздел31.КодПоказателя КАК КодПоказателя,
	|	Раздел31.Р31С140 КАК Р31С140,
	|	Раздел31.Отчет КАК Отчет,
	|	Раздел31.НомерСтраницы КАК НомерСтраницы,
	|	Раздел31.НомерСтроки КАК НомерСтроки,
	|	Раздел31.НомерСтроки2 КАК НомерСтроки2
	|ИЗ
	|	ВТ_Р31 КАК Раздел31
	|ГДЕ
	|	Раздел31.Отчет = &Отчет
	|	И Раздел31.КодТовара В(&МассивКодовТоваров)
	|	И Раздел31.КодПоказателя В(&МассивКодовПоказателей)
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки,
	|	НомерСтроки2
	|ИТОГИ
	|	МИНИМУМ(КодТовара),
	|	МИНИМУМ(КодПоказателя),
	|	СУММА(Р31С140)
	|ПО
	|	Ключ";	
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("Р31С140"		, "П00003М214003_");
	ИменаПоказателей.Вставить("Р23Г003"		, "П00023М100003_");
	
	СформироватьПоказатели(Показатели, ИменаПоказателей);
	
	РезультатЗапроса = Запрос.ВыполнитьПакет();
	ВыборкаР3 = РезультатЗапроса[1].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	ВыборкаР2 = РезультатЗапроса[0].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	
	ПоказательР3 = "Р31С140";
	ПоказательР2 = "Р23Г003";
	
	Пока ВыборкаР2.Следующий() Цикл
		
		ЗарегистрироватьФормулу(Настройки);
		Показатели[ПоказательР3].Итог = 0;
		Показатели[ПоказательР2].Итог = 0;
		
		Выборка = ВыборкаР2.Выбрать();
		Пока Выборка.Следующий() Цикл
			ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательР2], Выборка);
		КонецЦикла; 
		
		Если ВыборкаР3.НайтиСледующий(Новый Структура("Ключ", ВыборкаР2.Ключ)) Тогда
			Выборка	= ВыборкаР3.Выбрать();
			Пока Выборка.Следующий() Цикл
				ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательР3], Выборка);
			КонецЦикла;
			ВыборкаР3.Сбросить();
		КонецЕсли;
		
		Настройки.ЭтоОшибка = НЕ Показатели[ПоказательР3].Итог = Показатели[ПоказательР2].Итог;
		Настройки.Расшифровка = ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки,
			Показатели[ПоказательР2].Итог, Показатели[ПоказательР3].Итог, ЗначениеСтр(ВыборкаР2.КодТовара), 
			ЗначениеСтр(ВыборкаР2.КодПоказателя));
		ДополнитьСтруктуруОшибок(Настройки);
		
	КонецЦикла;
	
КонецПроцедуры

