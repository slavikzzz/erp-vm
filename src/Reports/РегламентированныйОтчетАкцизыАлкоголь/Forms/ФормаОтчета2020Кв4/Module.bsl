	Запрос.МенеджерВременныхТаблиц = ДанныеКС.ВременныеТаблицы;
	Запрос.УстановитьПараметр("Отчет", ДанныеКС.Отчет);
	Запрос.УстановитьПараметр("МассивКодовПоказателей", МассивКодовПоказателей);
