			Настройки.ЭтоОшибка = ДанныеКС.Раздел3["МнЧастьЗаполненаП00003М2_" + Счетчик + "_" + Счетчик2] 
				И НЕ ЗначениеЗаполнено(Показатели.Р31С001.Значение);
			Настройки.Расшифровка = СтрЗаменить(Настройки.ШаблонРасшифровки, "%1", Показатели.Р31С001.Значение);
