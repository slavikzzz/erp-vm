				ЗарегистрироватьФормулу(Настройки);
				ЗаполнитьИРасшифроватьПоказатели(Показатели, ДанныеКС.Раздел3, Счетчик, Счетчик2);
				
				Настройки.ЭтоОшибка = Список.Получить(Показатели.Р31С060.Значение) = Неопределено;
				Настройки.Расшифровка = СтрЗаменить(Настройки.ШаблонРасшифровки, "%1", Показатели.Р31С060.ЗначениеСтр);
				ДополнитьСтруктуруОшибок(Настройки);
			КонецЦикла;
		КонецЦикла;
	КонецЕсли;
	
КонецПроцедуры

