			|en = '- стр.430 - стр.440 - стр.450 Приложения 6'"));
	Заголовки.Вставить("ШаблонРасшифровки"	, НСтр("ru = '%1 = %2';
													|en = '%1 = %2'"));
	Заголовки.Вставить("ОписаниеОшибки"		, НСтр("ru = 'Ошибочное значение остатка этилового спирта';
														|en = 'Ошибочное значение остатка этилового спирта'"));
	Настройки = ОбработатьНастройки(		, Заголовки);

	Если Документирование Тогда
		Возврат;
	КонецЕсли;
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("П64С490"		, "П00106М349003_");
	ИменаПоказателей.Вставить("П64С405"		, "П00106М340503_");
	ИменаПоказателей.Вставить("П64С410"		, "П00106М341003_");
	ИменаПоказателей.Вставить("П64С420"		, "П00106М342003_");
	ИменаПоказателей.Вставить("П64С430"		, "П00106М343003_");
	ИменаПоказателей.Вставить("П64С440"		, "П00106М344003_");
	ИменаПоказателей.Вставить("П64С450"		, "П00106М345003_");
	
	СформироватьПоказатели(Показатели, ИменаПоказателей);
	
	Для каждого СтраницаРаздела Из ДанныеКС.Приложение6 Цикл
		Для каждого СтраницаПодраздела Из СтраницаРаздела.Приложение62 Цикл
			Для Счетчик = 1 По СтраницаПодраздела.КолвоСтрокП00106М3 Цикл
				ЗарегистрироватьФормулу(Настройки);
				ЗаполнитьИРасшифроватьПоказатели(Показатели, СтраницаПодраздела, XMLСтрока(Счетчик));
				
				РасчетнаяСумма = Макс(0, Показатели.П64С405.Значение - Показатели.П64С410.Значение - Показатели.П64С420.Значение 
					 - Показатели.П64С430.Значение - Показатели.П64С440.Значение - Показатели.П64С450.Значение);
				Настройки.ЭтоОшибка = НЕ Показатели.П64С490.Значение = РасчетнаяСумма;
				Настройки.Расшифровка = СтрЗаменить(Настройки.ШаблонРасшифровки	, "%1", Показатели.П64С490.Значение);
				Настройки.Расшифровка = СтрЗаменить(Настройки.Расшифровка		, "%2", РасчетнаяСумма);
				ДополнитьСтруктуруОшибок(Настройки);
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
КонецПроцедуры

