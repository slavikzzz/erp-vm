	ИменаТаблиц.Вставить("ТаблицаР22"	, "Т_Р22");
	ИменаТаблиц.Вставить("ТаблицаР23"	, "Т_Р23");
	ИменаТаблиц.Вставить("ТаблицаР24"	, "Т_Р24");
