Процедура ПроверкаКС_10_31_код(Настройки)
	
	Перем Показатели;
	
	ПоказательП6 = Настройки.ПоказательП6;
	ПоказательР2 = Настройки.ПоказательР2;
	
	МассивКодовПоказателей = Новый Массив;
	МассивКодовПоказателей.Добавить("");
	МассивКодовПоказателей.Добавить("10001");
	МассивКодовПоказателей.Добавить("10006");
	МассивКодовПоказателей.Добавить("10007");
	МассивКодовПоказателей.Добавить("10008");
	МассивКодовПоказателей.Добавить("10009");
	МассивКодовПоказателей.Добавить("10010");
	МассивКодовПоказателей.Добавить("10011");
	МассивКодовПоказателей.Добавить("10012");
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = ДанныеКС.ВременныеТаблицы;
	Запрос.УстановитьПараметр("Отчет", ДанныеКС.Отчет);
	Запрос.УстановитьПараметр("МассивКодовПоказателей", МассивКодовПоказателей);
	Запрос.Текст = 
	
	"ВЫБРАТЬ
	|	Раздел21.КодТовара КАК КодТовара,
	|	Раздел21.КодПоказателя КАК КодПоказателя,
	|	Раздел21.Р21Г003 КАК Р21Г003,
	|	Раздел21.Р21Г004 КАК Р21Г004,
	|	Раздел21.Отчет КАК Отчет,
	|	Раздел21.НомерСтраницы КАК НомерСтраницы,
	|	Раздел21.НомерСтроки КАК НомерСтроки
	|ИЗ
	|	ВТ_Р21 КАК Раздел21
	|ГДЕ
	|	Раздел21.Отчет = &Отчет
	|	И Раздел21.КодПоказателя В(&МассивКодовПоказателей)
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки
	|ИТОГИ
	|	СУММА(Р21Г003),
	|	СУММА(Р21Г004)
	|ПО
	|	КодТовара
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Приложение6.КодТовара КАК КодТовара,
	|	Приложение6.П62С250 КАК П62С250,
	|	Приложение6.П62С255 КАК П62С255,
	|	Приложение6.Отчет КАК Отчет,
	|	Приложение6.НомерСтраницы КАК НомерСтраницы,
	|	Приложение6.НомерСтроки КАК НомерСтроки
	|ИЗ
	|	ВТ_П62 КАК Приложение6
	|ГДЕ
	|	Приложение6.Отчет = &Отчет
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки
	|ИТОГИ
	|	МИНИМУМ(КодТовара),
	|	СУММА(П62С250),
	|	СУММА(П62С255)
	|ПО
	|	КодТовара";	
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("П62С250"		, "П00106М125003_");
	ИменаПоказателей.Вставить("П62С255"		, "П00106М125503_");
	ИменаПоказателей.Вставить("Р21Г003"		, "П00021М100003_");
	ИменаПоказателей.Вставить("Р21Г004"		, "П00021М100004_");
	
	СформироватьПоказатели(Показатели, ИменаПоказателей);
	
	РезультатЗапроса = Запрос.ВыполнитьПакет();
	ВыборкаП6 = РезультатЗапроса[1].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	ВыборкаР2 = РезультатЗапроса[0].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	
	Пока ВыборкаП6.Следующий() Цикл
		
		ЗарегистрироватьФормулу(Настройки);
		Показатели[ПоказательП6].Итог = 0;
		Показатели[ПоказательР2].Итог = 0;
		
		Выборка = ВыборкаП6.Выбрать();
		Пока Выборка.Следующий() Цикл
			ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательП6], Выборка);
		КонецЦикла; 
		
		Если ВыборкаР2.НайтиСледующий(Новый Структура("КодТовара", ВыборкаП6.КодТовара)) Тогда
			Выборка	= ВыборкаР2.Выбрать();
			Пока Выборка.Следующий() Цикл
				ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательР2], Выборка);
			КонецЦикла;
			ВыборкаР2.Сбросить();
		КонецЕсли;
		
		Настройки.ЭтоОшибка = НЕ Показатели[ПоказательП6].Итог = Показатели[ПоказательР2].Итог;
		Настройки.Расшифровка = ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки,
			Показатели[ПоказательП6].Итог, Показатели[ПоказательР2].Итог, ЗначениеСтр(ВыборкаП6.КодТовара));
		ДополнитьСтруктуруОшибок(Настройки);
		
	КонецЦикла;
	
КонецПроцедуры

