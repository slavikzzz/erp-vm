Процедура ПроверкаКС_10_82_код(Настройки)
	
	Перем Показатели;
	
	ПоказательП6 = Настройки.ПоказательП6;
	ПоказательР2 = Настройки.ПоказательР2;
	
	Запрос = Новый Запрос;
	Запрос.МенеджерВременныхТаблиц = ДанныеКС.ВременныеТаблицы;
	Запрос.УстановитьПараметр("Отчет", ДанныеКС.Отчет);
	Запрос.УстановитьПараметр("МассивКодовПоказателей", Настройки.МассивКодов);
	Запрос.Текст = 
	
	"ВЫБРАТЬ
	|	Раздел23.КодТовара КАК КодТовара,
	|	Раздел23.КодПоказателя КАК КодПоказателя,
	|	Раздел23.Р23Г003 КАК Р23Г003,
	|	Раздел23.Отчет КАК Отчет,
	|	Раздел23.НомерСтраницы КАК НомерСтраницы,
	|	Раздел23.НомерСтроки КАК НомерСтроки
	|ИЗ
	|	ВТ_Р23 КАК Раздел23
	|ГДЕ
	|	Раздел23.Отчет = &Отчет
	|	И Раздел23.КодПоказателя В(&МассивКодовПоказателей)
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки
	|ИТОГИ
	|	СУММА(Р23Г003)
	|ПО
	|	КодТовара
	|;
	|
	|////////////////////////////////////////////////////////////////////////////////
	|ВЫБРАТЬ
	|	Приложение6.КодТовара КАК КодТовара,
	|	Приложение6.П64С460 КАК П64С460,
	|	Приложение6.П64С470 КАК П64С470,
	|	Приложение6.Отчет КАК Отчет,
	|	Приложение6.НомерСтраницы КАК НомерСтраницы,
	|	Приложение6.НомерСтроки КАК НомерСтроки
	|ИЗ
	|	ВТ_П64 КАК Приложение6
	|ГДЕ
	|	Приложение6.Отчет = &Отчет
	|
	|УПОРЯДОЧИТЬ ПО
	|	НомерСтраницы,
	|	НомерСтроки
	|ИТОГИ
	|	МИНИМУМ(КодТовара),
	|	СУММА(П64С460),
	|	СУММА(П64С470)
	|ПО
	|	КодТовара";	
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("П64С460"		, "П00106М346003_");
	ИменаПоказателей.Вставить("П64С470"		, "П00106М347003_");
	ИменаПоказателей.Вставить("Р23Г003"		, "П00023М100003_");
	
	СформироватьПоказатели(Показатели, ИменаПоказателей);
	
	РезультатЗапроса = Запрос.ВыполнитьПакет();
	ВыборкаП6 = РезультатЗапроса[1].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	ВыборкаР2 = РезультатЗапроса[0].Выбрать(ОбходРезультатаЗапроса.ПоГруппировкам);
	
	Пока ВыборкаП6.Следующий() Цикл
		
		ЗарегистрироватьФормулу(Настройки);
		Показатели[ПоказательП6].Итог = 0;
		Показатели[ПоказательР2].Итог = 0;
		
		Выборка = ВыборкаП6.Выбрать();
		Пока Выборка.Следующий() Цикл
			ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательП6], Выборка);
		КонецЦикла; 
		
		Если ВыборкаР2.НайтиСледующий(Новый Структура("КодТовара", ВыборкаП6.КодТовара)) Тогда
			Выборка	= ВыборкаР2.Выбрать();
			Пока Выборка.Следующий() Цикл
				ЗаполнитьИРасшифроватьПоказатель(Показатели[ПоказательР2], Выборка);
			КонецЦикла;
			ВыборкаР2.Сбросить();
		КонецЕсли;
		
		Настройки.ЭтоОшибка = НЕ Показатели[ПоказательП6].Итог = Показатели[ПоказательР2].Итог;
		Настройки.Расшифровка = ПодставитьПараметрыВСтроку(Настройки.ШаблонРасшифровки,
			Показатели[ПоказательП6].Итог, Показатели[ПоказательР2].Итог, ЗначениеСтр(ВыборкаП6.КодТовара));
		ДополнитьСтруктуруОшибок(Настройки);
		
	КонецЦикла;
	
КонецПроцедуры

