				И ВсеСтрокиОтчета.КодДохода.НеОблагаетсяУНалоговогоАгента
			ТОГДА "4903"
		КОГДА ВсеСтрокиОтчета.ОписаниеСтроки = 1
				И ВсеСтрокиОтчета.КодДохода.КодПрименяемыйВНалоговойОтчетностиС2021Года В ("2610", "2630", "2640", "2641")
				И (ВсеСтрокиОтчета.ДатаПолученияДохода МЕЖДУ ДАТАВРЕМЯ(2021, 1, 1, 0, 0, 0) И ДАТАВРЕМЯ(2023, 12, 31, 23, 59, 59))
			ТОГДА "4901"
		КОГДА ВсеСтрокиОтчета.ОписаниеСтроки = 1
				ИЛИ ВсеСтрокиОтчета.ОписаниеСтроки = 3
			ТОГДА ЕСТЬNULL(ВсеСтрокиОтчета.КодДохода.КодПрименяемыйВНалоговойОтчетностиС2021Года, "")
		КОГДА ВсеСтрокиОтчета.ОписаниеСтроки = 2
				И ВсеСтрокиОтчета.ВидДохода = ЗНАЧЕНИЕ(Справочник.ВидыДоходовПоСтраховымВзносам.НеЯвляетсяОбъектом)
			ТОГДА "4902"
		КОГДА ВсеСтрокиОтчета.ОписаниеСтроки = 2
				И НЕ ВсеСтрокиОтчета.ВидДохода.ВходитВБазуФОМС
			ТОГДА "4901"
		ИНАЧЕ ""
	КОНЕЦ,
	ВЫБОР
		КОГДА ВсеСтрокиОтчета.ВидДохода = "01"
			ТОГДА "01"
		КОГДА ВсеСтрокиОтчета.ВидДохода = "02"
			ТОГДА "02"
		КОГДА ВсеСтрокиОтчета.ВидДохода = "03"
