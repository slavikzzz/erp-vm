			<dataSource>ИсточникДанных1</dataSource>
			<query>// !!! ИСПОЛЬЗУЮТСЯ ТЕГИ !!!

ВЫБРАТЬ РАЗРЕШЕННЫЕ РАЗЛИЧНЫЕ
	Сегменты.Номенклатура КАК Номенклатура,
	Сегменты.Характеристика КАК Характеристика,
	ИСТИНА КАК ИспользуетсяОтборПоСегментуНоменклатуры
ПОМЕСТИТЬ ОтборПоСегментуНоменклатуры
ИЗ
	РегистрСведений.НоменклатураСегмента КАК Сегменты
{ГДЕ
	Сегменты.Сегмент.* КАК СегментНоменклатуры,
	Сегменты.Номенклатура.* КАК Номенклатура,
	Сегменты.Характеристика.* КАК Характеристика}

ИНДЕКСИРОВАТЬ ПО
	Номенклатура,
	Характеристика,
	ИспользуетсяОтборПоСегментуНоменклатуры
;
////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	"МатериальныеРаспределенные" КАК ИмяНабора,
	&amp;МатериальныеЗатраты КАК ТипЗатрат,
	ИСТИНА КАК Распределено,
	Затраты.Организация КАК Организация,
	ВЫБОР
		КОГДА &amp;ДанныеОтчета = 3
			ТОГДА Затраты.Организация.ВалютаРегламентированногоУчета
		ИНАЧЕ &amp;ВалютаУправленческогоУчета
	КОНЕЦ КАК Валюта,
	Затраты.ЗаказНаПроизводство КАК ЗаказНаПроизводство,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура КАК НоменклатураЗатрат,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура.ЕдиницаИзмерения КАК ЕдиницаХранения,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура.ЕдиницаДляОтчетов КАК ЕдиницаДляОтчетов,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов КАК КоэффициентЕдиницыДляОтчетов,
	Затраты.АналитикаУчетаНоменклатуры.Характеристика КАК Характеристика,
	Затраты.АналитикаУчетаНоменклатуры.Серия КАК Серия,
	Затраты.АналитикаУчетаНоменклатуры.МестоХранения КАК Подразделение,
	ЕСТЬNULL(СпрВидыЗапасов.ГруппаПродукции, ЗНАЧЕНИЕ(Справочник.ГруппыАналитическогоУчетаНоменклатуры.ПустаяСсылка)) КАК ГруппаПродукции,
	Затраты.АналитикаУчетаНоменклатуры.Назначение КАК НазначениеЗатрат,
	НЕОПРЕДЕЛЕНО КАК Регистратор,
	
	ВЫБОР
		КОГДА СпрВидыЗапасов.ТипЗапасов = ЗНАЧЕНИЕ(Перечисление.ТипыЗапасов.ПолуфабрикатДавальца)
			ТОГДА Затраты.КоличествоНачальныйОстаток
			ИНАЧЕ 0
	КОНЕЦ КАК КоличествоНачальныйОстаток,
	0 КАК КоличествоНачальныйОстатокНеРаспределено,
	ВЫБОР
		КОГДА &amp;ЕдиницыКоличества = 0
			ТОГДА Затраты.КоличествоНачальныйОстаток
		КОГДА &amp;ЕдиницыКоличества = 1
			ТОГДА ВЫБОР
					КОГДА Затраты.АналитикаУчетаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов &lt;&gt; 0
						ТОГДА Затраты.КоличествоНачальныйОстаток / Затраты.АналитикаУчетаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов
					ИНАЧЕ 0
				КОНЕЦ
	КОНЕЦ КАК КоличествоНачальныйОстатокНаЭтапы,
	
	0 КАК КоличествоПриход,
	0 КАК КоличествоПриходНаЭтапы,
	
	0 КАК КоличествоРасходНаЭтапы,
	0 КАК КоличествоРасходНаВыпуск,
	0 КАК КоличествоРасходПрочий,
	
	ВЫБОР
			КОГДА СпрВидыЗапасов.ТипЗапасов = ЗНАЧЕНИЕ(Перечисление.ТипыЗапасов.ПолуфабрикатДавальца)
				ТОГДА Затраты.КоличествоКонечныйОстаток
			ИНАЧЕ 0
	КОНЕЦ КАК КоличествоКонечныйОстаток,
	0 КАК КоличествоКонечныйОстатокНеРаспределено,
	ВЫБОР
		КОГДА &amp;ЕдиницыКоличества = 0
			ТОГДА Затраты.КоличествоКонечныйОстаток
		КОГДА &amp;ЕдиницыКоличества = 1
			ТОГДА ВЫБОР
					КОГДА Затраты.АналитикаУчетаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов &lt;&gt; 0
						ТОГДА Затраты.КоличествоКонечныйОстаток / Затраты.АналитикаУчетаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов
					ИНАЧЕ 0
				КОНЕЦ
	КОНЕЦ КАК КоличествоКонечныйОстатокНаЭтапы,
	
	0 КАК СтоимостьНачальныйОстаток,
	0 КАК СтоимостьПриход,
	0 КАК СтоимостьРасходНаВыпуск,
	0 КАК СтоимостьРасходПрочий,
	0 КАК СтоимостьКонечныйОстаток,
	
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура КАК Номенклатура
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства.ОстаткиИОбороты({(&amp;НачалоПериода)},{(&amp;КонецПериода)}) КАК Затраты
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ВидыЗапасов КАК СпрВидыЗапасов
		ПО (СпрВидыЗапасов.Ссылка = Затраты.ВидЗапасов)
ГДЕ
	Затраты.АналитикаУчетаНоменклатуры.МестоХранения ССЫЛКА Справочник.СтруктураПредприятия
{ГДЕ
	((Затраты.АналитикаУчетаНоменклатуры.Номенклатура, Затраты.АналитикаУчетаНоменклатуры.Характеристика) В
		(ВЫБРАТЬ
			ОтборПоСегментуНоменклатуры.Номенклатура,
			ОтборПоСегментуНоменклатуры.Характеристика
		ИЗ
			ОтборПоСегментуНоменклатуры
		ГДЕ
			ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
	)
}
ОБЪЕДИНИТЬ ВСЕ
	
ВЫБРАТЬ
	"МатериальныеРаспределенные",
	&amp;МатериальныеЗатраты,
	ИСТИНА,
	Затраты.Организация,
	ВЫБОР
		КОГДА &amp;ДанныеОтчета = 3
			ТОГДА Затраты.Организация.ВалютаРегламентированногоУчета
		ИНАЧЕ &amp;ВалютаУправленческогоУчета
	КОНЕЦ КАК Валюта,
	Затраты.ЗаказНаПроизводство КАК ЗаказНаПроизводство,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура.ЕдиницаИзмерения,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура.ЕдиницаДляОтчетов,
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов,
	Затраты.АналитикаУчетаНоменклатуры.Характеристика,
	Затраты.АналитикаУчетаНоменклатуры.Серия,
	Затраты.АналитикаУчетаНоменклатуры.МестоХранения,
	ЕСТЬNULL(СпрВидыЗапасов.ГруппаПродукции, ЗНАЧЕНИЕ(Справочник.ГруппыАналитическогоУчетаНоменклатуры.ПустаяСсылка)),
	Затраты.АналитикаУчетаНоменклатуры.Назначение,
	Затраты.Регистратор,
	
	0 КАК КоличествоНачальныйОстаток,
	0 КАК КоличествоНачальныйОстатокНеРаспределено,
	0 КАК КоличествоНачальныйОстатокНаЭтапы,
	
	0 КАК КоличествоПриход,
	СУММА(ВЫБОР
			КОГДА Затраты.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				ТОГДА Затраты.Количество
			ИНАЧЕ 0
		КОНЕЦ
	)КАК КоличествоПриходНаЭтапы,
	
	СУММА(ВЫБОР
			КОГДА Затраты.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				ТОГДА Затраты.Количество
			ИНАЧЕ 0
		КОНЕЦ
	) КАК КоличествоРасходНаЭтапы,
	СУММА(ВЫБОР
			КОГДА Затраты.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
				И Не СпрВидыЗапасов.Ссылка ЕСТЬ NULL И СпрВидыЗапасов.ТипЗапасов = ЗНАЧЕНИЕ(Перечисление.ТипыЗапасов.ПолуфабрикатДавальца)
				ТОГДА Затраты.Количество
			ИНАЧЕ 0
		КОНЕЦ
	) КАК КоличествоРасходНаВыпуск,
	0 КАК КоличествоРасходПрочий,
	
	0 КАК КоличествоКонечныйОстаток,
	0 КАК КоличествоКонечныйОстатокНеРаспределено,
	0 КАК КоличествоКонечныйОстатокНаЭтапы,
	
	0 КАК СтоимостьНачальныйОстаток,
	0 КАК СтоимостьПриход,
	0 КАК СтоимостьРасходНаВыпуск,
	0 КАК СтоимостьРасходПрочий,
	0 КАК СтоимостьКонечныйОстаток,
	
	Затраты.АналитикаУчетаНоменклатуры.Номенклатура
ИЗ
	РегистрНакопления.ПартииНезавершенногоПроизводства КАК Затраты

	ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ВидыЗапасов КАК СпрВидыЗапасов
		ПО (СпрВидыЗапасов.Ссылка = Затраты.ВидЗапасов)
	ЛЕВОЕ СОЕДИНЕНИЕ Константы КАК Константы
		ПО (ИСТИНА)
ГДЕ
	((Затраты.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода)
		ИЛИ &amp;КонецПериода = ДатаВремя(1,1,1))
	И Затраты.АналитикаУчетаНоменклатуры.ТипМестаХранения = ЗНАЧЕНИЕ(Перечисление.ТипыМестХранения.Подразделение)
	И (Затраты.Период &lt; Константы.ДатаПереходаНаПартионныйУчетВерсии22
		ИЛИ НЕ Константы.ПартионныйУчетВерсии22)
{ГДЕ
	((Затраты.АналитикаУчетаНоменклатуры.Номенклатура, Затраты.АналитикаУчетаНоменклатуры.Характеристика) В
			(ВЫБРАТЬ
				ОтборПоСегментуНоменклатуры.Номенклатура,
				ОтборПоСегментуНоменклатуры.Характеристика
			ИЗ
				ОтборПоСегментуНоменклатуры
			ГДЕ
				ОтборПоСегментуНоменклатуры.ИспользуетсяОтборПоСегментуНоменклатуры = &amp;ИспользуетсяОтборПоСегментуНоменклатуры)
	)
}
