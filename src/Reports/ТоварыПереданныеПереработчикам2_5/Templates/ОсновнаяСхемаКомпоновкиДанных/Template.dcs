///////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТоварыОрганизаций.Регистратор                                   КАК Регистратор,
	
	ТоварыОрганизаций.ПериодСекунда                                 КАК ПериодСекунда,
	ТоварыОрганизаций.ПериодДень                                    КАК ПериодДень,
	ТоварыОрганизаций.ПериодНеделя                                  КАК ПериодНеделя,
	ТоварыОрганизаций.ПериодДекада                                  КАК ПериодДекада,
	ТоварыОрганизаций.ПериодМесяц                                   КАК ПериодМесяц,
	ТоварыОрганизаций.ПериодКвартал                                 КАК ПериодКвартал,
	ТоварыОрганизаций.ПериодПолугодие                               КАК ПериодПолугодие,
	ТоварыОрганизаций.ПериодГод                                     КАК ПериодГод,
	
	ТоварыОрганизаций.Организация                                   КАК Организация,
	АналитикаНоменклатуры.Номенклатура                              КАК Номенклатура,
	АналитикаНоменклатуры.Номенклатура.ЕдиницаИзмерения             КАК ЕдиницаХранения,
	АналитикаНоменклатуры.Номенклатура.ЕдиницаДляОтчетов            КАК ЕдиницаДляОтчетов,
	АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов КАК КоэффициентЕдиницыДляОтчетов,
	&amp;ТекстЗапросаВесНоменклатуры                                    КАК НоменклатураВес,
	&amp;ТекстЗапросаОбъемНоменклатуры                                  КАК НоменклатураОбъем,
	АналитикаНоменклатуры.Характеристика                            КАК Характеристика,
	АналитикаНоменклатуры.Назначение                                КАК Назначение,
	АналитикаНоменклатуры.Серия                                     КАК Серия,
	АналитикаНоменклатуры.Партнер                                   КАК Переработчик,
	АналитикаНоменклатуры.Договор                                   КАК Договор,
	ТоварыОрганизаций.ВидЗапасов                                    КАК ВидЗапасов,
	
	0                                                               КАК КПередаче,
	0                                                               КАК КПередачеВес,
	0                                                               КАК КПередачеОбъем,
	
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			ИЛИ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5)
			ТОГДА 0
		КОГДА &amp;ЕдиницыКоличества = 0
			ТОГДА ТоварыОрганизаций.КоличествоПриход
		КОГДА &amp;ЕдиницыКоличества = 1
			ТОГДА
				ВЫБОР
					КОГДА АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов &lt;&gt; 0
						ТОГДА ТоварыОрганизаций.КоличествоПриход / АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов
					ИНАЧЕ 0
				КОНЕЦ
	КОНЕЦ                                                                         КАК КоличествоПриход,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			ИЛИ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5)
			ТОГДА 0
		ИНАЧЕ ТоварыОрганизаций.КоличествоПриход * &amp;ТекстЗапросаВесНоменклатуры
	КОНЕЦ                                                                         КАК ПриходВес,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			ИЛИ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5)
			ТОГДА 0
		ИНАЧЕ ТоварыОрганизаций.КоличествоПриход * &amp;ТекстЗапросаОбъемНоменклатуры
	КОНЕЦ                                                                         КАК ПриходОбъем,
	
	ВЫБОР
		КОГДА НЕ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			И НЕ (ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5))
			ТОГДА 0
		КОГДА &amp;ЕдиницыКоличества = 0
			ТОГДА ТоварыОрганизаций.КоличествоПриход
		КОГДА &amp;ЕдиницыКоличества = 1
			ТОГДА
				ВЫБОР
					КОГДА АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов &lt;&gt; 0
						ТОГДА ТоварыОрганизаций.КоличествоПриход / АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов
					ИНАЧЕ 0
				КОНЕЦ
	КОНЕЦ                                                                         КАК КоличествоПроизведено,
	ВЫБОР
		КОГДА НЕ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			И НЕ (ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5))
			ТОГДА 0
		ИНАЧЕ ТоварыОрганизаций.КоличествоПриход * &amp;ТекстЗапросаВесНоменклатуры
	КОНЕЦ                                                                         КАК ПроизведеноВес,
	ВЫБОР
		КОГДА НЕ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			И НЕ (ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5))
			ТОГДА 0
		ИНАЧЕ ТоварыОрганизаций.КоличествоПриход * &amp;ТекстЗапросаОбъемНоменклатуры
	КОНЕЦ                                                                         КАК ПроизведеноОбъем,
	
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			ИЛИ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5)
			ТОГДА
				ВЫБОР
					КОГДА &amp;ЕдиницыКоличества = 0
						ТОГДА ТоварыОрганизаций.КоличествоРасход
					КОГДА &amp;ЕдиницыКоличества = 1
						ТОГДА
							ВЫБОР
								КОГДА АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов &lt;&gt; 0
									ТОГДА ТоварыОрганизаций.КоличествоРасход
											/ АналитикаНоменклатуры.Номенклатура.КоэффициентЕдиницыДляОтчетов
								ИНАЧЕ 0
							КОНЕЦ
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ                                                           КАК КоличествоИзрасходовано,
	
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.ОтчетПереработчика2_5)
			ИЛИ ТИПЗНАЧЕНИЯ(ТоварыОрганизаций.Регистратор) = ТИП(Документ.Сторно)
				И ТИПЗНАЧЕНИЯ(ВЫРАЗИТЬ(ТоварыОрганизаций.Регистратор КАК Документ.Сторно).СторнируемыйДокумент) = ТИП(Документ.ОтчетПереработчика2_5)
			ТОГДА ТоварыОрганизаций.КоличествоРасход * &amp;ТекстЗапросаВесНоменклатуры
		ИНАЧЕ 0
