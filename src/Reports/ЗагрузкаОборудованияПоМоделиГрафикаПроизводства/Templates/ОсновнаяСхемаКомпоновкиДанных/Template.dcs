	<calculatedField>
		<dataPath>ТипЗагрузкиПерегрузки</dataPath>
		<expression>ВЫБОР
	КОГДА Перегрузка &gt; 0
		ТОГДА
			ВЫБОР
				КОГДА Доступно &gt; 0
					ТОГДА 
						ВЫБОР
							КОГДА (Перегрузка/Доступно * 100) &lt;= 100
								ТОГДА 3
							КОГДА (Перегрузка/Доступно * 100) &lt;= 200
								ТОГДА 4
							ИНАЧЕ 5
						КОНЕЦ
				ИНАЧЕ 6
			КОНЕЦ
	ИНАЧЕ
		ВЫБОР
			КОГДА Доступно &gt; 0
