&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяТекущейСтраницы = "", ОбластьИмя = "") Экспорт
	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет ИЛИ Форма.СтруктураРеквизитовФормы.мРежимПечати Тогда
		Возврат;
	КонецЕсли;
	Если Форма.СтруктураРеквизитовФормы.ОтключитьВыполнениеРасчета Тогда
		Возврат;
	КонецЕсли;
	
	Области = Форма.ТабличныйДокумент.Области;
	СредОбластьИмя = Сред(ОбластьИмя, 10);
	
	Если СтрНачинаетсяС(ОбластьИмя, "П1100006_") Или СтрНачинаетсяС(ОбластьИмя, "П1100007_")  Или СтрНачинаетсяС(ОбластьИмя, "П1100008_") Тогда 
		О6 = Области.Найти("П1100006_" + СредОбластьИмя);
		О7 = Области.Найти("П1100007_" + СредОбластьИмя);
		О8 = Области.Найти("П1100008_" + СредОбластьИмя);
		О9 = Области.Найти("П1100009_" + СредОбластьИмя);
		О10 = Области.Найти("П1100010_" + СредОбластьИмя);
		
		О9.Значение = О7.Значение * О8.Значение;
		О10.Значение = ((О6.Значение * О9.Значение)/1000.0);
		СохранитьЗначениеПоказателяВСтруктуре(Форма, "ТовУпакНеИсп", О9.Имя, О9.Значение);
		СохранитьЗначениеПоказателяВСтруктуре(Форма, "ТовУпакНеИсп", О10.Имя, О10.Значение);
	ИначеЕсли СтрНачинаетсяС(ОбластьИмя, "П2100010_") Или СтрНачинаетсяС(ОбластьИмя, "П2100009_") Или СтрНачинаетсяС(ОбластьИмя, "П2100014_") Тогда 
		О1 = Области.Найти("П2100009_" + СредОбластьИмя);
		О2 = Области.Найти("П2100010_" + СредОбластьИмя);
		О3 = Области.Найти("П2100011_" + СредОбластьИмя);
		О4 = Области.Найти("П2100014_" + СредОбластьИмя);
		О5 = Области.Найти("П2100015_" + СредОбластьИмя);
		
		О3.Значение = О1.Значение - О2.Значение;
		О5.Значение = ((О3.Значение * О4.Значение) / 1000);
		СохранитьЗначениеПоказателяВСтруктуре(Форма, "ТовУпакИсп", О3.Имя, О3.Значение);
		СохранитьЗначениеПоказателяВСтруктуре(Форма, "ТовУпакИсп", О5.Имя, О5.Значение);
	ИначеЕсли СтрНачинаетсяС(ОбластьИмя, "П3100008_")  Или СтрНачинаетсяС(ОбластьИмя, "П3100009_") Тогда 
		О1 = Области.Найти("П3100007_" + СредОбластьИмя);
		О2 = Области.Найти("П3100008_" + СредОбластьИмя);
		О3 = Области.Найти("П3100009_" + СредОбластьИмя);
		О4 = Области.Найти("П3100010_" + СредОбластьИмя);
		О5 = Области.Найти("П3100011_" + СредОбластьИмя);
		
		О4.Значение = О2.Значение * О3.Значение;
		О5.Значение = ((О1.Значение * О4.Значение) / 1000);
		СохранитьЗначениеПоказателяВСтруктуре(Форма, "Упаковка", О4.Имя, О4.Значение);
		СохранитьЗначениеПоказателяВСтруктуре(Форма, "Упаковка", О5.Имя, О5.Значение);
	ИначеЕсли ОбластьИмя = "П040101" Или ОбластьИмя = "П040201" Или ОбластьИмя = "П040301" Тогда
		Области.Найти("П040103").Значение = Области.Найти("П040100").Значение - Области.Найти("П040101").Значение;
		Области.Найти("П040203").Значение = Области.Найти("П040200").Значение - Области.Найти("П040201").Значение;
		Области.Найти("П040303").Значение = Области.Найти("П040300").Значение - Области.Найти("П040301").Значение;
	ИначеЕсли Не ЗначениеЗаполнено(ОбластьИмя) Тогда 
		Для Каждого СтрОбласть Из СтрРазделить("П1100006_1,П2100010_1,П3100008_1", ",") Цикл 
			Обл = Области.Найти(СтрОбласть);
			Если ТипЗнч(Обл) = Тип("ОбластьЯчеекТабличногоДокумента") Тогда
				Расчет(Форма, ИмяТекущейСтраницы, СтрОбласть);
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Процедура РасчетИтогов(Форма)
	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет ИЛИ Форма.СтруктураРеквизитовФормы.мРежимПечати Тогда
		Возврат;
	КонецЕсли;
	
	Области = Форма.ТабличныйДокумент.Области;
	П01 = 0;
	П02 = 0;
	П03 = 0;
	
	Для Каждого Стр0 Из Форма.ТаблицаСтраницыТовУпакНеИсп Цикл 
		Для Каждого Стр1 Из Стр0.ДанныеДопСтрок Цикл 
			Для Каждого Стр2 Из Стр1.Значение.П11000 Цикл 
				П01 = П01 + Стр2.П1100010;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	Для Каждого Стр0 Из Форма.ТаблицаСтраницыТовУпакИсп Цикл 
		Для Каждого Стр1 Из Стр0.ДанныеДопСтрок Цикл 
			Для Каждого Стр2 Из Стр1.Значение.П21000 Цикл 
				П02 = П02 + Стр2.П2100015;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	Для Каждого Стр0 Из Форма.ТаблицаСтраницыУпаковка Цикл 
		Для Каждого Стр1 Из Стр0.ДанныеДопСтрок Цикл 
			Для Каждого Стр2 Из Стр1.Значение.П31000 Цикл 
				П03 = П03 + Стр2.П3100011;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	Форма.СтруктураДанныхИтоги.П040100 = П01;
	Форма.СтруктураДанныхИтоги.П040200 = П02;
	Форма.СтруктураДанныхИтоги.П040300 = П03;
	Форма.СтруктураДанныхИтоги.П040103 = П01 - Форма.СтруктураДанныхИтоги.П040101;
	Форма.СтруктураДанныхИтоги.П040203 = П02 - Форма.СтруктураДанныхИтоги.П040201;
	Форма.СтруктураДанныхИтоги.П040303 = П03 - Форма.СтруктураДанныхИтоги.П040301;
	
	Если Области.Найти("П040100") <> Неопределено Тогда 
		Для Каждого Стр Из СтрРазделить("П040100,П040200,П040300,П040103,П040203,П040303", ",") Цикл 
			Области.Найти(Стр).Значение = Форма.СтруктураДанныхИтоги[Стр];
		КонецЦикла;
	КонецЕсли;
	Форма.Модифицированность = Истина;
КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Процедура ПолныйПерерасчет(Форма)
	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет ИЛИ Форма.СтруктураРеквизитовФормы.мРежимПечати Тогда
		Возврат;
	КонецЕсли;
	
	П01 = 0;
	П02 = 0;
	П03 = 0;
	Для Каждого Стр Из Форма.ТаблицаСтраницыТовУпакНеИсп Цикл
		Для Каждого ДДС Из Стр.ДанныеДопСтрок Цикл 
			Для Каждого СтрМнг Из ДДС.Значение.П11000 Цикл
				СтрМнг.П1100009 = СтрМнг.П1100007 * СтрМнг.П1100008;
				СтрМнг.П1100010 = СтрМнг.П1100006 * СтрМнг.П1100009 / 1000;
				П01 = П01 + СтрМнг.П1100010;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	Для Каждого Стр Из Форма.ТаблицаСтраницыТовУпакИсп Цикл
		Для Каждого ДДС Из Стр.ДанныеДопСтрок Цикл 
			Для Каждого СтрМнг Из ДДС.Значение.П21000 Цикл
				СтрМнг.П2100011 = СтрМнг.П2100009 - СтрМнг.П2100010;
				СтрМнг.П2100015 = (СтрМнг.П2100014 * СтрМнг.П2100011) / 1000;
				П02 = П02 + СтрМнг.П2100015;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	Для Каждого Стр Из Форма.ТаблицаСтраницыУпаковка Цикл
		Для Каждого ДДС Из Стр.ДанныеДопСтрок Цикл 
			Для Каждого СтрМнг Из ДДС.Значение.П31000 Цикл
				СтрМнг.П3100010 = СтрМнг.П3100008 * СтрМнг.П3100009;
				СтрМнг.П3100011 = (СтрМнг.П3100010 * СтрМнг.П3100007) / 1000;
				П03 = П03 + СтрМнг.П3100011;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	Области = Форма.ТабличныйДокумент.Области;
	П040100 = Области.Найти("П040100");
	Если ТипЗнч(П040100) = Тип("ОбластьЯчеекТабличногоДокумента") Тогда 
		П040100.Значение = П01;
		Области.Найти("П040200").Значение = П02;
		Области.Найти("П040300").Значение = П03;
		Области.Найти("П040103").Значение = П01 - Области.Найти("П040101").Значение;
		Области.Найти("П040203").Значение = П02 - Области.Найти("П040201").Значение;
		Области.Найти("П040303").Значение = П03 - Области.Найти("П040301").Значение;
		СобратьДанныеТекущегоТаблПоля(Форма, "ТабличныйДокумент");
	КонецЕсли;
	
	Форма.СтруктураДанныхИтоги.П040100 = П01;
	Форма.СтруктураДанныхИтоги.П040200 = П02;
	Форма.СтруктураДанныхИтоги.П040300 = П03;
	Форма.СтруктураДанныхИтоги.П040103 = П01 - Форма.СтруктураДанныхИтоги.П040101;
	Форма.СтруктураДанныхИтоги.П040203 = П02 - Форма.СтруктураДанныхИтоги.П040201;
	Форма.СтруктураДанныхИтоги.П040303 = П03 - Форма.СтруктураДанныхИтоги.П040301;
КонецПроцедуры

&НаСервере
Процедура УдалитьДополнительнуюСтраницуНаСервере(ИмяТекущейСтраницыПанели = Неопределено)
	ТаблицаСтраниц = ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(мСтруктураМногостраничныхРазделов, ИмяТекущейСтраницыПанели);
	СтрТаблицаСтраницы = ЭтотОбъект[ТаблицаСтраниц].НайтиСтроки(Новый Структура("АктивнаяСтраница", Истина));
	ИндексТекСтраницыРаздела = ЭтотОбъект[ТаблицаСтраниц].Индекс(СтрТаблицаСтраницы[0]);
	ИзменитьТаблицуПараметровАвтозаполнения(-1, ИмяТекущейСтраницыПанели);
	СтруктураРеквизитовФормы.ОтключитьВыполнениеРасчета = Истина;
	РегламентированнаяОтчетность.УдалитьДополнительнуюСтраницуНаСервере(ЭтотОбъект, ИмяТекущейСтраницыПанели);
	СтруктураРеквизитовФормы.ОтключитьВыполнениеРасчета = Ложь;
	СтруктураРеквизитовФормы.ВыводитьСтраницуВТаблДокНаСервере = Ложь;
КонецПроцедуры

