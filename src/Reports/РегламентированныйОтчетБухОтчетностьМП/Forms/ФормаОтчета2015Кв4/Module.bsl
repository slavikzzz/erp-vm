&НаКлиентеНаСервереБезКонтекста
Процедура Расчет(Форма, ИмяСтраницыПанели = "", Знач ОбластьИмя = "")
	
	Если Форма.СтруктураРеквизитовФормы.ФлажокОтклАвтоРасчет Тогда
		Если НЕ ЗначениеЗаполнено(ИмяСтраницыПанели) ИЛИ ИмяСтраницыПанели = "ОЦИС" Тогда
			// Синтетические показатели для выгрузки рассчитываются всегда
			Расчет_ОЦИС_6200_6300(Форма, ОбластьИмя);
		КонецЕсли;
		
		Возврат;
		
	КонецЕсли;
	
	Если ИмяСтраницыПанели = "Баланс"
	 ИЛИ ИмяСтраницыПанели = "БалансНКО" Тогда
		Расчет_Баланс(Форма, ОбластьИмя);
	КонецЕсли;
	
	Если ИмяСтраницыПанели = "ОПУ" Тогда
		Расчет_ОПУ(Форма, ОбластьИмя);
	КонецЕсли;
	
	Если ИмяСтраницыПанели = "ОИК" Тогда
		Расчет_ОИК(Форма, ОбластьИмя);
	КонецЕсли;
	
	Если ИмяСтраницыПанели = "ОДДС" Тогда
		Расчет_ОДДС(Форма, ОбластьИмя);
	КонецЕсли;
	
	Если ИмяСтраницыПанели = "ОЦИС" Тогда
		Расчет_ОЦИС(Форма, ОбластьИмя);
		Расчет_ОЦИС_6200_6300(Форма, ОбластьИмя);
	КонецЕсли;
	
	Если ИмяСтраницыПанели = "" Тогда
		Расчет(Форма, "Баланс", ОбластьИмя);
		Расчет(Форма, "ОПУ",    ОбластьИмя);
		Расчет(Форма, "ОИК",    ОбластьИмя);
		Расчет(Форма, "ОДДС",   ОбластьИмя);
		Расчет(Форма, "ОЦИС",   ОбластьИмя);
	КонецЕсли;
	
КонецПроцедуры

&НаКлиентеНаСервереБезКонтекста
Процедура Расчет_Баланс(Форма, ОбластьИмя)
	
	ЭтоБалансНКО = Форма.СтруктураРеквизитовФормы.ЭтоБалансНекоммерческойОрганизации;
	
	Если ЭтоБалансНКО Тогда
		РазмещениеДанных = МестоРазмещенияДанных(Форма, "БалансНКО");
	Иначе
		РазмещениеДанных = МестоРазмещенияДанных(Форма, "Баланс");
	КонецЕсли;
	
	Для Графа = 3 По 5 Цикл
		// Актив
		Если НЕ ЗначениеЗаполнено(ОбластьИмя)
		 ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета["П00001000060" + Графа], ОбластьИмя) > 0 Тогда
			РасчетСтроки1600 = ПоказательОтчета(Форма, РазмещениеДанных, "П00001000010" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000020" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000030" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000040" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000050" + Графа);
			
			УстановитьЗначениеПоказателя(РазмещениеДанных, "П00001000060" + Графа, РасчетСтроки1600);
			
		КонецЕсли;
		
		// Пассив
		Если НЕ ЗначениеЗаполнено(ОбластьИмя)
		 ИЛИ СтрЧислоВхождений(Форма.СтруктураЗависимыхЯчеекДляАвторасчета["П00001000130" + Графа], ОбластьИмя) > 0 Тогда
			Если ЭтоБалансНКО Тогда
				РасчетСтроки1700 = ПоказательОтчета(Форма, РазмещениеДанных, "П00001000710" + Графа)
					+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000720" + Графа);
			Иначе
				РасчетСтроки1700 = ПоказательОтчета(Форма, РазмещениеДанных, "П00001000070" + Графа);
			КонецЕсли;
			
			РасчетСтроки1700 = РасчетСтроки1700
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000080" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000090" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000100" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000110" + Графа)
				+ ПоказательОтчета(Форма, РазмещениеДанных, "П00001000120" + Графа);
			
			УстановитьЗначениеПоказателя(РазмещениеДанных, "П00001000130" + Графа, РасчетСтроки1700);
			
		КонецЕсли;
		
