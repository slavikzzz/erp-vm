			?(НЕ РезультатПоиска = Неопределено И РезультатПоиска.ПоказатьСтраницу = 1, Истина, Ложь);
	КонецЦикла;
	
	ВидимыеРазделы.Вставить("Баланс", ВидимыеРазделы[?(ДанныеКС.НКО, "БалансНКО", "Баланс")]);
	
