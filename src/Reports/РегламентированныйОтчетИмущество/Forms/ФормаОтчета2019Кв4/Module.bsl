			?(Настройки.ЭтоОшибка, "не ", ""));
		ДополнитьСтруктуруОшибок(Настройки);
		
	КонецЦикла;
	
	СтруктураРезультата = ОбработатьРезультат(Настройки, СтруктураРезультата);
	
