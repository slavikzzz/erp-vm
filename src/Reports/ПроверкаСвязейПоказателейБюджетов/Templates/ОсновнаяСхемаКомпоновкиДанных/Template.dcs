	<totalField>
		<dataPath>КонечныйОстатокПоОборотамСтатей</dataPath>
		<expression>ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(ПриходПоОборотамСтатей),0) - ЕстьNULL(Сумма(РасходПоОборотамСтатей),0)</expression>
	</totalField>
	<totalField>
		<dataPath>Отклонение</dataPath>
		<expression>ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(ПриходПоОборотамСтатей),0) - ЕстьNULL(Сумма(РасходПоОборотамСтатей),0) - ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0)</expression>
	</totalField>
	<totalField>
		<dataPath>ПроцентОтклонения</dataPath>
		<expression>ВЫБОР 
	КОГДА ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0) = 0 И (ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(ПриходПоОборотамСтатей),0) - ЕстьNULL(Сумма(РасходПоОборотамСтатей),0)) = 0
		ТОГДА 0
	КОГДА ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0) = 0 
		ТОГДА 1
	КОГДА (ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(ПриходПоОборотамСтатей),0) - ЕстьNULL(Сумма(РасходПоОборотамСтатей),0) - Сумма(КонечныйОстатокПоПравилам)) / Сумма(КонечныйОстатокПоПравилам) &lt; 0
		ТОГДА -(ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(ПриходПоОборотамСтатей),0) - ЕстьNULL(Сумма(РасходПоОборотамСтатей),0) - Сумма(КонечныйОстатокПоПравилам)) / Сумма(КонечныйОстатокПоПравилам)
