		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ГрафикЭтаповПроизводства2_2.Распоряжение                             КАК Распоряжение,
	ГрафикЭтаповПроизводства2_2.ЭтапПроизводства                         КАК ЭтапПроизводства,
	ЭтапПроизводства2_2.Спецификация                                     КАК Спецификация,
	ЭтапПроизводства2_2.НазначениеПродукция                              КАК Назначение,
	ЭтапПроизводства2_2.Подразделение                                    КАК Подразделение,
	ГрафикЭтаповПроизводства2_2.ЭтапПроизводства                         КАК ЭтапПродукция,
	ВЫРАЗИТЬ(&amp;ОбозначениеЭтап КАК СТРОКА(25))                            КАК ЭтапПродукцияОбозначение,
	1                                                                    КАК ЭтапПродукцияПорядок,
	ВЫБОР
		КОГДА ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаФакт
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаОтменено
			ТОГДА ЭтапПроизводства2_2.УпаковкаПлан
		ИНАЧЕ ЭтапПроизводства2_2.ПартияПроизводства.ОсновноеИзделиеНоменклатура.ЕдиницаИзмерения
	КОНЕЦ                                                                КАК ЕдиницаИзмерения,
	ЭтапПроизводства2_2.Статус В (
		ЗНАЧЕНИЕ(Перечисление.СтатусыЭтаповПроизводства2_2.Начат),
		ЗНАЧЕНИЕ(Перечисление.СтатусыЭтаповПроизводства2_2.Завершен))    КАК ЗапускВыполнен,
	
	ВЫБОР
		КОГДА &amp;ВариантДатыГрафика = 0
			ТОГДА ГрафикЭтаповПроизводства2_2.НачалоЭтапа
		ИНАЧЕ ГрафикЭтаповПроизводства2_2.ОкончаниеЭтапа
	КОНЕЦ КАК ДатаГрафикаПлан,
	ВЫБОР
		КОГДА НЕ ЭтапПроизводства2_2.Статус В (
					ЗНАЧЕНИЕ(Перечисление.СтатусыЭтаповПроизводства2_2.Начат), 
					ЗНАЧЕНИЕ(Перечисление.СтатусыЭтаповПроизводства2_2.Завершен))
			ТОГДА ДАТАВРЕМЯ(1, 1, 1)
		КОГДА &amp;ВариантДатыГрафика = 0
			ТОГДА ЭтапПроизводства2_2.ФактическоеНачалоЭтапа
		КОГДА ЭтапПроизводства2_2.ФактическоеОкончаниеЭтапа = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ГрафикЭтаповПроизводства2_2.ОкончаниеЭтапа
		ИНАЧЕ ЭтапПроизводства2_2.ФактическоеОкончаниеЭтапа
	КОНЕЦ                                                                КАК ДатаГрафикаФакт,
	
	ВЫБОР
		КОГДА ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаФакт
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаОтменено
			ТОГДА ЭтапПроизводства2_2.КоличествоУпаковокПлан
		ИНАЧЕ ЭтапПроизводства2_2.КоличествоУпаковокПлан * ЕСТЬNULL(&amp;КоэффициентУпаковкиПлан, 1)
	КОНЕЦ                                                                КАК КоличествоПлановое,
	ВЫБОР
		КОГДА ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаФакт
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаОтменено
			ТОГДА ЭтапПроизводства2_2.КоличествоУпаковокОтменено
		ИНАЧЕ ЭтапПроизводства2_2.КоличествоУпаковокОтменено * ЕСТЬNULL(&amp;КоэффициентУпаковкиОтменено, 1)
	КОНЕЦ                                                                КАК КоличествоОтменено,
	ВЫБОР
		КОГДА НЕ ЭтапПроизводства2_2.Статус В (
					ЗНАЧЕНИЕ(Перечисление.СтатусыЭтаповПроизводства2_2.Начат), 
					ЗНАЧЕНИЕ(Перечисление.СтатусыЭтаповПроизводства2_2.Завершен))
			ТОГДА 0
		КОГДА &amp;ВариантДатыГрафика = 0 
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаФакт
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаОтменено
			ТОГДА ЭтапПроизводства2_2.КоличествоУпаковокПлан
		КОГДА &amp;ВариантДатыГрафика = 0
				И (ЭтапПроизводства2_2.УпаковкаПлан &lt;&gt; ЭтапПроизводства2_2.УпаковкаФакт
					ИЛИ ЭтапПроизводства2_2.УпаковкаПлан &lt;&gt; ЭтапПроизводства2_2.УпаковкаОтменено)
			ТОГДА ЭтапПроизводства2_2.КоличествоУпаковокПлан * ЕСТЬNULL(&amp;КоэффициентУпаковкиПлан, 1)
		КОГДА &amp;ВариантДатыГрафика = 1 
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаФакт
				И ЭтапПроизводства2_2.УпаковкаПлан = ЭтапПроизводства2_2.УпаковкаОтменено
			ТОГДА ЭтапПроизводства2_2.КоличествоУпаковокФакт
		ИНАЧЕ ЭтапПроизводства2_2.КоличествоУпаковокФакт * ЕСТЬNULL(&amp;КоэффициентУпаковкиФакт, 1)
	КОНЕЦ                                                                КАК КоличествоФактическое,
	
	ГрафикЭтаповПроизводства2_2.ОкончаниеЭтапа                           КАК ОкончаниеЭтапа,
	ЭтапПроизводства2_2.ПартияПроизводства.ОсновноеИзделиеНоменклатура   КАК Номенклатура,
