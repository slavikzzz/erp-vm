	Стр = Результат.Добавить();
	Стр.ИмяФормы = "Форма2024_Постановка";
	Стр.ОписаниеФормы = "В соответствии с приказом ФНС России от 20.08.2024 № БВ-7-14/665@";
	Стр.ВидУведомления = Перечисления.ВидыУведомленийОСпецрежимахНалогообложения.ПостановкаНаУчетИностранцаФНС;
	
	Стр = Результат.Добавить();
	Стр.ИмяФормы = "Форма2024_Изменение";
	Стр.ОписаниеФормы = "В соответствии с приказом ФНС России от 20.08.2024 № БВ-7-14/665@";
	Стр.ВидУведомления = Перечисления.ВидыУведомленийОСпецрежимахНалогообложения.ИзменениеСведенийИностранцаФНС;
	
	Стр = Результат.Добавить();
	Стр.ИмяФормы = "Форма2024_Снятие";
	Стр.ОписаниеФормы = "В соответствии с приказом ФНС России от 20.08.2024 № БВ-7-14/665@";
	Стр.ВидУведомления = Перечисления.ВидыУведомленийОСпецрежимахНалогообложения.СнятиеСУчетаИностранцаФНС;
	
	Возврат Результат;
