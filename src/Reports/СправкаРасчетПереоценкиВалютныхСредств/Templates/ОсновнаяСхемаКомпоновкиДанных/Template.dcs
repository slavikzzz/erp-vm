				<dataSource>ИсточникДанных1</dataSource>
				<query>
//++ Локализация

//++ НЕ УТ
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	&amp;НачалоПериода КАК Период,
	НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ) КАК ДатаОстатка,
	НЕОПРЕДЕЛЕНО КАК Регистратор,
	ДенежныеСредства.ДенежныйДокумент.Валюта КАК Валюта,
	ДенежныеСредства.ДенежныйДокумент КАК ДенежныйДокумент,
	ДенежныеСредства.Организация КАК Организация,
	ЗНАЧЕНИЕ(Справочник.НаправленияДеятельности.ПустаяСсылка) КАК НаправлениеДеятельности,
	ДенежныеСредства.Подразделение КАК Подразделение,
	ДенежныеСредства.СуммаОстаток КАК Сумма,
	ДенежныеСредства.СуммаУпрОстаток КАК СуммаУпр,
	ДенежныеСредства.СуммаРеглОстаток КАК СуммаРегл,
	0 КАК ДоходУпр,
	0 КАК РасходУпр,
	0 КАК ДоходРегл,
	0 КАК РасходРегл
ПОМЕСТИТЬ втОстаткиИОбороты
ИЗ
	РегистрНакопления.ДенежныеДокументы.Остатки(
			&amp;Период,
			ДенежныйДокумент.Валюта &lt;&gt; &amp;ВалютаУпр
				ИЛИ ДенежныйДокумент.Валюта &lt;&gt; Организация.ВалютаРегламентированногоУчета {(Организация).*, (Подразделение).*, (МОЛ).*, (ДенежныйДокумент).*}) КАК ДенежныеСредства

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ДенежныеСредства.Период КАК Период,
	НАЧАЛОПЕРИОДА(ДенежныеСредства.Период, ДЕНЬ) КАК ДатаОстатка,
	ДенежныеСредства.Регистратор КАК Регистратор,
	ДенежныеСредства.ДенежныйДокумент.Валюта КАК Валюта,
	ДенежныеСредства.ДенежныйДокумент КАК ДенежныйДокумент,
	ДенежныеСредства.Организация КАК Организация,
	ЗНАЧЕНИЕ(Справочник.НаправленияДеятельности.ПустаяСсылка) КАК НаправлениеДеятельности,
	ДенежныеСредства.Подразделение КАК Подразделение,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
			ТОГДА ДенежныеСредства.Сумма
		ИНАЧЕ -ДенежныеСредства.Сумма
	КОНЕЦ КАК Сумма,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
			ТОГДА ДенежныеСредства.СуммаУпр
		ИНАЧЕ -ДенежныеСредства.СуммаУпр
	КОНЕЦ КАК СуммаУпр,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
			ТОГДА ДенежныеСредства.СуммаРегл
		ИНАЧЕ -ДенежныеСредства.СуммаРегл
	КОНЕЦ КАК СуммаРегл,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				И ТИПЗНАЧЕНИЯ(ДенежныеСредства.Регистратор) = ТИП(Документ.РасчетКУрсовыхРазниц)
			ТОГДА ДенежныеСредства.СуммаУпр
		ИНАЧЕ 0
	КОНЕЦ КАК ДоходУпр,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
				И ТИПЗНАЧЕНИЯ(ДенежныеСредства.Регистратор) = ТИП(Документ.РасчетКУрсовыхРазниц)
			ТОГДА -ДенежныеСредства.СуммаУпр
		ИНАЧЕ 0
	КОНЕЦ КАК РасходУпр,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				И ТИПЗНАЧЕНИЯ(ДенежныеСредства.Регистратор) = ТИП(Документ.РасчетКУрсовыхРазниц)
			ТОГДА ДенежныеСредства.СуммаРегл
		ИНАЧЕ 0
	КОНЕЦ КАК ДоходРегл,
	ВЫБОР
		КОГДА ДенежныеСредства.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Расход)
				И ТИПЗНАЧЕНИЯ(ДенежныеСредства.Регистратор) = ТИП(Документ.РасчетКУрсовыхРазниц)
			ТОГДА -ДенежныеСредства.СуммаРегл
		ИНАЧЕ 0
	КОНЕЦ КАК РасходРегл
ИЗ
	РегистрНакопления.ДенежныеДокументы КАК ДенежныеСредства
ГДЕ
	ДенежныеСредства.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И (ДенежныеСредства.ДенежныйДокумент.Валюта &lt;&gt; &amp;ВалютаУпр ИЛИ ДенежныеСредства.ДенежныйДокумент.Валюта &lt;&gt; Организация.ВалютаРегламентированногоУчета)
{ГДЕ
	ДенежныеСредства.Регистратор,
	ДенежныеСредства.Организация.*,
	ДенежныеСредства.Подразделение.*,
	ДенежныеСредства.МОЛ.*,
	ДенежныеСредства.ДенежныйДокумент.*}
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ОстаткиИОбороты.Период КАК Период,
	ОстаткиИОбороты.ДатаОстатка КАК ДатаОстатка,
	ОстаткиИОбороты.Регистратор КАК Регистратор,
	ОстаткиИОбороты.Валюта КАК Валюта,
	ОстаткиИОбороты.ДенежныйДокумент КАК Аналитика,
	ОстаткиИОбороты.ДенежныйДокумент КАК ДенежныйДокумент,
	ВЫБОР КОГДА ОстаткиИОбороты.Валюта = ОстаткиИОбороты.Организация.ВалютаРегламентированногоУчета ТОГДА
		ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ДенежныеДокументы)
	ИНАЧЕ
		ЗНАЧЕНИЕ(ПланСчетов.Хозрасчетный.ДенежныеДокументыВал)
	КОНЕЦ КАК СчетУчета,
	ДАТАВРЕМЯ(3000,1,1) КАК ДатаНачалаПрименения67ФЗ,
	СУММА(ОстаткиИОбороты.СуммаРегл) КАК СуммаНУ,
	СУММА(ВЫБОР КОГДА ТИПЗНАЧЕНИЯ(ОстаткиИОбороты.Регистратор) = ТИП(Документ.РасчетКурсовыхРазниц) ТОГДА 0 ИНАЧЕ ОстаткиИОбороты.СуммаРегл КОНЕЦ) КАК СуммаНУДО,
