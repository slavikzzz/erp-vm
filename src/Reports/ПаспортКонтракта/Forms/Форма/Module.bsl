Функция БанковскиеСчета(ОсновнаяИнформация, БанковскиеСчета, ЗаказчикИсполнитель)
	СтрокаБанковскиеСчета = "";
	Префикс = ЗаказчикИсполнитель + "БанковскийСчет";
	
	Отбор = Новый Структура(ЗаказчикИсполнитель, ОсновнаяИнформация["Контракт" + ЗаказчикИсполнитель]);
	НайденныеСтроки = БанковскиеСчета.НайтиСтроки(Отбор);
	
	МассивСтрок = Новый Массив;
	
	Для Каждого НайденнаяСтрока Из НайденныеСтроки Цикл 
		РеквизитыСчета = Новый Массив;
		РеквизитыСчета.Добавить(Строка(НайденнаяСтрока[Префикс + "НаименованиеБанка"]) + " " + НСтр("ru = 'в';
																									|en = 'in'") + " " + Строка(НайденнаяСтрока[Префикс + "ГородБанка"]));
		РеквизитыСчета.Добавить(НСтр("ru = 'р/с';
									|en = 'account'") + " " + Строка(НайденнаяСтрока[Префикс + "НомерСчета"]));
		РеквизитыСчета.Добавить(НСтр("ru = 'кор/счет';
									|en = 'cor/account'") + " " + Строка(НайденнаяСтрока[Префикс + "КоррСчетБанка"]));
		РеквизитыСчета.Добавить(НСтр("ru = 'БИК';
									|en = 'BIC'")+ ": " + Строка(НайденнаяСтрока[Префикс + "БИКБанка"]));
		РеквизитыСчета.Добавить(НСтр("ru = 'SWIFT';
									|en = 'SWIFT'")+ ": " + Строка(НайденнаяСтрока[Префикс + "СВИФТБанка"]));
		
		МассивСтрок.Добавить(СтрСоединить(РеквизитыСчета, ", "));
	КонецЦикла;
	
	СтрокаБанковскиеСчета = СтрСоединить(МассивСтрок, Символы.ПС + Символы.ПС);
	
	Возврат СтрокаБанковскиеСчета;
КонецФункции

