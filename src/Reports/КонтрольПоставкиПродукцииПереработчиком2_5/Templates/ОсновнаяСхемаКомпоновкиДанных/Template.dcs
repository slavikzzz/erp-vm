		<dataSource>ИсточникДанных1</dataSource>
		<query>// ИСПОЛЬЗУЮТСЯ ТЕГИ!!!
 ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Заказы.ПериодГод,
	Заказы.ПериодДекада,
	Заказы.ПериодДень,
	Заказы.ПериодКвартал,
	Заказы.ПериодМесяц,
	Заказы.ПериодМинута,
	Заказы.ПериодНеделя,
	Заказы.ПериодПолугодие,
	Заказы.ПериодСекунда,
	Заказы.ПериодЧас,
	ВЫБОР
		КОГДА Заказы.Регистратор = НЕОПРЕДЕЛЕНО
			ТОГДА NULL
		ИНАЧЕ Заказы.Регистратор
	КОНЕЦ КАК Регистратор,
	РеестрДокументов.Организация КАК Организация,
	РеестрДокументов.Партнер КАК Переработчик,
	РеестрДокументов.Договор КАК Договор,
	Заказы.ЗаказПоставщику,
	РеестрДокументов.ДатаДокументаИБ КАК ДатаДокументаИБ,
	РеестрДокументов.НомерДокументаИБ КАК НомерДокументаИБ,
	Заказы.Номенклатура,
	Заказы.Номенклатура.ЕдиницаИзмерения КАК ЕдиницаИзмерения,
	Заказы.Характеристика,
	Заказы.КодСтроки,
	Заказы.Склад,

	Заказы.КОформлениюНачальныйОстаток,
	ВЫБОР
		КОГДА Заказы.КодСтроки = 0
			ТОГДА 0
		ИНАЧЕ Заказы.КОформлениюПриход
	КОНЕЦ КАК КОформлениюПриход,
	Заказы.КОформлениюРасход,
	Заказы.КОформлениюКонечныйОстаток,

	Заказы.КПоступлениюНачальныйОстаток,
	ВЫБОР
		КОГДА Заказы.КодСтроки = 0
			ТОГДА 0
		ИНАЧЕ Заказы.КПоступлениюПриход
	КОНЕЦ КАК КПоступлениюПриход,
	Заказы.КПоступлениюРасход,
	Заказы.КПоступлениюКонечныйОстаток
ПОМЕСТИТЬ ДанныеРегистра
ИЗ
	РегистрНакопления.ЗаказыПоставщикам.ОстаткиИОбороты(
			{(&amp;ДатаНачала)},
			{(&amp;ДатаОкончания)},
			Авто,,
			ТИПЗНАЧЕНИЯ(ЗаказПоставщику) = ТИП(Документ.ЗаказПереработчику2_5)
			{ЗаказПоставщику.* КАК ЗаказПоставщику,
			(КодСтроки = 0) КАК СверхЗаказа,
			Номенклатура.* КАК Номенклатура,
			Характеристика.* КАК Характеристика,
			КодСтроки КАК КодСтроки}
		) КАК Заказы
	
	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.РеестрДокументов КАК РеестрДокументов
	ПО РеестрДокументов.Ссылка = Заказы.ЗаказПоставщику
	И НЕ РеестрДокументов.ДополнительнаяЗапись
	
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику2_5.Услуги КАК УслугиЗаказа
	ПО УслугиЗаказа.Ссылка = Заказы.ЗаказПоставщику
	И УслугиЗаказа.КодСтроки = Заказы.КодСтроки

ГДЕ
	УслугиЗаказа.Ссылка ЕСТЬ NULL

{ГДЕ
	Заказы.Регистратор.* КАК Регистратор,
	РеестрДокументов.Организация.* КАК Организация,
	РеестрДокументов.Партнер.* КАК Переработчик,
	РеестрДокументов.Договор.* КАК Договор}
;

////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ РАЗЛИЧНЫЕ
	ДанныеРегистра.ЗаказПоставщику КАК ЗаказПоставщику,
	ДанныеРегистра.КодСтроки КАК КодСтроки
ПОМЕСТИТЬ ВтЗаказы
ИЗ
	ДанныеРегистра КАК ДанныеРегистра
СГРУППИРОВАТЬ ПО
	ДанныеРегистра.ЗаказПоставщику,
	ДанныеРегистра.КодСтроки

ИНДЕКСИРОВАТЬ ПО
	ЗаказПоставщику,
	КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Заказы.ЗаказПоставщику КАК ЗаказПоставщику,
	Заказы.КодСтроки КАК КодСтроки,
	МАКСИМУМ(
		ЕСТЬNULL(ТЧВыходныеИзделия.ДатаПоступления,
			ЕСТЬNULL(ТЧПобочныеИзделия.ДатаПоступления, ДАТАВРЕМЯ(1, 1, 1)))) КАК СрокПоставкиПлан
ПОМЕСТИТЬ ВтСрокиПоставки
ИЗ
	ВтЗаказы КАК Заказы
	
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику2_5.ВыходныеИзделия КАК ТЧВыходныеИзделия
	ПО Заказы.ЗаказПоставщику = ТЧВыходныеИзделия.Ссылка
	И Заказы.КодСтроки = ТЧВыходныеИзделия.КодСтроки
	
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику2_5.ПобочныеИзделия КАК ТЧПобочныеИзделия
	ПО Заказы.ЗаказПоставщику = ТЧПобочныеИзделия.Ссылка
	И Заказы.КодСтроки = ТЧПобочныеИзделия.КодСтроки

ГДЕ
	НЕ ТЧВыходныеИзделия.Ссылка ЕСТЬ NULL
	ИЛИ НЕ ТЧПобочныеИзделия.Ссылка ЕСТЬ NULL

СГРУППИРОВАТЬ ПО
	Заказы.ЗаказПоставщику,
	Заказы.КодСтроки

//++ НЕ УТКА
ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	Заказы.ЗаказПоставщику КАК ЗаказПоставщику,
	Заказы.КодСтроки КАК КодСтроки,
	МАКСИМУМ(
		ЕСТЬNULL(ТЧВыходныеИзделия.ДатаПроизводства,
			ЕСТЬNULL(ТЧПобочныеИзделия.ДатаПроизводства, ДАТАВРЕМЯ(1, 1, 1)))) КАК СрокПоставкиПлан
ИЗ
	ВтЗаказы КАК Заказы
	
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику2_5 КАК ДокЗаказ
	ПО Заказы.ЗаказПоставщику = ДокЗаказ.Ссылка
	И ДокЗаказ.ГруппировкаЗатрат = ЗНАЧЕНИЕ(Перечисление.ГруппировкиЗатратВЗаказеПереработчику.ПоЭтапамПроизводства)
	
	ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЭтапПроизводства2_2 КАК ДокЭтап
	ПО ДокЗаказ.Ссылка = ДокЭтап.ЗаказПереработчику
	
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЭтапПроизводства2_2.ВыходныеИзделия КАК ТЧВыходныеИзделия
	ПО ДокЭтап.Ссылка = ТЧВыходныеИзделия.Ссылка
	И Заказы.КодСтроки = ТЧВыходныеИзделия.КодСтроки
	
	ЛЕВОЕ СОЕДИНЕНИЕ Документ.ЭтапПроизводства2_2.ПобочныеИзделия КАК ТЧПобочныеИзделия
	ПО ДокЭтап.Ссылка = ТЧПобочныеИзделия.Ссылка
	И Заказы.КодСтроки = ТЧПобочныеИзделия.КодСтроки

ГДЕ
	НЕ ТЧВыходныеИзделия.Ссылка ЕСТЬ NULL
	ИЛИ НЕ ТЧПобочныеИзделия.Ссылка ЕСТЬ NULL

СГРУППИРОВАТЬ ПО
	Заказы.ЗаказПоставщику,
	Заказы.КодСтроки
//-- НЕ УТКА

ИНДЕКСИРОВАТЬ ПО
	ЗаказПоставщику,
	КодСтроки
;

////////////////////////////////////////////////////////////////////////////////
 ВЫБРАТЬ
	ДанныеРегистра.ПериодГод,
	ДанныеРегистра.ПериодДекада,
	ДанныеРегистра.ПериодДень,
	ДанныеРегистра.ПериодКвартал,
	ДанныеРегистра.ПериодМесяц,
	ДанныеРегистра.ПериодМинута,
	ДанныеРегистра.ПериодНеделя,
	ДанныеРегистра.ПериодПолугодие,
	ДанныеРегистра.ПериодСекунда,
	ДанныеРегистра.ПериодЧас,
	ДанныеРегистра.Регистратор,
	ДанныеРегистра.Организация,
	ДанныеРегистра.Переработчик,
	ДанныеРегистра.Договор,
	ДанныеРегистра.ЗаказПоставщику,
	ДанныеРегистра.ДатаДокументаИБ,
	ДанныеРегистра.НомерДокументаИБ,
	ДанныеРегистра.Номенклатура,
	ДанныеРегистра.ЕдиницаИзмерения,
	ДанныеРегистра.Характеристика,
	ДанныеРегистра.КодСтроки,
	ДанныеРегистра.Склад,
	СрокиПоставки.СрокПоставкиПлан КАК СрокПоставкиПлан,
	
	ДанныеРегистра.КОформлениюНачальныйОстаток,
	ДанныеРегистра.КОформлениюПриход,
	ДанныеРегистра.КОформлениюРасход,
	ДанныеРегистра.КОформлениюКонечныйОстаток,
	
	ДанныеРегистра.КПоступлениюНачальныйОстаток,
	ДанныеРегистра.КПоступлениюПриход,
	ДанныеРегистра.КПоступлениюРасход,
	ДанныеРегистра.КПоступлениюКонечныйОстаток
ИЗ
	ДанныеРегистра КАК ДанныеРегистра
	
	ЛЕВОЕ СОЕДИНЕНИЕ ВтСрокиПоставки КАК СрокиПоставки
	ПО ДанныеРегистра.ЗаказПоставщику = СрокиПоставки.ЗаказПоставщику
	И ДанныеРегистра.КодСтроки = СрокиПоставки.КодСтроки
	</query>
	</dataSet>
	<calculatedField>
		<dataPath>ПоступилоНеВСрок</dataPath>
		<expression>ВЫБОР
	КОГДА НЕ КодСтроки = 0
		И ПериодДень &gt; СрокПоставкиПлан
		ТОГДА КПоступлениюРасход
	ИНАЧЕ 0
КОНЕЦ</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Поступило не в срок</v8:content>
			</v8:item>
			<v8:item>
				<v8:lang>en</v8:lang>
				<v8:content>Delivered out-of-time</v8:content>
			</v8:item>
		</title>
		<appearance>
			<dcscor:item xsi:type="dcsset:SettingsParameterValue">
