		Объект.РегистрацияВИФНС = Результат;
		УстановитьДанныеПоРегистрацииВИФНС();
		Модифицированность = Истина;
	КонецЕсли;
КонецПроцедуры

&НаКлиенте
Функция ЭтоОбластьОКСМ(Область)
	Если ЭтотОбъект["ТекущееИДНаименования"] = "ЛистЗ" Тогда 
		Если Область.Имя = "З0801" Или Область.Имя = "З1001" Тогда 
			Возврат Истина;
		КонецЕсли;
	ИначеЕсли ЭтотОбъект["ТекущееИДНаименования"] = "ЛистЕ" Тогда 
		Если Область.Имя = "Е03" Тогда 
			Возврат Истина;
		КонецЕсли;
	ИначеЕсли ЭтотОбъект["ТекущееИДНаименования"] = "ЛистА" Тогда 
		Если Область.Имя = "А03" Тогда 
			Возврат Истина;
		КонецЕсли;
	ИначеЕсли ЭтотОбъект["ТекущееИДНаименования"] = "Лист002" Тогда 
		Если Область.Имя = "Стр205" Тогда 
			Возврат Истина;
		КонецЕсли;
	ИначеЕсли ЭтотОбъект["ТекущееИДНаименования"] = "ЛистВ" Тогда 
		Если Область.Имя = "В0801" Или Область.Имя = "В1001" Тогда 
			Возврат Истина;
		КонецЕсли;
	ИначеЕсли ЭтотОбъект["ТекущееИДНаименования"] = "ЛистГ" Тогда 
		Если Область.Имя = "Г0205" Тогда 
			Возврат Истина;
		КонецЕсли;
	КонецЕсли;
	
	Возврат Ложь;
КонецФункции

&НаКлиенте
Функция ЭтоОбластьДоля(Область)
	Если ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистА") И (Область.Имя = "А08"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистБ") И (Область.Имя = "Б07"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистВ") И (Область.Имя = "В12"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистГ") И (Область.Имя = "Г0209"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистЕ") И (Область.Имя = "Е08"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистЖ") И (Область.Имя = "Ж07"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистЗ") И (Область.Имя = "З12"))
		Или ((ЭтотОбъект["ТекущееИДНаименования"] = "ЛистД") И (Область.Имя = "Д0106" Или Область.Имя = "Д0202") 
													Или СтрНачинаетсяС(Область.Имя, "Д020302" + ЭтотОбъект["РазделительНомераСтроки"])
													Или СтрНачинаетсяС(Область.Имя, "Д020303" + ЭтотОбъект["РазделительНомераСтроки"]))Тогда 
		Возврат Истина;
	Иначе 
		Возврат Ложь;
	КонецЕсли;
КонецФункции

