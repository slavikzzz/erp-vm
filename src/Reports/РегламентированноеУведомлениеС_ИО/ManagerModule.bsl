Процедура Проверить_Титульная_2022_1(Данные, ТаблицаОшибок)
	Титульная = Данные.ДанныеУведомления.Титульная;
	Если Не ЗначениеЗаполнено(Титульная.НалПер)
		Или СтрДлина(Титульная.НалПер) <> 4 Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указан / неправильно указан налоговый период", "Титульная", "НалПер"));
	КонецЕсли;
	УведомлениеОСпецрежимахНалогообложенияСлужебный.ПроверкаИННКПП(Истина, Титульная, ТаблицаОшибок);
	УведомлениеОСпецрежимахНалогообложенияСлужебный.ПроверкаКодаНО(Титульная.КодНО, ТаблицаОшибок, "Титульная");
	Если Не ЗначениеЗаполнено(Титульная.ДАТА_ПОДПИСИ) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указана дата подписи", "Титульная", "ДАТА_ПОДПИСИ"));
	КонецЕсли;
	Если Не ЗначениеЗаполнено(Титульная.Наименование) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указано наименование организации", "Титульная", "Наименование"));
	КонецЕсли;
	УведомлениеОСпецрежимахНалогообложенияСлужебный.ПроверкаПодписантаНалоговойОтчетности(Данные, ТаблицаОшибок, "Титульная", Истина);
	УведомлениеОСпецрежимахНалогообложенияСлужебный.ПроверкаИНН(Титульная, ТаблицаОшибок, "Титульная", "ИННФЛ", Ложь, Ложь);
	Если Не ЗначениеЗаполнено(Титульная.ПРИЗНАК_НП_ПОДВАЛ) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указан признак подписанта", "Титульная", "ПРИЗНАК_НП_ПОДВАЛ"));
	КонецЕсли;
	Если Не ЗначениеЗаполнено(Титульная.НаимДок)
		И Титульная.ПРИЗНАК_НП_ПОДВАЛ = "2" Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указан документ подписанта", "Титульная", "НаимДок"));
	КонецЕсли;
КонецПроцедуры

Процедура Проверить_СведИСБезЮЛ_2022_1(Данные, ТаблицаОшибок)
	Страница = Данные.ДанныеУведомления.СведИСБезЮЛ;
	Если Не УведомлениеОСпецрежимахНалогообложения.СтраницаЗаполнена(Страница) Тогда 
		Возврат;
	КонецЕсли;
	
	Если Не ЗначениеЗаполнено(Страница.ОргФорм) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указан признак орг. формы", "СведИСБезЮЛ", "ОргФорм"));
	КонецЕсли;
	Если Не ЗначениеЗаполнено(Страница.НаимОрг) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указано наименование", "СведИСБезЮЛ", "НаимОрг"));
	КонецЕсли;
	Если Не ЗначениеЗаполнено(Страница.НаимОргЛат) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указано наименование в латинской транскрипции", "СведИСБезЮЛ", "НаимОргЛат"));
	КонецЕсли;
	Если Не ЗначениеЗаполнено(Страница.ДатаУчр) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указана дата учреждения ", "СведИСБезЮЛ", "ДатаУчр"));
	КонецЕсли;
	Если Не ЗначениеЗаполнено(Страница.СтранаУчрИС) Тогда 
		ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
			"Не указана страна регистрации", "СведИСБезЮЛ", "СтранаУчрИС"));
	КонецЕсли;
КонецПроцедуры

Процедура Проверить_СвИОУчИО_2022_1(Данные, ТаблицаОшибок)
	Для Каждого Стр Из Данные.ДанныеМногостраничныхРазделов.СвИОУчИО Цикл 
		Страница = Стр.Значение;
		Если Не УведомлениеОСпецрежимахНалогообложения.СтраницаЗаполнена(Страница) Тогда 
			Продолжить;
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Страница.НомерИО) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указан номер иностранной организации", "СвИОУчИО", "НомерИО", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.ПублКомп) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указан признак публичной компании", "СвИОУчИО", "ПублКомп", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.Участие) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указан признак участия", "СвИОУчИО", "Участие", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.НаимОрг) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указано наименование", "СвИОУчИО", "НаимОрг", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.НаимОргЛат) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указано наименование в латинской транскрипции", "СвИОУчИО", "НаимОргЛат", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.СтрРег) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указана страна регистрации", "СвИОУчИО", "СтрРег", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.РегНомер) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указан регистрационный номер", "СвИОУчИО", "РегНомер", Страница.УИД));
		КонецЕсли;
		Если Не ЗначениеЗаполнено(Страница.АдрСтрРег) Тогда 
			ТаблицаОшибок.Добавить(Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПустуюСтруктуруНавигацииПоОшибкам(
				"Не указан адрес", "СвИОУчИО", "АдрСтрРег", Страница.УИД));
		КонецЕсли;
		Если (Страница.Участие = "2" Или Страница.Участие = "3") И Страница.ПублКомп = "1" Тогда 
			ЕстьСоответствующийЛистД = Ложь;
			Для Каждого Стр2 Из Данные.ДанныеМногостраничныхРазделов.РаскрУчИОКосв Цикл 
				Если Стр2.Значение.НомерИО = Страница.НомерИО Тогда 
					ЕстьСоответствующийЛистД = Истина;
					Прервать;
				КонецЕсли;
