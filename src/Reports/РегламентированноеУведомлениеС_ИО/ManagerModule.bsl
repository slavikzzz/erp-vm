Процедура СформироватьПараметрыДляЛистовДК_2022(Параметры, ИдСтраницы)
	ОТЧ = Новый ОписаниеТипов("Число");
	Если ИдСтраницы = "РаскрУчИОКосв" Тогда 
		МнгСтр = "МнгСтр1";
		Уровень0 = "ЛистДДляВыгрузки";
		Уровень1 = "ЛистД0ДляВыгрузки";
		Уровень2 = "ЛистД00ДляВыгрузки";
	Иначе
		МнгСтр = "МнгСтр2";
		Уровень0 = "ЛистКДляВыгрузки";
		Уровень1 = "ЛистК0ДляВыгрузки";
		Уровень2 = "ЛистК00ДляВыгрузки";
	КонецЕсли;
	Параметры.ДанныеМногостраничныхРазделов.Вставить(Уровень0, Новый СписокЗначений);
	Параметры.ДанныеМногостраничныхРазделов.Вставить(Уровень1, Новый СписокЗначений);
	Параметры.ДанныеМногостраничныхРазделов.Вставить(Уровень2, Новый СписокЗначений);
	
	ВсеУчастники = Новый Соответствие;
	УИДыЛист = Новый Соответствие;
	Для Каждого Стр Из Параметры.ДанныеМногостраничныхРазделов[ИдСтраницы] Цикл
		ПарамСтр = Стр.Значение;
		СтрКлюч = "";
		Если ЗначениеЗаполнено(ПарамСтр.НомерИО) Тогда
			СтрКлюч = "ИО-" + Формат(ПарамСтр.НомерИО, "ЧЦ=5; ЧН=; ЧВН=; ЧГ=");
		ИначеЕсли ЗначениеЗаполнено(ПарамСтр.НомерРО) Тогда
			СтрКлюч = "РО-" + Формат(ПарамСтр.НомерРО, "ЧЦ=5; ЧН=; ЧВН=; ЧГ=");
		ИначеЕсли ЗначениеЗаполнено(ПарамСтр.НомерФЛ) Тогда
			СтрКлюч = "ФЛ-" + Формат(ПарамСтр.НомерФЛ, "ЧЦ=5; ЧН=; ЧВН=; ЧГ=");
		КонецЕсли;
		Если Не ЗначениеЗаполнено(СтрКлюч) Тогда 
			Продолжить;
		КонецЕсли;
		
		Если ВсеУчастники[СтрКлюч] = Неопределено Тогда 
			ВсеУчастники.Вставить(СтрКлюч, Новый СписокЗначений);
			УИДыЛист.Вставить(СтрКлюч, ПарамСтр.УИД);
		КонецЕсли;
		ВсеУчастники[СтрКлюч].Добавить(ПарамСтр);
	КонецЦикла;
	
	Для Каждого КЗ Из ВсеУчастники Цикл
		Стр = ПолучитьСтруктураЛистДКДляВыгрузки_2022();
		ЗаполнитьЗначенияСвойств(Стр, КЗ.Значение[0].Значение);
		ЛКлюч = Лев(КЗ.Ключ, 3);
		Если ЛКлюч = "ИО-" Тогда
			Стр.НомерИОСтр = КЗ.Ключ;
			Стр.Фамилия = "";
			Стр.Имя = "";
			Стр.Отчество = "";
		ИначеЕсли ЛКлюч = "РО-" Тогда 
			Стр.НомерРОСтр = КЗ.Ключ;
			Стр.Фамилия = "";
			Стр.Имя = "";
			Стр.Отчество = "";
		ИначеЕсли ЛКлюч = "ФЛ-" Тогда 
			Стр.НомерФЛСтр = КЗ.Ключ;
			Стр.НаимОрг = "";
		КонецЕсли;
		Параметры.ДанныеМногостраничныхРазделов[Уровень0].Добавить(Стр);
		УИДРодителя = КЗ.Значение[0].Значение.УИД;
		
		Для Каждого Элт Из КЗ.Значение Цикл
			Стр = ПолучитьСтруктураЛистДК0ДляВыгрузки_2022();
			ЗаполнитьЗначенияСвойств(Стр, Элт.Значение);
			Стр.УИДРодителя = УИДРодителя;
			Стр.УИД = Новый УникальныйИдентификатор;
			Стр.НомерПосл = Формат(Стр.НомерПосл, "ЧЦ=5; ЧН=; ЧВН=; ЧГ=");
			Параметры.ДанныеМногостраничныхРазделов[Уровень1].Добавить(Стр);
			
			МнгСтрСтроки = Параметры.ДанныеДопСтрокБД[МнгСтр].НайтиСтроки(Новый Структура("УИД", Элт.Значение.УИД));
			Для Каждого СтрокаМнг Из МнгСтрСтроки Цикл
				СтрПодч = ПолучитьСтруктураЛистДК00ДляВыгрузки_2022();
				ЗаполнитьЗначенияСвойств(СтрПодч, СтрокаМнг);
				СтрПодч.УИДРодителя = Стр.УИД;
				Параметры.ДанныеМногостраничныхРазделов[Уровень2].Добавить(СтрПодч);
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
КонецПроцедуры

Процедура ДополнитьПараметры_2022(Параметры)
	Параметры.Вставить("ЕстьАГ", Не УведомлениеОСпецрежимахНалогообложения.СтраницаЗаполнена(Параметры.ДанныеУведомления.СведИСБезЮЛ));
	Если Не ЗначениеЗаполнено(Параметры.ДанныеУведомления.Титульная.НомерКорректировки) Тогда 
		Параметры.ДанныеУведомления.Титульная.НомерКорректировки = "0";
	КонецЕсли;
	
	Для Каждого КЗ Из Параметры.ДанныеМногостраничныхРазделов Цикл 
		Для Каждого Стр Из КЗ.Значение Цикл 
			Для Каждого Ном Из СтрРазделить("НомерИО,НомерРО,НомерИС,НомерФЛ", ",") Цикл 
				Если Стр.Значение.Свойство(Ном) И ЗначениеЗаполнено(Стр.Значение[Ном]) Тогда 
					Стр.Значение.Вставить(Ном + "Стр", Прав(Ном, 2) + "-" + Формат(Стр.Значение[Ном], "ЧЦ=5; ЧВН=; ЧГ="));
				КонецЕсли;
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	СформироватьПараметрыДляЛистовДК_2022(Параметры, "РаскрУчИОКосв");
	СформироватьПараметрыДляЛистовДК_2022(Параметры, "РаскрУчИСБезЮЛКосв");
КонецПроцедуры

Процедура ЗаполнитьДанными_Форма2022_1(Объект, Параметры, ДеревоВыгрузки)
	Документы.УведомлениеОСпецрежимахНалогообложения.ОбработатьУсловныеЭлементы(Параметры, ДеревоВыгрузки);
	Документы.УведомлениеОСпецрежимахНалогообложения.ЗаполнитьПараметрыСРазделами(Параметры, ДеревоВыгрузки);
	ДанныеУведомления = Объект.ДанныеУведомления.Получить();
	ДополнитьПараметры_2022(ДанныеУведомления);
	УведомлениеОСпецрежимахНалогообложения.ЗаполнитьДаннымиУзелНов(ДанныеУведомления, ДеревоВыгрузки);
	
	Узел_Документ = Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПодчиненныйЭлемент(ДеревоВыгрузки, "Документ");
	Узел = Документы.УведомлениеОСпецрежимахНалогообложения.ПолучитьПодчиненныйЭлемент(Узел_Документ, "СообУчИО");
	Если ДанныеУведомления.ЕстьАГ Тогда
