	Контейнер.Титульная.Вставить("ДатаПрекрПСН");
	Контейнер.Титульная.Вставить("НомПатент");
	Контейнер.Титульная.Вставить("ДатаВыд");
	Контейнер.Титульная.Вставить("КодНО");
	
	Возврат Контейнер;
