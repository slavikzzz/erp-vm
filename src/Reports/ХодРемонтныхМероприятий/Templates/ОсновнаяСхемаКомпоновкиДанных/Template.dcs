				<dcscor:parameter>ГоризонтальноеПоложение</dcscor:parameter>
				<dcscor:value xsi:type="v8ui:HorizontalAlign">Right</dcscor:value>
			</dcscor:item>
		</appearance>
		<valueType>
			<v8:Type>xs:decimal</v8:Type>
			<v8:NumberQualifiers>
				<v8:Digits>0</v8:Digits>
				<v8:FractionDigits>0</v8:FractionDigits>
				<v8:AllowedSign>Any</v8:AllowedSign>
			</v8:NumberQualifiers>
		</valueType>
	</calculatedField>
	<totalField>
		<dataPath>ДатаЗавершения</dataPath>
		<expression>Максимум(ДатаЗавершения)</expression>
	</totalField>
	<totalField>
		<dataPath>ДатаНачала</dataPath>
		<expression>Минимум(ДатаНачала)</expression>
	</totalField>
	<totalField>
		<dataPath>Длительность</dataPath>
