&НаКлиенте
Процедура ПолеТабличногоДокументаФормаОтчетаВыбор(Элемент, Область, СтандартнаяОбработка)
	
	// ПроцессыОбработкиДокументов
	//
	Если Элементы.ПолеТабличногоДокументаФормаОтчета.Защита Тогда
		
		Возврат;
		
	КонецЕсли;
	//
	// ПроцессыОбработкиДокументов
	
	РегламентированнаяОтчетностьКлиент.ОбработкаВыбораПоляОбособленныхПодразделений(
		ЭтотОбъект, Область, СтандартнаяОбработка);
	
	Если Область.Имя = "ОргАдрес" Тогда
		РегламентированнаяОтчетностьКлиент.ОбработкаАдресаВСтатистическомОтчетеJSON(
			ЭтотОбъект, Область, СтандартнаяОбработка);
	ИначеЕсли СтрЧислоВхождений(Область.Имя, "ДобавитьСтроку") > 0 Тогда
		СтандартнаяОбработка = Ложь;
		ДобавитьСтроку("");
	ИначеЕсли СтрЧислоВхождений(Область.Имя, "УдалитьСтроку") > 0 Тогда
		УдалитьСтроку("");
	ИначеЕсли РегламентированнаяОтчетностьКлиент.ВыбраннаяОбластьРегламентированногоОтчетаНеВерна(ЭтаФорма, Область, СтандартнаяОбработка) Тогда
		Возврат;
	Иначе
		
		Если Сред(Область.Имя, 13 , 1) = "5" И Сред(Область.Имя, 7 , 1) <> "2" Тогда
			ИмяСправочника = "s_oktmo_f";
			ОбластьКод = Область;
			
			ПараметрВводаПоля = РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтотОбъект, ИмяСправочника);
			ВвестиПоказатель(Элемент, ОбластьКод, , ИмяСправочника, , ПараметрВводаПоля.ТекстПриВыборе, , ,СтандартнаяОбработка);
		ИначеЕсли Лев(Область.Имя, 13) = "П001001000106"
			  Или Лев(Область.Имя, 13) = "П002001000108"
			  Или Лев(Область.Имя, 13) = "П003001000106"
			  Или Лев(Область.Имя, 13) = "П004001000106" Тогда
			ИмяСправочника = "СправочникКодХарактераСтроительства";
			ПараметрВводаПоля = РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтотОбъект, ИмяСправочника);
			КопироватьДанныеФормы(ПараметрВводаПоля.ТаблицаЗначений, ТЗВыбора);
			ВвестиПоказатель(Элемент, Область, , ИмяСправочника, , ПараметрВводаПоля.ТекстПриВыборе, ТЗВыбора, , СтандартнаяОбработка);
		ИначеЕсли Лев(Область.Имя, 13) = "П002002000108"
			  Или Лев(Область.Имя, 13) = "П004001000110" Тогда
			ИмяСправочника = "СправочникПоМатериаламНаружныхСтен";
			ПараметрВводаПоля = РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтотОбъект, ИмяСправочника);
			КопироватьДанныеФормы(ПараметрВводаПоля.ТаблицаЗначений, ТЗВыбора);
			ВвестиПоказатель(Элемент, Область, , ИмяСправочника, , ПараметрВводаПоля.ТекстПриВыборе, ТЗВыбора, , СтандартнаяОбработка);
		ИначеЕсли Лев(Область.Имя, 13) = "П002001000109" Тогда
			ИмяСправочника = "СправочникКодТипаОбъекта";
			ПараметрВводаПоля = РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтотОбъект, ИмяСправочника);
			КопироватьДанныеФормы(ПараметрВводаПоля.ТаблицаЗначений, ТЗВыбора);
			ВвестиПоказатель(Элемент, Область, , ИмяСправочника, , ПараметрВводаПоля.ТекстПриВыборе, ТЗВыбора, , СтандартнаяОбработка);
		ИначеЕсли Лев(Область.Имя, 13) = "П002001000107" Тогда
			ИмяСправочника = "s_ei";
			
			ОбластьКод = ПолеТабличногоДокументаФормаОтчета.Области.Найти(СтрЗаменить(Область.Имя, Сред(Область.Имя, 1, 11) + "07_", Сред(Область.Имя, 1, 11) + "10_"));
			ОбластьНаим  = ПолеТабличногоДокументаФормаОтчета.Области.Найти(СтрЗаменить(Область.Имя, Сред(Область.Имя, 1, 11) + "10_", Сред(Область.Имя, 1, 11) + "07_"));
			
			ПараметрВводаПоля = РегламентированнаяОтчетностьКлиентСервер.НайтиСвойстваПоказателя(ЭтотОбъект, ИмяСправочника);
			ВвестиПоказатель(Элемент, ОбластьКод, ОбластьНаим, ИмяСправочника, , ПараметрВводаПоля.ТекстПриВыборе, , ,СтандартнаяОбработка);
		ИначеЕсли СтрНайти(Область.Имя, "П002001000110_") = 1 Тогда
			СтандартнаяОбработка = Ложь;
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

