			<totalField>
				<dataPath>ПроцентОтклонения</dataPath>
				<expression>ВЫБОР 
    КОГДА ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0) = 0 И (ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(Оборот),0)) = 0
    	ТОГДА 0
    КОГДА ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0) = 0 
        ТОГДА 1
    КОГДА (ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(Оборот),0) - ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0)) / Сумма(КонечныйОстатокПоПравилам) &lt; 0
        ТОГДА -(ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(Оборот),0) - ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0)) / Сумма(КонечныйОстатокПоПравилам)
    ИНАЧЕ (ЕстьNULL(Сумма(НачальныйОстатокПоПравилам),0) + ЕстьNULL(Сумма(Оборот),0) - ЕстьNULL(Сумма(КонечныйОстатокПоПравилам),0)) / Сумма(КонечныйОстатокПоПравилам)
КОНЕЦ * 100</expression>
			</totalField>
