	СУММА(ВЫБОР
			КОГДА &amp;ДанныеПоСебестоимости = 1
				ТОГДА ЕСТЬNULL(Стоимости.ПостатейныеПостоянныеСНДС + Стоимости.ПостатейныеПеременныеСНДС, 0)
			КОГДА &amp;ДанныеПоСебестоимости = 2
				ТОГДА ЕСТЬNULL(Стоимости.ПостатейныеПостоянныеБезНДС + Стоимости.ПостатейныеПеременныеБезНДС, 0)
			ИНАЧЕ
				ЕСТЬNULL(Стоимости.ПостатейныеПостоянныеРегл + Стоимости.ПостатейныеПеременныеРегл, 0)
		КОНЕЦ) КАК СтоимостьПостатейные,
	СУММА(ВЫБОР
			КОГДА &amp;ДанныеПоСебестоимости = 1
				ТОГДА ЕСТЬNULL(Стоимости.ДопРасходы, 0)
			КОГДА &amp;ДанныеПоСебестоимости = 2
				ТОГДА ЕСТЬNULL(Стоимости.ДопРасходыБезНДС, 0)
			ИНАЧЕ
				ЕСТЬNULL(Стоимости.ДопРасходыРегл, 0)
		КОНЕЦ) КАК ДопРасходы,
	СУММА(ВЫБОР
			КОГДА &amp;ДанныеПоСебестоимости = 3
				ТОГДА ЕСТЬNULL(Стоимости.СтоимостьЗабалансоваяРегл, 0)
			ИНАЧЕ
				ЕСТЬNULL(Стоимости.СтоимостьЗабалансовая, 0)
		КОНЕЦ) КАК Забалансовая
ПОМЕСТИТЬ РаботыДавальца
ИЗ
	ПартииПродукции КАК ПартииПродукции
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ РегистрНакопления.СебестоимостьТоваров КАК ДД
		ПО ПартииПродукции.Партия = ДД.Регистратор
		ЛЕВОЕ СОЕДИНЕНИЕ Константы КАК Константы
		ПО (ИСТИНА)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.СебестоимостьТоваров КАК Стоимости
		ПО (ДД.Регистратор = Стоимости.Регистратор)
			И (ДД.ВидДвижения = Стоимости.ВидДвижения)
			И (ДД.АналитикаУчетаНоменклатуры = Стоимости.АналитикаУчетаНоменклатуры)
			И (ДД.РазделУчета = Стоимости.РазделУчета)
			И (ДД.ВидЗапасов = Стоимости.ВидЗапасов)
			И (ДД.Организация = Стоимости.Организация)
			И (ДД.Партия = Стоимости.Партия)
			И (ДД.АналитикаУчетаПартий = Стоимости.АналитикаУчетаПартий)
			И (ДД.АналитикаФинансовогоУчета = Стоимости.АналитикаФинансовогоУчета)
			И (ДД.ВидДеятельностиНДС = Стоимости.ВидДеятельностиНДС)
ГДЕ
	ДД.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
	И ДД.Активность
	И ДД.РазделУчета = ЗНАЧЕНИЕ(Перечисление.РазделыУчетаСебестоимостиТоваров.ПроизводственныеЗатраты)
	И ДД.ХозяйственнаяОперация В (ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукции), ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукцииНаСклад), ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукцииВПодразделение))
	И &amp;ПартионныйУчетВерсии22
	И ДД.Период &gt;= &amp;ДатаПереходаНаПартионныйУчетВерсии22
	И НЕ ДД.КорАналитикаУчетаНоменклатуры = ЗНАЧЕНИЕ(Справочник.КлючиАналитикиУчетаНоменклатуры.ПустаяСсылка)

СГРУППИРОВАТЬ ПО
	ДД.Организация,
	ВЫБОР
		КОГДА ДД.КорАналитикаУчетаНоменклатуры.ТипМестаХранения = ЗНАЧЕНИЕ(Перечисление.ТипыМестХранения.Подразделение)
			ТОГДА ДД.КорАналитикаУчетаНоменклатуры.Подразделение
