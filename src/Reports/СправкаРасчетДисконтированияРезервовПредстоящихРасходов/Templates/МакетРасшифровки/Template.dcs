			<dataPath>ВидРезервов</dataPath>
			<field>ВидРезервов</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>НаправлениеДеятельности</dataPath>
			<field>НаправлениеДеятельности</field>
