	НоваяФорма.ДатаНачалоДействия = '20190701';
	НоваяФорма.ДатаКонецДействия  = '20220630';
	
	НоваяФорма = ТаблицаФормОтчета.Добавить();
	НоваяФорма.ФормаОтчета        = "ФормаОтчета2023Кв1";
	НоваяФорма.ОписаниеОтчета     = "Приказ Департамента труда и социальной защиты населения города Москвы от 30.09.2022 № 992.";
	НоваяФорма.РедакцияФормы	  = "от 30.09.2022 № 992.";
	НоваяФорма.ДатаНачалоДействия = '20220701';
	НоваяФорма.ДатаКонецДействия  = '20231231';
	
	НоваяФорма = ТаблицаФормОтчета.Добавить();
	НоваяФорма.ФормаОтчета        = "ФормаОтчета2024Кв1";
