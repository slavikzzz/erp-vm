// Возвращает сокращения частей адреса.
// Предзначена в первую очередь для сокращения поля "иное" или "другое" в случае заполнения.
// Там возможно все что угодно, поэтому представлены максимум вариантов.
// Также используется для сокращения стандартных значений в ТипДома, ТипКорпуса и ТипКвартиры.
//
// Возвращаемое значение:
//      Соответствие - Список сокращений.
//
Функция СокращенияОбъектовАдресацииАдресаРФ() Экспорт
	
	Результат = Новый Соответствие;
	
	// При заполнении поля ИНОЕ, все помещается в "квартиру".
	Результат.Вставить(НСтр("ru = 'иное';
							|en = 'иное'"), НСтр("ru = ''")); 
	Результат.Вставить(НСтр("ru = 'другое';
							|en = 'другое'"), НСтр("ru = ''")); 
	Результат.Вставить(НСтр("ru = '№';
							|en = '№'"), НСтр("ru = ''")); 
	
	Результат.Вставить(НСтр("ru = 'дом';
							|en = 'дом'"), НСтр("ru = 'д.';
												|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'домом';
							|en = 'домом'"), НСтр("ru = 'д.';
													|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'дому';
							|en = 'дому'"), НСтр("ru = 'д.';
												|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'доме';
							|en = 'доме'"), НСтр("ru = 'д.';
												|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'домов';
							|en = 'домов'"), НСтр("ru = 'д.';
													|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'дома';
							|en = 'дома'"), НСтр("ru = 'д.';
												|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'домам';
							|en = 'домам'"), НСтр("ru = 'д.';
													|en = 'д.'"));
	Результат.Вставить(НСтр("ru = 'домами';
							|en = 'домами'"), НСтр("ru = 'д.';
													|en = 'д.'"));
	
	Результат.Вставить(НСтр("ru = 'владение';
							|en = 'владение'"), НСтр("ru = 'вл.';
													|en = 'вл.'"));
	Результат.Вставить(НСтр("ru = 'владением';
							|en = 'владением'"), НСтр("ru = 'вл.';
														|en = 'вл.'"));
	Результат.Вставить(НСтр("ru = 'владения';
							|en = 'владения'"), НСтр("ru = 'вл.';
													|en = 'вл.'"));
	Результат.Вставить(НСтр("ru = 'владениям';
							|en = 'владениям'"), НСтр("ru = 'вл.';
														|en = 'вл.'"));
	Результат.Вставить(НСтр("ru = 'владениями';
