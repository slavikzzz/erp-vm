// Функция возвращает текст запроса для отражения документа в регламентированном учете.
//
// Возвращаемое значение:
//	Строка - Текст запроса
//
Функция ТекстОтраженияВРеглУчете() Экспорт
	
	ТекстыОтражения = Новый Массив;
	
	//++ Локализация
	
	ТекстыОтражения.Добавить(ТекстПеремещениеСобственныхТоваров()); // Перемещение собственных товаров (Дт 41.01, 20 :: Кт 41.01, 20)
	ТекстыОтражения.Добавить(ТекстПеремещениеПродукцииДавальцевБаланс()); // Перемещение продукции давальцев (Дт 20 :: Кт 20)
	ТекстыОтражения.Добавить(ТекстПередачаТоваровЗаБалансомСОбособленногоПодразделения()); // Передача товаров забалансом с обособленного подразделения (Дт :: Кт 002, 003, 004)
	ТекстыОтражения.Добавить(ТекстПередачаТоваровЗаБалансомНаОбособленноеПодразделение()); // Передача товаров забалансом на обособленное подразделение (Дт 002, 003, 004 :: Кт)
	ТекстыОтражения.Добавить(ТекстПередачаСобственныхТоваровСОбособленногоПодразделения()); // Передача собственных товаров с обособленного подразделения (Дт 79.01 :: Кт 41.01, 20)
	ТекстыОтражения.Добавить(ТекстПередачаСобственныхТоваровНаОбособленноеПодразделение()); // Передача собственных товаров на обособленное подразделение (Дт 41.01, 20 :: Кт 79.01)
	ТекстыОтражения.Добавить(ТекстПеремещениеТоваровЗаБалансом()); // Перемещение товаров за балансом (Дт 002, 003, 004 :: Кт 002, 003, 004)
	ТекстыОтражения.Добавить(ТекстПередачаПродукцииДавальцевСОбособленногоПодразделенияБаланс()); // Передача продукции давальцев с обособленного подразделения (Дт 79.01 :: Кт 20)
	ТекстыОтражения.Добавить(ТекстПередачаПродукцииДавальцевНаОбособленноеПодразделениеБаланс()); // Передача продукции давальцев на обособленное подразделение (Дт 20 :: Кт 79.01)
	ТекстыОтражения.Добавить(ТекстВключениеИсключениеНДСВСтоимость()); // Включение/исключение НДС в стоимость товара на складе - получателе (Дт 41.01 :: Кт 19.03) @НДСПредъявленный
	ТекстыОтражения.Добавить(ТекстПередачаРезервовСОбособленногоПодразделения()); // Передача резервов с обособленного подразделения (Дт 79.01 :: Кт 14)
	ТекстыОтражения.Добавить(ТекстПередачаРезервовНаОбособленноеПодразделение()); // Передача резервов на обособленное подразделение (Дт 14 :: Кт 79.01)
	
	//-- Локализация
	
	Возврат СтрСоединить(ТекстыОтражения, ОбщегоНазначенияУТ.РазделительЗапросовВОбъединении());
	
КонецФункции

