			Сообщение.Текст = НСтр("ru = 'В приложении не задана дата договора (контракта).';
									|en = 'Contract date is not specified in the application.'");
			Сообщение.Сообщить();
			СтатусПроверки = Ложь;
		КонецЕсли;
	
	КонецЦикла;
	
