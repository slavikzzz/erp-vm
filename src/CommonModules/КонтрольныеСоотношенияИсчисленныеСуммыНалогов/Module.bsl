	СтруктураРезультата.Вставить("ЧислоОшибок", КонтекстПроверки.ДанныеКС.ЧислоОшибок);
	СтруктураРезультата.Вставить("ТекстЗаголовка", Строка(ВидУведомления));
	СтруктураРезультата.Вставить("МакетСКП", КонтекстПроверки.ДанныеКС.МакетСКП);
	СтруктураРезультата.Вставить("ВариантСКП", КонтекстПроверки.ДанныеКС.ВариантСКП);
	
