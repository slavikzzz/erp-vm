Процедура ПроверкаКС_01_код(КонтекстПроверки)
	
	ИменаПоказателей = Новый Структура;
	ИменаПоказателей.Вставить("Р1С01", "КБК");
	ИменаПоказателей.Вставить("Р1С02", "КППДекл");
	ИменаПоказателей.Вставить("Р0С03", "ИНН");
	
	Показатели = СформироватьПоказатели(КонтекстПроверки, ИменаПоказателей);
	
	Показатели.Р1С02.Расшифровывать = ЗначениеЗаполнено(КонтекстПроверки.НастройкиКС.Заявитель);
	Показатели.Р0С03.Расшифровывать = КонтекстПроверки.НастройкиКС.Заявитель = "ФизЛицо";
	
	Для каждого СтраницаРаздела Из КонтекстПроверки.ДанныеКС.Р1 Цикл
		
		ЗарегистрироватьПроверку(КонтекстПроверки);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р1С01, СтраницаРаздела);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р1С02, СтраницаРаздела);
		ЗаполнитьИРасшифроватьПоказатель(КонтекстПроверки, Показатели.Р0С03, КонтекстПроверки.ДанныеКС.Р0);
		
		УсловиеВыполнено = КонтекстПроверки.НастройкиКС.Заявитель = "" ИЛИ КонтекстПроверки.НастройкиКС.Заявитель = "ЮрЛицо"
			И ЗначениеЗаполнено(Показатели.Р1С02.Значение) И СтрДлина(СокрЛП(Показатели.Р1С02.Значение)) = 9
			ИЛИ КонтекстПроверки.НастройкиКС.Заявитель = "ФизЛицо" И НЕ ЗначениеЗаполнено(Показатели.Р1С02.Значение)
			И СтрДлина(СокрЛП(Показатели.Р0С03.Значение)) = 12;
	
		КонтекстПроверки.НастройкиКС.ЭтоОшибка = УсловиеВыполнено И СтраницаРаздела.СтраницаЗаполнена И
			КонтекстПроверки.НастройкиКС.КБК.Найти(Показатели.Р1С01.Значение) = Неопределено;
		КонтекстПроверки.НастройкиКС.ДопСведения = СтрШаблон(КонтекстПроверки.НастройкиКС.ШаблонДопСведений,
			Сред(СтраницаРаздела.НомерСтраницы, 5));
		КонтекстПроверки.НастройкиКС.Детализация = СтрШаблон(КонтекстПроверки.НастройкиКС.ШаблонДетализации, 
			Показатели.Р1С01.ЗначениеСтр);
		ДополнитьТаблицуРезультата(КонтекстПроверки);
		
	КонецЦикла;
	
