	КонтекстПроверки.ДанныеКС.Вставить("ПриоритетыРазделов", ПриоритетыРазделов);
	
КонецПроцедуры

Процедура СформироватьДанныеОтчета(КонтекстПроверки, Ссылка)
	
	ИсходнаяСтруктура = Ссылка.ДанныеУведомления.Получить();
	
	КонтекстПроверки.ДанныеКС.Вставить("Р0", ОбщегоНазначения.СкопироватьРекурсивно(
		ИсходнаяСтруктура.ДанныеУведомления.Титульная));
	КонтекстПроверки.ДанныеКС.Р0.Вставить("ДатаПодписи2", Формат(КонтекстПроверки.ДанныеКС.Р0.ДАТА_ПОДПИСИ, "ДФ=MMdd; ДП=0000"));
	КонтекстПроверки.ДанныеКС.Р0.Вставить("ДатаПодписи3", Формат(КонтекстПроверки.ДанныеКС.Р0.ДАТА_ПОДПИСИ, "ДФ=dd.MM; ДП=00.00"));
	КонтекстПроверки.ДанныеКС.Р0.Вставить("ГодПодписи", Формат(КонтекстПроверки.ДанныеКС.Р0.ДАТА_ПОДПИСИ, "ДФ=yyyy; ДП=(...)"));
	
	КонтекстПроверки.ДанныеКС.Вставить("ИНН", СокрЛП(КонтекстПроверки.ДанныеКС.Р0.ИНН));
	СтруктураПолей = Новый Структура("КППДекл,ОКТМО,КБК,СумНалогАванс,Период,НомерМесКварт,Год");
	
	Р1 = Новый Массив;
	Для каждого Элемент Из ИсходнаяСтруктура.ДанныеМногостраничныхРазделов.СумИсчНалог Цикл
		СтраницаРаздела = ОбщегоНазначения.СкопироватьРекурсивно(Элемент.Значение);
		СтраницаРаздела.Вставить("НомерСтраницы", 
			"Стр. " + XMLСтрока((ИсходнаяСтруктура.ДанныеМногостраничныхРазделов.СумИсчНалог.Индекс(Элемент) + 1)));
		СтраницаРаздела.Вставить("СтраницаЗаполнена", РазделЗаполнен(СтраницаРаздела, СтруктураПолей));
		СформироватьКодПериода(СтраницаРаздела);
		Р1.Добавить(СтраницаРаздела);
	КонецЦикла;
	
	КонтекстПроверки.ДанныеКС.Вставить("Р1", Р1);
	
КонецПроцедуры

Процедура СформироватьКодПериода(СтраницаРаздела)
	
	ПериодРаздела = ?(ЗначениеЗаполнено(СтраницаРаздела.Период), СтраницаРаздела.Период, "(...)");
	НомерМесяца = ?(ЗначениеЗаполнено(СтраницаРаздела.НомерМесКварт), СтраницаРаздела.НомерМесКварт, "(...)");
	СтраницаРаздела.Вставить("КодПериода", ПериодРаздела + "/" + НомерМесяца);
	
КонецПроцедуры

Функция РазделЗаполнен(СтраницаРаздела, СтруктураПолей)
	
	Результат = Ложь;
	
	Для каждого Элемент Из СтруктураПолей Цикл
		Результат = Результат ИЛИ ЗначениеЗаполнено(СтраницаРаздела[Элемент.Ключ]);
	КонецЦикла;
	
	Возврат Результат;

