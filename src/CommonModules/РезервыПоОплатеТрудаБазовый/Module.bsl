				НСтр("ru = 'Возможно ошибочно не заполнена колонка ""Подразделение"" в строке %1 списка "" Обязательства и резервы (сводно)""';
					|en = 'Возможно ошибочно не заполнена колонка ""Подразделение"" в строке %1 списка "" Обязательства и резервы (сводно)""'"),
			СтрокаТаблицы.НомерСтроки);
			Поле = Префикс + "Подразделение";
			ОбщегоНазначения.СообщитьПользователю(ТекстСообщения, ЭтотОбъект, Поле, "Объект");
		КонецЕсли;
		Если Не ЗначениеЗаполнено(СтрокаТаблицы.СпособОтраженияЗарплатыВБухучете) Тогда
			ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
				НСтр("ru = 'Возможно ошибочно не заполнена колонка ""Способ отражения"" в строке %1 списка "" Обязательства и резервы (сводно)""';
					|en = 'Возможно ошибочно не заполнена колонка ""Способ отражения"" в строке %1 списка "" Обязательства и резервы (сводно)""'"),
			СтрокаТаблицы.НомерСтроки);
			Поле = Префикс + "СпособОтраженияЗарплатыВБухучете";
			ОбщегоНазначения.СообщитьПользователю(ТекстСообщения, ЭтотОбъект, Поле, "Объект");
		КонецЕсли;
		Если ИспользоватьСтатьиФинансированияЗарплата И Не ЗначениеЗаполнено(СтрокаТаблицы.СтатьяФинансирования) Тогда
			ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
				НСтр("ru = 'Возможно ошибочно не заполнена колонка ""Статья финансирования"" в строке %1 списка "" Обязательства и резервы (сводно)""';
					|en = 'Возможно ошибочно не заполнена колонка ""Статья финансирования"" в строке %1 списка "" Обязательства и резервы (сводно)""'"),
