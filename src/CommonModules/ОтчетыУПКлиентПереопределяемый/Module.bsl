			МенюОтчетов  = Новый Массив;
			МенюДействий = Новый Массив;
			
			// Расшифровать отчетом -> "Остатки и доступность товаров"
			ПараметрыОтчета = Новый Структура;
			ПараметрыОтчета.Вставить("Имя",       "ПоНоменклатуреКонтекст");
			ПараметрыОтчета.Вставить("Заголовок", НСтр("ru = 'Остатки и доступность товаров';
														|en = 'Stock availability'"));
			ПараметрыОтчета.Вставить("ИмяОтчета", "Отчет.ОстаткиИДоступностьТоваров");
			
