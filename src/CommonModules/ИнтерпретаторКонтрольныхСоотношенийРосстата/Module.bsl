#Область СлужебныеПроцедурыИФункции

Функция СформироватьКонтекстДляПроверок(Шаблон, ФайлВырузки, ВыгрузкаПрошлогоПериода = Неопределено) Экспорт 
	Попытка
		Результат = Истина;
		Контекст = Новый Структура;
		Контекст.Вставить("СтруктураПоискаСекцияСтрока", Новый Структура("section, row"));
		Контекст.Вставить("ВыполнятьПроверку", Истина);
		Контекст.Вставить("ПроверкаОтчетаНаПустоту", Истина);
		Контекст.Вставить("ОТЧ", Новый ОписаниеТипов("Число"));
		Контекст.Вставить("ОПС", ОбщегоНазначения.ОписаниеТипаСтрока(1000));
		Контекст.Вставить("ОПБ", Новый ОписаниеТипов("Булево"));
		Контекст.Вставить("ПолучитьВсеАргументыКромеСуммированийВВыражении", Новый Соответствие);
		Контекст.Вставить("ПолучитьВсеАргументыВыражения", Новый Соответствие);
		Контекст.Вставить("СписокЗначений", Новый СписокЗначений);
		Контекст.Вставить("ГодыПрименимости", Новый СписокЗначений);
		Контекст.Вставить("РеквизитОтчетаГод");
		Контекст.Вставить("РеквизитОтчетаПериод");
		Контекст.Вставить("ОшибочнаяСтрока");
		Контекст.Вставить("ОшибочнаяГрафа");
		Контекст.Вставить("УИД1", Строка(Новый УникальныйИдентификатор));
		Контекст.Вставить("УИД2", Строка(Новый УникальныйИдентификатор));
		ВариантыГраф = Новый Соответствие;
		ДополнитьКонтекстТаблицейВариацийСтрок(Контекст);
		ДополнитьКонтекстТаблицейДиапазонов(Контекст);
		СформироватьСоответствиеЗамен(Контекст);
		Результат = Результат И ПолучитьТаблицуДанныеШаблона(Шаблон, Контекст);
		ДанныеОтчетаВТаблицу(ФайлВырузки, Контекст, "ОсновнойПериод_");
		Контекст.Вставить("ОсновнойПериод_ДанныеОтчета_МножественныеСтроки", ОтобратьМножественныеСтроки(Контекст["ОсновнойПериод_ДанныеОтчета"]));
		Если Не ВыгрузкаПрошлогоПериода = Неопределено Тогда
			Если ТипЗнч(ВыгрузкаПрошлогоПериода) = Тип("ТекстовыйДокумент") Тогда 
				Если ЗначениеЗаполнено(ВыгрузкаПрошлогоПериода.ПолучитьТекст()) Тогда 
					ДанныеОтчетаВТаблицу(ВыгрузкаПрошлогоПериода.ПолучитьТекст(), Контекст, "ПредыдущийПериод_");
					Контекст.Вставить("ПредыдущийПериод_ДанныеОтчета_МножественныеСтроки", ОтобратьМножественныеСтроки(Контекст["ПредыдущийПериод_ДанныеОтчета"]));
				КонецЕсли;
			Иначе
				ДанныеОтчетаВТаблицу(ВыгрузкаПрошлогоПериода, Контекст, "ПредыдущийПериод_");
				Контекст.Вставить("ПредыдущийПериод_ДанныеОтчета_МножественныеСтроки", ОтобратьМножественныеСтроки(Контекст["ПредыдущийПериод_ДанныеОтчета"]));
			КонецЕсли;
		КонецЕсли;
		ФильтрацияОбязательныхДляВводаЯчеекПоПериоду(Контекст);
		ПервичноеПреобразованиеКонтрольныхСоотношений(Контекст);
	Исключение
		ОбщегоНазначения.СообщитьПользователю(НСтр("ru = 'Проверка контрольных соотношений осуществляться не будет';
													|en = 'Проверка контрольных соотношений осуществляться не будет'"));
		Возврат Неопределено;
	КонецПопытки;
	
	Если Результат Тогда 
		Возврат Контекст;
	КонецЕсли;
КонецФункции

Функция ПервыйКлючСоответсвия(Соответствие)
	Для Каждого КЗ Из Соответствие Цикл 
		Возврат КЗ.Ключ;
	КонецЦикла;
КонецФункции

Функция СписокПоВыражению(Контекст, Выражение)
	Соответствие = Новый Соответствие;
	Для Каждого Элт Из СтрРазделить(Выражение, ",") Цикл 
		Если ЗначениеЗаполнено(Контекст.ОТЧ.ПривестиЗначение(Элт)) Тогда 
			Соответствие.Вставить(Элт);
		ИначеЕсли ЗначениеЗаполнено(СтрНайти(Элт, "-")) Тогда 
			Разложение = СтрРазделить(Элт, "-");
			ДиапазонНиз = Контекст.ОТЧ.ПривестиЗначение(Разложение[0]);
			ДиапазонВерх = Контекст.ОТЧ.ПривестиЗначение(Разложение[1]);
			Если ДиапазонНиз <= ДиапазонВерх И ДиапазонНиз > 0 Тогда 
				Для Инд = ДиапазонНиз По ДиапазонВерх Цикл 
					Соответствие.Вставить(Формат(Инд, "ЧГ="));
				КонецЦикла;
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	
	Результат = Новый Массив;
	Для Каждого КЗ Из Соответствие Цикл 
		Результат.Добавить(КЗ.Ключ);
	КонецЦикла;
	Возврат Результат;
КонецФункции

Процедура ПреобразованныеВыражения(СтруктураОбхода, Знач СтартовыйИндекс)
	Если СтартовыйИндекс = СтруктураОбхода.МассивИсходный.Количество() Тогда 
		ВыражениеРезультат = СтруктураОбхода.Правило;
		Для Инд = 0 По СтруктураОбхода.МассивИсходный.ВГраница() Цикл 
			ЧтоМеняем = "";
			НаЧтоМеняем = "";
			Для Итер = 1 По 6 Цикл 
				Если СтруктураОбхода.НомерСкобки = Итер Тогда 
					ЧтоМеняем = ЧтоМеняем + "[" + СтруктураОбхода.МассивИсходный[Инд] + "]";
					НаЧтоМеняем = НаЧтоМеняем + "[" + СтруктураОбхода.НакопленныеЗамены[Инд] + "]";
				Иначе
					АргСк = ПервыйКлючСоответсвия(СтруктураОбхода.АргументыСкобки[Итер]);
					Если ЗначениеЗаполнено(АргСк) Тогда 
						ЧтоМеняем = ЧтоМеняем + "[" + АргСк + "]";
						НаЧтоМеняем = НаЧтоМеняем + "[" + АргСк + "]";
					КонецЕсли;
				КонецЕсли;
			КонецЦикла;
			ВыражениеРезультат = СтрЗаменить(ВыражениеРезультат, ЧтоМеняем, НаЧтоМеняем);
		КонецЦикла;
		СтруктураОбхода.РезультатПреобразований.Вставить(ВыражениеРезультат);
	Иначе
		Для Каждого Элт Из СтруктураОбхода.МассивПреобразованный[СтартовыйИндекс] Цикл 
			СтруктураОбхода.НакопленныеЗамены.Вставить(СтартовыйИндекс, Элт);
			ПреобразованныеВыражения(СтруктураОбхода, СтартовыйИндекс + 1);
			СтруктураОбхода.НакопленныеЗамены.Удалить(СтартовыйИндекс);
		КонецЦикла;
	КонецЕсли;
КонецПроцедуры

Процедура ПервичноеПреобразованиеКонтрольныхСоотношений(Контекст)
	ИтоговаяТаблицаКС = Контекст.ТаблицаКонтрольныхСоотношений.СкопироватьКолонки();
	ИтоговаяТаблицаКС.Колонки.Добавить("uuid", Новый ОписаниеТипов("УникальныйИдентификатор"));
	Для Каждого Стр Из Контекст.ТаблицаКонтрольныхСоотношений Цикл 
		Если Не ЭтоСкалярноеУсловие(Контекст, Стр.condition)
			Или ПолучитьВсеАргументыСуммированияВВыражении(Стр.rule).Количество() <> 0 Тогда 
			ЗаполнитьЗначенияСвойств(ИтоговаяТаблицаКС.Добавить(), Стр);
			Продолжить;
		КонецЕсли;
		
		АргументыВыражения = ПолучитьВсеАргументыВыражения(Контекст, Стр.rule);
		АргументыСкобки = Новый Массив;
		Для Инд = 1 По 6 Цикл 
			АргументыСкобки.Вставить(Инд, Новый Соответствие);
		КонецЦикла;
		Для Каждого КЗ Из АргументыВыражения Цикл 
			Для Инд = 1 По 6 Цикл 
				АргументыСкобки[Инд].Вставить(ПолучитьАргументСкобки(КЗ.Ключ, Инд), Истина);
			КонецЦикла;
		КонецЦикла;
		
		Если АргументыСкобки[1].Количество() <> 1 Или АргументыСкобки[4].Количество() <> 1 
			Или АргументыСкобки[5].Количество() <> 1 Или АргументыСкобки[6].Количество() <> 1
			Или (АргументыСкобки[2].Количество() + АргументыСкобки[3].Количество() <> 3) Тогда 
			ЗаполнитьЗначенияСвойств(ИтоговаяТаблицаКС.Добавить(), Стр);
			Продолжить;
		КонецЕсли;
		
		Если (АргументыСкобки[2].Количество() = 1 И ЗначениеЗаполнено(Контекст.ОТЧ.ПривестиЗначение(ПервыйКлючСоответсвия(АргументыСкобки[2]))))
			Или (АргументыСкобки[3].Количество() = 1 И ЗначениеЗаполнено(Контекст.ОТЧ.ПривестиЗначение(ПервыйКлючСоответсвия(АргументыСкобки[3])))) Тогда 
			ЗаполнитьЗначенияСвойств(ИтоговаяТаблицаКС.Добавить(), Стр);
			Продолжить;
		КонецЕсли;
		
		АргументыДляРазложения = ?(АргументыСкобки[2].Количество() = 1, АргументыСкобки[3], АргументыСкобки[2]);
		Для Каждого КЗ Из АргументыДляРазложения Цикл 
			АргументыДляРазложения[КЗ.Ключ] = СписокПоВыражению(Контекст, КЗ.Ключ);
		КонецЦикла;
		
		СтруктураОбхода = Новый Структура;
		СтруктураОбхода.Вставить("Контекст", Контекст);
		СтруктураОбхода.Вставить("МассивИсходный", Новый Массив);
		СтруктураОбхода.Вставить("МассивПреобразованный", Новый Массив);
		СтруктураОбхода.Вставить("Правило", Стр.rule);
		СтруктураОбхода.Вставить("РезультатПреобразований", Новый Соответствие);
		СтруктураОбхода.Вставить("НакопленныеЗамены", Новый Массив);
		СтруктураОбхода.Вставить("НомерСкобки", ?(АргументыСкобки[2].Количество() = 1, 3, 2));
		СтруктураОбхода.Вставить("АргументыСкобки", АргументыСкобки);
		
		Для Каждого КЗ1 Из АргументыДляРазложения Цикл 
			СтруктураОбхода.МассивИсходный.Добавить(КЗ1.Ключ);
			СтруктураОбхода.МассивПреобразованный.Добавить(АргументыДляРазложения[КЗ1.Ключ]);
		КонецЦикла;
		
		ПреобразованныеВыражения(СтруктураОбхода, 0);
		Если СтруктураОбхода.РезультатПреобразований.Количество() > 1 Тогда
			УИД = Новый УникальныйИдентификатор;
			Для Каждого КЗ Из СтруктураОбхода.РезультатПреобразований Цикл 
				НовСтр = ИтоговаяТаблицаКС.Добавить();
				ЗаполнитьЗначенияСвойств(НовСтр, Стр);
				НовСтр.uuid = УИД;
				НовСтр.rule = КЗ.Ключ;
			КонецЦикла;
		Иначе
			ЗаполнитьЗначенияСвойств(ИтоговаяТаблицаКС.Добавить(), Стр);
		КонецЕсли;
	КонецЦикла;
	Контекст.ТаблицаКонтрольныхСоотношений = ИтоговаяТаблицаКС;
	Контекст.Вставить("ОбработанныеКС", Новый Соответствие);
КонецПроцедуры

Функция ПолныеАргументыСуммированияВВыражении(Контекст, Выражение)
	ВсеАргументыСуммирования = Новый Соответствие;
	ВыражениеИтер = Выражение;
	ИндексСуммы = СтрНайти(ВыражениеИтер, "sum");
	Пока Не ИндексСуммы = 0 Цикл
		Если Сред(ВыражениеИтер, ИндексСуммы + 3, 1) = "(" Тогда 
			Остаток = СтрДлина(ВыражениеИтер);
			ИндексОткр = ИндексСуммы + 3;
			Итер = 0;
			Счетчик = 1;
			Пока Счетчик > 0 И (Итер + ИндексОткр <= Остаток + 1) Цикл 
				Итер = Итер + 1;
				СимволВПозиции = Сред(ВыражениеИтер, ИндексОткр + Итер, 1);
				Если СимволВПозиции = "(" Тогда 
					Счетчик = Счетчик + 1;
				ИначеЕсли СимволВПозиции = ")" Тогда 
					Счетчик = Счетчик - 1;
				КонецЕсли;
			КонецЦикла;
			Если Счетчик = 0 Тогда 
				ВсеАргументыСуммирования.Вставить(Сред(ВыражениеИтер, ИндексОткр, Итер + 1));
			КонецЕсли;
		ИначеЕсли Сред(ВыражениеИтер, ИндексСуммы + 3, 1) = "{" Тогда 
			ИндексОткр = СтрНайти(ВыражениеИтер, "{", НаправлениеПоиска.СНачала, ИндексСуммы);
			ИндексЗакр = СтрНайти(ВыражениеИтер, "}", НаправлениеПоиска.СНачала, ИндексОткр);
			ВсеАргументыСуммирования.Вставить(Сред(ВыражениеИтер, ИндексОткр, ИндексЗакр - ИндексОткр + 1));
		КонецЕсли;
		ВыражениеИтер = Лев(ВыражениеИтер, ИндексСуммы - 1) + Сред(ВыражениеИтер, ИндексСуммы + 3);
		ИндексСуммы = СтрНайти(ВыражениеИтер, "sum");
	КонецЦикла;
	
	Возврат ВсеАргументыСуммирования;
КонецФункции

Функция ЭтоСкалярныйАргумент(Контекст, Выражение)
	Если СтрЧислоВхождений(Выражение, "[") <> 3 Или СтрЧислоВхождений(Выражение, "]") <> 3 Тогда 
		Возврат Ложь;
	КонецЕсли;
	Для Инд = 1 По 3 Цикл
		АргументСкобки = ПолучитьАргументСкобки(Выражение, Инд);
		Если СтрНайти(АргументСкобки, ",") > 0 Или Контекст.ОТЧ.ПривестиЗначение(АргументСкобки) = 0 Тогда 
			Возврат Ложь;
		КонецЕсли;
	КонецЦикла;
	Возврат Истина;
КонецФункции

Функция УдалениеОператораСуммированияСкаляровВВыражении(Контекст, Выражение)
	Если СтрНайти(ВРег(Выражение), "SUM") = 0 Тогда 
		Возврат Выражение;
	КонецЕсли;
	
	Выражение = НРег(Выражение);
	СтруктураОтбора = Новый Структура("row, section");
	Для Каждого КЗ Из ПолныеАргументыСуммированияВВыражении(Контекст, Выражение) Цикл 
		МожноУдалитьСумму = Истина;
		Для Каждого КЗ1 Из ПолучитьВсеАргументыВыражения(Контекст, КЗ.Ключ) Цикл 
			СтруктураОтбора.row = Прав("00000" + ПолучитьАргументСкобки(КЗ.Ключ, 2), 5);
			СтруктураОтбора.section = Прав("000" + ПолучитьАргументСкобки(КЗ.Ключ, 1), 3);
			
			МожноУдалитьСумму = МожноУдалитьСумму 
				И ЭтоСкалярныйАргумент(Контекст, КЗ1.Ключ)
				И Контекст.ТаблицаМножественныхСтрок.НайтиСтроки(СтруктураОтбора).Количество() = 0;
			Если Не МожноУдалитьСумму Тогда 
				Прервать;
			КонецЕсли;
		КонецЦикла;
		Если МожноУдалитьСумму Тогда
			Выражение = СтрЗаменить(Выражение, "sum" + КЗ.Ключ, "(" + КЗ.Ключ + ")");
		КонецЕсли;
	КонецЦикла;
	
	Возврат Выражение;
КонецФункции

Процедура УдалениеОператораСуммированияСкаляров(Контекст)
	Для Каждого Стр Из Контекст.ТаблицаКонтрольныхСоотношений Цикл 
		Стр.condition = УдалениеОператораСуммированияСкаляровВВыражении(Контекст, Стр.condition);
		Стр.rule = УдалениеОператораСуммированияСкаляровВВыражении(Контекст, Стр.rule);
	КонецЦикла;
КонецПроцедуры

Функция ОтобратьМножественныеСтроки(Таблица)
	ТаблицаРез = Таблица.СкопироватьКолонки();
	Для Каждого Стр Из Таблица Цикл
		Если Стр.s1_isPresent Или Стр.s2_isPresent Или Стр.s3_isPresent Тогда 
			ЗаполнитьЗначенияСвойств(ТаблицаРез.Добавить(), Стр);
		КонецЕсли;
	КонецЦикла;
	Возврат ТаблицаРез;
КонецФункции

Процедура ДополнитьКонтекстТаблицейВариацийСтрок(Контекст)
	Строки = Новый ТаблицаЗначений;
	Строки.Колонки.Добавить("section");
	Строки.Колонки.Добавить("row");
	Строки.Колонки.Добавить("s1", Контекст.ОПС);
	Строки.Колонки.Добавить("s1_isPresent", Контекст.ОПБ);
	Строки.Колонки.Добавить("s2", Контекст.ОПС);
	Строки.Колонки.Добавить("s2_isPresent", Контекст.ОПБ);
	Строки.Колонки.Добавить("s3", Контекст.ОПС);
	Строки.Колонки.Добавить("s3_isPresent", Контекст.ОПБ);
	Контекст.Вставить("ВариацииСтрок", Строки);
	
	Строки = Новый ТаблицаЗначений;
	Строки.Колонки.Добавить("Секция");
	Строки.Колонки.Добавить("Колонка");
	Строки.Колонки.Добавить("Специфика");
	Контекст.Вставить("КолонкиСпецифики", Строки);
КонецПроцедуры

Процедура ДополнитьКонтекстТаблицейДиапазонов(Контекст)
	Строки = Новый ТаблицаЗначений;
	Строки.Колонки.Добавить("Секция");
	Строки.Колонки.Добавить("Строка");
	Строки.Колонки.Добавить("Колонка");
