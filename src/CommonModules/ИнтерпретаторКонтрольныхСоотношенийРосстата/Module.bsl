Функция СтруктурироватьВыражение(Выражение, Контекст)
	Если Не ЗначениеЗаполнено(Выражение) Тогда 
		Возврат Выражение;
	КонецЕсли;
	
	Результат = НРег(Выражение);
	Для Каждого Терм Из СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок("sumisnull,sumnullif,sumround,sumabs,sumcoalesce,sumfloor", ",") Цикл 
		ДлТерм = СтрДлина(Терм);
		ЗаменаНа = "sum(" + Сред(Терм, 4);
		Пока Истина Цикл 
			ИндексСум = СтрНайти(Результат, Терм);
			Если ИндексСум = 0 Тогда 
				Прервать;
			КонецЕсли;
			ОткрСимвол = Сред(Результат, ИндексСум + ДлТерм, 1);
			Если ОткрСимвол = "(" Тогда 
				ЗакрСимвол = ")";
			ИначеЕсли ОткрСимвол = "{" Тогда 
				ЗакрСимвол = "}";
			Иначе
				ВызватьИсключение "";
			КонецЕсли;
			
			Инд = ДлТерм + 1;
			Счетчик = 1;
			Пока Счетчик > 0 И Инд < СтрДлина(Результат) Цикл 
				Если Сред(Результат, ИндексСум + Инд, 1) = ОткрСимвол Тогда 
					Счетчик = Счетчик + 1;
				ИначеЕсли Сред(Результат, ИндексСум + Инд, 1) = ЗакрСимвол Тогда 
					Счетчик = Счетчик - 1;
				КонецЕсли;
				Инд = Инд + 1;
			КонецЦикла;
			Если Счетчик <> 0 Тогда 
				ВызватьИсключение "";
			КонецЕсли;
			
			НовыйАрг = ЗаменаНа + Сред(Результат, ИндексСум + ДлТерм, ИндексСум + Инд) + ")";
			Результат = Лев(Результат, ИндексСум - 1) + НовыйАрг + Сред(Результат, ИндексСум + Инд);
		КонецЦикла;
	КонецЦикла;
	
	Аргументы = ПолучитьВсеАргументыВыражения(Контекст, Результат);
	Для Каждого КЗ Из Аргументы Цикл
		Аргумент = Кз.Ключ;
		Если ЗначениеЗаполнено(Контекст.ОТЧ.ПривестиЗначение(ПолучитьАргументСкобки(Аргумент, 2))) Тогда 
			Продолжить;
		КонецЕсли;
		Секция = Прав("000" + Контекст.ОТЧ.ПривестиЗначение(ПолучитьАргументСкобки(Аргумент, 1)), 3);
		Если Контекст.СтрокиСекции[Секция] = Неопределено
			Или Контекст.СтрокиСекции[Секция].Количество() <> 1 Тогда 
			Продолжить;
		КонецЕсли;
		
		Для Каждого Кз1 Из Контекст.СтрокиСекции[Секция] Цикл 
			НовАрг = Лев(Аргумент, СтрНайти(Аргумент, "[", НаправлениеПоиска.СНачала, 1, 2)) + Контекст.ОТЧ.ПривестиЗначение(Кз1.Ключ) + Сред(Аргумент, СтрНайти(Аргумент, "]", НаправлениеПоиска.СНачала, 1, 2));
			Результат = СтрЗаменить(Результат, Аргумент, НовАрг);
		КонецЦикла;
	КонецЦикла;
	
	Возврат Результат;
КонецФункции

Функция УсловиеИКонтрольИзРазныхРазделов(Контекст, condition, rule)
	СекцииУсловия = Новый Соответствие;
	СекцииКонтроля = Новый Соответствие;
	Аргументы = ПолучитьВсеАргументыКромеСуммированийВВыражении(Контекст, condition);
	Для Каждого Арг Из Аргументы Цикл 
		СекцииУсловия.Вставить(ПолучитьАргументСкобки(Арг.Ключ, 1), Истина);
	КонецЦикла;
	Аргументы = ПолучитьВсеАргументыКромеСуммированийВВыражении(Контекст, rule);
	Для Каждого Арг Из Аргументы Цикл 
		СекцииКонтроля.Вставить(ПолучитьАргументСкобки(Арг.Ключ, 1), Истина);
	КонецЦикла;
	Если СекцииКонтроля.Количество() = 1
		И СекцииУсловия.Количество() = 1 Тогда 
		
		Возврат (Неопределено = СекцииУсловия[ПолучитьАргументСкобки(Арг.Ключ, 1)]);
	КонецЕсли;
	
	Возврат Ложь;
КонецФункции

Функция ДополнениеСтрокИГраф(condition, rule, СтрокиИГрафы, Контекст)
	Если СтрокиИГрафы.Количество() = 0 Тогда 
		Возврат Неопределено;
	КонецЕсли;
	РезультатАнализа = Новый Массив;
	Для Инд = 0 По 6 Цикл 
		РезультатАнализа.Вставить(Инд, Новый Соответствие);
	КонецЦикла;
	Аргументы = ПолучитьВсеАргументыВыражения(Контекст, condition);
	Для Каждого КЗ Из Аргументы Цикл 
		РезультатАнализа[0].Вставить(СтрЧислоВхождений(КЗ.Ключ, "["));
		Для Инд = 1 По 6 Цикл
			РезультатАнализа[Инд].Вставить(ПолучитьАргументСкобки(КЗ.Ключ, Инд));
		КонецЦикла;
	КонецЦикла;
	Аргументы = ПолучитьВсеАргументыВыражения(Контекст, rule);
	Для Каждого КЗ Из Аргументы Цикл
		РезультатАнализа[0].Вставить(СтрЧислоВхождений(КЗ.Ключ, "["));
		Для Инд = 1 По 6 Цикл
			РезультатАнализа[Инд].Вставить(ПолучитьАргументСкобки(КЗ.Ключ, Инд));
		КонецЦикла;
	КонецЦикла;
	
	КонтрольноеЗначение = РезультатАнализа[0].Количество();
	Если КонтрольноеЗначение <> 1 Тогда 
		Возврат Неопределено;
	КонецЕсли;
	
	Для Инд = 1 По 3 Цикл
		КонтрольноеЗначение = КонтрольноеЗначение + ?(РезультатАнализа[Инд].Количество() = 1, 0, 1);
	КонецЦикла;
	
	Если КонтрольноеЗначение <> 1 Тогда 
		Возврат Неопределено;
	КонецЕсли;
	
	Для Инд = 4 По 6 Цикл
		КонтрольноеЗначение = КонтрольноеЗначение + ?(РезультатАнализа[Инд].Количество() = 1, 0, 1);
	КонецЦикла;
	
	Если КонтрольноеЗначение <> 2 Тогда 
		Возврат Неопределено;
	КонецЕсли;
	ИндексРазличныеВариации = 0;
	Для Инд = 4 По 6 Цикл
		Если РезультатАнализа[Инд].Количество() > 1 Тогда 
			ИндексРазличныеВариации = Инд;
			Прервать;
		КонецЕсли;
	КонецЦикла;
	Если ИндексРазличныеВариации = 0 Тогда 
		Возврат Неопределено;
	Иначе
		Для Каждого КЗ Из РезультатАнализа[ИндексРазличныеВариации] Цикл 
			Если СтрНайти(КЗ.Ключ, "-") > 0 Или СтрНайти(КЗ.Ключ, ",") > 0 Или СтрНайти(КЗ.Ключ, "*") > 0 Тогда 
				Возврат Неопределено;
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
	Для Каждого КЗ Из РезультатАнализа[0] Цикл
		ВсегоАргументов = КЗ.Ключ;
		Прервать;
	КонецЦикла;
	
	Отбор = ОбщегоНазначения.СтрокаТаблицыЗначенийВСтруктуру(СтрокиИГрафы[0]);
	Если ВсегоАргументов < 6 Тогда 
		Отбор.s3_IsPresent = Ложь;
		Отбор.Удалить("s3");
	КонецЕсли;
	Если ВсегоАргументов < 5 Тогда 
		Отбор.s2_IsPresent = Ложь;
		Отбор.Удалить("s2");
	КонецЕсли;
	Отбор.Удалить("columns");
	
	Для Каждого КЗ Из Аргументы Цикл
		АргументСкобки = ПолучитьАргументСкобки(КЗ.Ключ, ИндексРазличныеВариации);
		Отбор["s" + (ИндексРазличныеВариации - 3)] = АргументСкобки;
		Если СтрокиИГрафы.НайтиСтроки(Отбор).Количество() = 0
			И Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Отбор).Количество() > 0 Тогда 
			
			НовСтр = СтрокиИГрафы.Добавить();
			ЗаполнитьЗначенияСвойств(НовСтр, СтрокиИГрафы[0]);
			НовСтр["s" + (ИндексРазличныеВариации - 3)] = АргументСкобки;
		КонецЕсли;
	КонецЦикла;
	
	Возврат Новый Структура("ВсегоАргументов, ИндексРазличныеВариации", ВсегоАргументов, ИндексРазличныеВариации);
КонецФункции

Процедура УдалениеОшибочноМножественныхСтрок(Контекст, Выражение)
	Аргументы = ПолучитьВсеАргументыКромеСуммированийВВыражении(Контекст, Выражение);
	ОшибочныеМногострочныеАргументы = Новый Соответствие;
	Для Каждого КЗ Из Аргументы Цикл 
		Если ЭтоОшибочноМногострочноеВыражение(Контекст, КЗ.Ключ) Тогда 
			Индекс4Ск = СтрНайти(КЗ.Ключ, "[", НаправлениеПоиска.СНачала, 1, 4);
			Если Индекс4Ск > 0 Тогда 
				ОшибочныеМногострочныеАргументы.Вставить(КЗ.Ключ, Лев(КЗ.Ключ, Индекс4Ск-1));
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	
	Для Каждого КЗ Из ОшибочныеМногострочныеАргументы Цикл 
		Выражение = СтрЗаменить(Выражение, "{{" + КЗ.Ключ + "}}", "{{" + КЗ.Значение + "}}");
		Выражение = СтрЗаменить(Выражение, "{" + КЗ.Ключ + "}", "{" + КЗ.Значение + "}");
	КонецЦикла;
КонецПроцедуры

Функция ПроверитьКонтрольноеСоотношение(СтрокаТаблицы, Контекст) Экспорт
	Если Не Контекст.Свойство("ПредыдущийПериод_ДанныеОтчета")
		И (СтрНайти(СтрокаТаблицы.condition, "{{") > 0 Или СтрНайти(СтрокаТаблицы.rule, "{{") > 0) Тогда 
		
		// Нет данных предидущего периода
		// Соотношение не проверяется
		Возврат Истина;
	КонецЕсли;
	
	Если ЗначениеЗаполнено(СтрокаТаблицы.periodClause) Тогда 
		Если Не ПроверитьУсловиеНаПериод(Контекст, СтрокаТаблицы.periodClause) Тогда 
			Возврат Истина;
		КонецЕсли;
	КонецЕсли;
	
	СтрокаТаблицы.condition = СтруктурироватьВыражение(СтрокаТаблицы.condition, Контекст);
	СтрокаТаблицы.rule = СтруктурироватьВыражение(СтрокаТаблицы.rule, Контекст);
	
	Графы = Неопределено;
	Строки = Неопределено;
	Специфики = Неопределено;
	СтрокиИГрафы = Неопределено;
	Контекст["ОшибочнаяСтрока"] = Неопределено;
	Контекст["ОшибочнаяГрафа"] = Неопределено;
	Если ЗначениеЗаполнено(СтрокаТаблицы.condition) Тогда
		УдалениеОшибочноМножественныхСтрок(Контекст, СтрокаТаблицы.condition);
		УдалениеОшибочноМножественныхСтрок(Контекст, СтрокаТаблицы.rule);
		ПреобразованиеУсловияСуммированияМножественныхСтрокИСкаляров(Контекст, СтрокаТаблицы.condition);
		ПодстановкаИзвестныхСкалярныхАргументов(Контекст, СтрокаТаблицы.condition, СтрокаТаблицы.precision);
		ПодстановкаИзвестныхСкалярныхАргументов(Контекст, СтрокаТаблицы.rule, СтрокаТаблицы.precision);
		Если ЭтоСкалярноеУсловие(Контекст, СтрокаТаблицы.condition) Тогда
			Если Не ВычислитьСкалярноеУсловие(СтрокаТаблицы.condition, Контекст, СтрокаТаблицы.precision) Тогда 
				Возврат Истина;
			Иначе
				ПреобразованиеУсловияСуммированияМножественныхСтрокИСкаляров(Контекст, СтрокаТаблицы.rule);
				Если ЭтоСкалярноеУсловие(Контекст, СтрокаТаблицы.rule) Тогда
					Возврат ВычислитьСкалярноеУсловие(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision);
				ИначеЕсли ЭтоВариацияПоГрафам(Контекст, СтрокаТаблицы.rule, Графы) Тогда
					Возврат ВычислитьУсловиеПоГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Графы);
				ИначеЕсли ЭтоВариацияПоСтрокам(Контекст, СтрокаТаблицы.rule, Строки) Тогда
					Возврат ВычислитьУсловиеПоСтрокам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Строки);
				ИначеЕсли ЭтоВариацияПоСтрокамИлиГрафам(Контекст, СтрокаТаблицы.rule, СтрокиИГрафы) Тогда
					Возврат ВычислитьУсловиеПоСтрокамИлиГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, СтрокиИГрафы);
				Иначе
					ВызватьИсключение "";
				КонецЕсли;
			КонецЕсли;
		ИначеЕсли ЭтоВариацияПоГрафам(Контекст, СтрокаТаблицы.condition, Графы) Тогда
			ОтбГрафы = "";
			ГрафыИтер = Графы;
			Пока Истина Цикл
				Гр = Лев(ГрафыИтер, 4);
				Если Контекст.ОТЧ.ПривестиЗначение(Гр) = 0 Тогда 
					Прервать;
				КонецЕсли;
				ГрафыИтер = Сред(ГрафыИтер, 5);
				Если ВычислитьУсловиеПоГрафам(СтрокаТаблицы.condition, Контекст, СтрокаТаблицы.precision, Гр) Тогда 
					ОтбГрафы = ОтбГрафы + Гр;
				КонецЕсли;
			КонецЦикла;
			Если ЭтоСкалярноеУсловие(Контекст, СтрокаТаблицы.rule) Тогда
				Возврат (Не ЗначениеЗаполнено(ОтбГрафы)) Или ВычислитьСкалярноеУсловие(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision);
			Иначе
				Возврат ВычислитьУсловиеПоГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, ОтбГрафы);
			КонецЕсли;
		ИначеЕсли ЭтоВариацияПоСпецификам(Контекст, СтрокаТаблицы.condition, СтрокаТаблицы.rule, Строки, Специфики) Тогда
			Возврат ВычислитьУсловиеПоСпецификамСУсловием(СтрокаТаблицы.condition, СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Специфики);
		ИначеЕсли ЭтоВариацияПоСтрокам(Контекст, СтрокаТаблицы.condition, Строки) Тогда
			ОбработатьУсловиеПоСтрокам(СтрокаТаблицы.condition, Контекст, СтрокаТаблицы.precision, Строки);
			Если Строки.Количество() = 0 Тогда 
				Возврат Истина;
			КонецЕсли;
			ПодстановкаИзвестныхСкалярныхАргументов(Контекст, СтрокаТаблицы.rule, СтрокаТаблицы.precision);
			Если ЭтоСкалярноеУсловие(Контекст, СтрокаТаблицы.rule) Тогда
				Возврат ВычислитьСкалярноеУсловие(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision);
			Иначе
				Если УсловиеИКонтрольИзРазныхРазделов(Контекст, СтрокаТаблицы.condition, СтрокаТаблицы.rule) Тогда 
					ПодстановкаИзвестныхСкалярныхАргументов(Контекст, СтрокаТаблицы.rule, СтрокаТаблицы.precision);
					Если ЭтоСкалярноеУсловие(Контекст, СтрокаТаблицы.rule) Тогда
						Возврат ВычислитьСкалярноеУсловие(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision);
					ИначеЕсли ЭтоВариацияПоГрафам(Контекст, СтрокаТаблицы.rule, Графы) Тогда
						Возврат ВычислитьУсловиеПоГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Графы);
					ИначеЕсли ЭтоВариацияПоСтрокам(Контекст, СтрокаТаблицы.rule, Строки) Тогда
						Возврат ВычислитьУсловиеПоСтрокам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Строки);
					ИначеЕсли ЭтоВариацияПоСтрокамИлиГрафам(Контекст, СтрокаТаблицы.rule, СтрокиИГрафы) Тогда
						ВсеАргументыВыражения = ПолучитьВсеАргументыВыражения(Контекст, СтрокаТаблицы.rule);
						ВариацииГраф = ПолучитьВариацииГраф(Контекст, ВсеАргументыВыражения);
						
						Если ВариацииГраф.Количество() = 1 Тогда
							ГрафыВУсловии = "";
							Для Каждого КЗ Из ВариацииГраф Цикл 
								ГрафыВУсловии = КЗ.Ключ;
							КонецЦикла;
							
							Для Кол = 1 По Контекст.КоличествоКолонок Цикл
								Если Не ЧислоВходитВДиапазон(Контекст, Кол, ГрафыВУсловии) Тогда 
									Продолжить;
								КонецЕсли;
								Выражение = ЗаменитьВариациюПоГрафамВВыражении(СтрокаТаблицы.rule, ВсеАргументыВыражения, Контекст, ГрафыВУсловии, Кол);
								Если ЭтоВариацияПоСтрокам(Контекст, Выражение, Строки) Тогда 
									Если Не ВычислитьУсловиеПоСтрокам(Выражение, Контекст, СтрокаТаблицы.precision, Строки) Тогда
										Возврат Ложь;
									КонецЕсли;
								ИначеЕсли ЭтоСкалярноеУсловие(Контекст, Выражение) Тогда
									Если Не ВычислитьСкалярноеУсловие(Выражение, Контекст, СтрокаТаблицы.precision) Тогда 
										Возврат Ложь;
									КонецЕсли;
								Иначе
									ВызватьИсключение "";
								КонецЕсли;
							КонецЦикла;
							Возврат Истина;
						КонецЕсли;
						
						Если СтрНайти(НРег(СтрокаТаблицы.rule), "sum") > 0 Тогда 
							Возврат ВычислитьБезусловноеВыражениеПоСтрокамИГрафам(Контекст, СтрокаТаблицы.rule, СтрокаТаблицы.precision);
						Иначе
							Возврат ВычислитьУсловиеПоСтрокамИлиГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, СтрокиИГрафы);
						КонецЕсли;
					КонецЕсли;
				КонецЕсли;
				
				Возврат ВычислитьУсловиеПоСтрокам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Строки);
			КонецЕсли;
		ИначеЕсли ЭтоВариацияПоСтрокамИлиГрафам(Контекст, СтрокаТаблицы.condition, СтрокиИГрафы) Тогда
			ОбработатьУсловиеПоСтрокамИлиГрафам(СтрокаТаблицы.condition, Контекст, СтрокаТаблицы.precision, СтрокиИГрафы);
			РезультатДополнения = ДополнениеСтрокИГраф(СтрокаТаблицы.condition, СтрокаТаблицы.rule, СтрокиИГрафы, Контекст);
			Если ТипЗнч(РезультатДополнения) = Тип("Структура") Тогда
				Возврат ВычислитьУсловиеДляМножественныхСтрокПоГрафам(СтрокаТаблицы.rule, Контекст, СтрокиИГрафы, РезультатДополнения, СтрокаТаблицы.precision);
			Иначе
				Возврат ВычислитьУсловиеПоСтрокамИлиГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, СтрокиИГрафы);
			КонецЕсли;
		Иначе
			ВызватьИсключение "";
		КонецЕсли;
	Иначе
		УдалениеОшибочноМножественныхСтрок(Контекст, СтрокаТаблицы.rule);
		ПреобразованиеУсловияСуммированияМножественныхСтрокИСкаляров(Контекст, СтрокаТаблицы.rule);
		ПодстановкаИзвестныхСкалярныхАргументов(Контекст, СтрокаТаблицы.rule, СтрокаТаблицы.precision);
		Если ЭтоСкалярноеУсловие(Контекст, СтрокаТаблицы.rule) Тогда
			Возврат ВычислитьСкалярноеУсловие(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision);
		ИначеЕсли ЭтоВариацияПоГрафам(Контекст, СтрокаТаблицы.rule, Графы) Тогда
			Возврат ВычислитьУсловиеПоГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Графы);
		ИначеЕсли ЭтоВариацияПоСтрокам(Контекст, СтрокаТаблицы.rule, Строки) Тогда
			Возврат ВычислитьУсловиеПоСтрокам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, Строки);
		ИначеЕсли ЭтоВариацияПоСтрокамИлиГрафам(Контекст, СтрокаТаблицы.rule, СтрокиИГрафы) Тогда
			ВсеАргументыВыражения = ПолучитьВсеАргументыВыражения(Контекст, СтрокаТаблицы.rule);
			ВариацииГраф = ПолучитьВариацииГраф(Контекст, ВсеАргументыВыражения);
			
			Если ВариацииГраф.Количество() = 1 Тогда
				ГрафыВУсловии = "";
				Для Каждого КЗ Из ВариацииГраф Цикл 
					ГрафыВУсловии = КЗ.Ключ;
				КонецЦикла;
				
				Для Кол = 1 По Контекст.КоличествоКолонок Цикл
					Если Не ЧислоВходитВДиапазон(Контекст, Кол, ГрафыВУсловии) Тогда 
						Продолжить;
					КонецЕсли;
					Выражение = ЗаменитьВариациюПоГрафамВВыражении(СтрокаТаблицы.rule, ВсеАргументыВыражения, Контекст, ГрафыВУсловии, Кол);
					Если ЭтоВариацияПоСтрокам(Контекст, Выражение, Строки) Тогда 
						Если Не ВычислитьУсловиеПоСтрокам(Выражение, Контекст, СтрокаТаблицы.precision, Строки) Тогда
							Возврат Ложь;
						КонецЕсли;
					ИначеЕсли ЭтоСкалярноеУсловие(Контекст, Выражение) Тогда
						Если Не ВычислитьСкалярноеУсловие(Выражение, Контекст, СтрокаТаблицы.precision) Тогда 
							Возврат Ложь;
						КонецЕсли;
					ИначеЕсли ЭтоОшибочноМногострочноеВыражение(Контекст, Выражение) Тогда 
						Возврат Истина;
					Иначе
						ВызватьИсключение "";
					КонецЕсли;
				КонецЦикла;
				Возврат Истина;
			КонецЕсли;
			
			Если СтрНайти(НРег(СтрокаТаблицы.rule), "sum") > 0 Тогда 
				Возврат ВычислитьБезусловноеВыражениеПоСтрокамИГрафам(Контекст, СтрокаТаблицы.rule, СтрокаТаблицы.precision);
			Иначе
				Возврат ВычислитьУсловиеПоСтрокамИлиГрафам(СтрокаТаблицы.rule, Контекст, СтрокаТаблицы.precision, СтрокиИГрафы);
			КонецЕсли;
		Иначе
			ВызватьИсключение "";
		КонецЕсли;
	КонецЕсли;
	
	Возврат Истина;
КонецФункции

Функция ПолучитьСведенияОШаблоне(Форма, ИмяМакетаАтрибутов)
	Результат = Новый Структура("ОКУД,КодШаблона,Версия,Шифр,КодФормы,Наименование,КодПериодичности");
	Если ТипЗнч(ИмяМакетаАтрибутов) = Тип("Строка") Тогда 
		ДопАтрибуты = РегламентированнаяОтчетность.СформироватьСтруктуруДопАтрибутов(Форма, ИмяМакетаАтрибутов);
		ДопАтрибуты.Свойство("OKUD", Результат.ОКУД);
		ДопАтрибуты.Свойство("code", Результат.КодШаблона);
		ДопАтрибуты.Свойство("version", Результат.Версия);
		ДопАтрибуты.Свойство("shifr", Результат.Шифр);
		ДопАтрибуты.Свойство("name", Результат.Наименование);
		ДопАтрибуты.Свойство("idf", Результат.КодФормы);
		ДопАтрибуты.Свойство("idp", Результат.КодПериодичности);
	ИначеЕсли ТипЗнч(ИмяМакетаАтрибутов) = Тип("Структура") Тогда
		ЗаполнитьЗначенияСвойств(Результат, ИмяМакетаАтрибутов);
	КонецЕсли;
	
	ТипЧисло = Новый ОписаниеТипов("Число");
	Результат.КодФормы = ТипЧисло.ПривестиЗначение(Результат.КодФормы);
	Результат.КодПериодичности = ТипЧисло.ПривестиЗначение(Результат.КодПериодичности);
	Возврат Результат;
КонецФункции

Процедура ПроверкаСоотношенийПриОтсутствииФормата(ПараметрыДляПроверки, АдресХранилища) Экспорт 
	Результат = Новый Структура;
	Результат.Вставить("КоличествоНайденныхБезОшибок", 0);
	Результат.Вставить("КоличествоНайденныхОшибок", 1);
	Результат.Вставить("ЭтоАвтопроверкаСтатистики", Истина);
	Результат.Вставить("СтруктураОшибок", Новый Структура);
	
	Результат.СтруктураОшибок.Вставить("Описание1", "");
	Результат.СтруктураОшибок.Вставить("Комментарий1", "Нет возможности проверить контрольные соотношения."
		+ " Возможность проверки появится по мере публикации формата выгрузки на сайте Росстата");
	Результат.СтруктураОшибок.Вставить("Нарушение1", "Опубликованный шаблон");
	Результат.СтруктураОшибок.Вставить("Расшифровка1", "");
	Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы1", "Контрольные соотношения из шаблона Росстата");
	Результат.СтруктураОшибок.Вставить("ЭтоОшибка1", Истина);
	
	ПоместитьВоВременноеХранилище(Результат, ПараметрыДляПроверки.АдресХранилища);
КонецПроцедуры

Функция ПроверитьКонтрольныеСоотношенияИзШаблона(Форма, ПараметрыПроверкиКонтрольныхСоотношений) Экспорт
	Разложение = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(ПараметрыПроверкиКонтрольныхСоотношений.ИмяФормы, ".");
	Если СтрНайти(ПараметрыПроверкиКонтрольныхСоотношений.ИмяФормы, "Внешний") <> 0 Тогда 
		мОбъектОтчета = РегламентированнаяОтчетностьВызовСервера.ОбъектОтчета(ПараметрыПроверкиКонтрольныхСоотношений.ИмяФормы);
		ВерсияФорматаВыгрузки = мОбъектОтчета.ВерсияФорматаВыгрузки(ТекущаяДатаСеанса(), Разложение[3]);
	Иначе
		ВерсияФорматаВыгрузки = Отчеты[Разложение[1]].ВерсияФорматаВыгрузки(ТекущаяДатаСеанса(), Разложение[3]);
	КонецЕсли;
	
	Если ВерсияФорматаВыгрузки <> Перечисления.ВерсииФорматовВыгрузки.ВерсияФСГС Тогда 
		РезультатВыполнения = Новый Структура("ЗаданиеВыполнено, АдресХранилища", Истина, ПоместитьВоВременноеХранилище(Неопределено, Форма.УникальныйИдентификатор));
		ПроверкаСоотношенийПриОтсутствииФормата(ПараметрыПроверкиКонтрольныхСоотношений, РезультатВыполнения.АдресХранилища);
		Возврат РезультатВыполнения;
	КонецЕсли;
	
	СведШаблон = ПолучитьСведенияОШаблоне(Форма, ПараметрыПроверкиКонтрольныхСоотношений.Атрибуты);
	ПараметрыПроверкиКонтрольныхСоотношений.Вставить("ВерсияШаблона", СведШаблон.Версия);
	ДанныеШаблонаИзРегистра = РегламентированнаяОтчетность.ПолучитьДанныеШаблона(Форма,
																				ПараметрыПроверкиКонтрольныхСоотношений.Атрибуты,
																				ПараметрыПроверкиКонтрольныхСоотношений.ШаблонРосстата,
																				СведШаблон,
																				СведШаблон.Версия);
	Если Не ПараметрыПроверкиКонтрольныхСоотношений.ДоступноФормированиеТекстаВыгрузкиБезКонтекстаФормы Тогда 
		Если ПараметрыПроверкиКонтрольныхСоотношений.ТиповойСпособФормированияТекстаВыгрузки Тогда 
			РегламентированнаяОтчетность.СформироватьСтруктуруПоказателей(Форма);
			ПараметрыВыгрузки = Форма.СформироватьСтруктуруПараметров();
			Если ТипЗнч(ДанныеШаблонаИзРегистра) = Тип("ХранилищеЗначения") И ПараметрыВыгрузки.Свойство("ВерсияШаблона") Тогда
				ПараметрыВыгрузки.ВерсияШаблона = СведШаблон.Версия;
			КонецЕсли;
			Если ТипЗнч(ПараметрыПроверкиКонтрольныхСоотношений.СхемаВыгрузки) = Тип("Строка") Тогда 
				ДеревоВыгрузки = РегламентированнаяОтчетность.ПолучитьДеревоВыгрузки(Форма, ПараметрыПроверкиКонтрольныхСоотношений.СхемаВыгрузки);
			ИначеЕсли ТипЗнч(ПараметрыПроверкиКонтрольныхСоотношений.СхемаВыгрузки) = Тип("ДеревоЗначений") Тогда
				ДеревоВыгрузки = ПараметрыПроверкиКонтрольныхСоотношений.СхемаВыгрузки;
			КонецЕсли;
			ПараметрыПроверкиКонтрольныхСоотношений.Вставить("ДеревоВыгрузки", ОбщегоНазначения.СкопироватьРекурсивно(ДеревоВыгрузки));
			Форма.ЗаполнитьДанными(ДеревоВыгрузки, ПараметрыВыгрузки);
			ПараметрыПроверкиКонтрольныхСоотношений.ТекстВыгрузки = РегламентированнаяОтчетность.ВыгрузитьДеревоВXML(ДеревоВыгрузки, ПараметрыВыгрузки);
		КонецЕсли;
	КонецЕсли;
	
	ПараметрыПроверкиКонтрольныхСоотношений.Вставить("ДополнятьРасшифровкой", 
		ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(ПараметрыПроверкиКонтрольныхСоотношений, "ДополнятьРасшифровкой", Ложь));
	ПараметрыПроверкиКонтрольныхСоотношений.Вставить("РасшифровкаПолейТитула", 
		ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(ПараметрыПроверкиКонтрольныхСоотношений, "РасшифровкаПолейТитула", Истина));
	Если ПараметрыПроверкиКонтрольныхСоотношений["ДополнятьРасшифровкой"] Тогда 
		ПараметрыПроверкиКонтрольныхСоотношений.Вставить("СписокРасшифровкиПоказателей", ПоместитьВоВременноеХранилище(Неопределено, Форма.УникальныйИдентификатор));
		ПараметрыПроверкиКонтрольныхСоотношений.Вставить("СписокРасшифровкиформулы", ПоместитьВоВременноеХранилище(Неопределено, Форма.УникальныйИдентификатор));
		ПараметрыПроверкиКонтрольныхСоотношений.Вставить("СсылкаНаДокументРО", Форма.СтруктураРеквизитовФормы.мСохраненныйДок);
	КонецЕсли;
	
	Если ОбщегоНазначенияКлиентСервер.СвойствоСтруктуры(ПараметрыПроверкиКонтрольныхСоотношений, "ВыполнятьСинхронно", Ложь) Тогда 
		РезультатВыполнения = Новый Структура("ЗаданиеВыполнено, АдресХранилища", Истина, ПоместитьВоВременноеХранилище(Неопределено, Форма.УникальныйИдентификатор));
		ПроверитьКонтрольныеСоотношенияВОтчетеВФоне(ПараметрыПроверкиКонтрольныхСоотношений, РезультатВыполнения.АдресХранилища);
		Возврат РезультатВыполнения;
	Иначе
		ПараметрыВыполненияВФоне = ДлительныеОперации.ПараметрыВыполненияВФоне(Форма.УникальныйИдентификатор);
		ПараметрыВыполненияВФоне.ЗапуститьВФоне = Истина;
		РезультатВыполнения = ДлительныеОперации.ВыполнитьВФоне("ИнтерпретаторКонтрольныхСоотношенийРосстата.ПроверитьКонтрольныеСоотношенияВОтчетеВФоне",
																ПараметрыПроверкиКонтрольныхСоотношений, ПараметрыВыполненияВФоне);
											
		РезультатВыполнения.Вставить("ЗаданиеВыполнено", НРег(РезультатВыполнения.Статус) = "выполнено");
		ПараметрыПроверкиКонтрольныхСоотношений.АдресХранилища       = РезультатВыполнения.АдресРезультата;
		ПараметрыПроверкиКонтрольныхСоотношений.ИдентификаторЗадания = РезультатВыполнения.ИдентификаторЗадания;
		
		Возврат РезультатВыполнения;
	КонецЕсли;
КонецФункции

Процедура ПроверитьКонтрольныеСоотношенияВОтчетеВФоне(ПараметрыДляПроверки, АдресХранилища) Экспорт 
	Результат = ПроверитьКонтрольныеСоотношенияВОтчете(ПараметрыДляПроверки);
	ПоместитьВоВременноеХранилище(Результат, ПараметрыДляПроверки.АдресХранилища);
КонецПроцедуры

Процедура ИзменитьВерсиюФорматаСПроверкойСовместимостиШаблонов(МодульОтчета, ПараметрыДляПроверки, ИтоговыйФайл) Экспорт
	Попытка
		Если СтрНачинаетсяС(ПараметрыДляПроверки.ИмяФормы, "Внешний") Тогда
			Возврат;
		КонецЕсли;
		
		ДопАтрибуты = РегламентированнаяОтчетность.СформироватьСтруктуруДопАтрибутов(ПараметрыДляПроверки.ИмяФормы, ПараметрыДляПроверки.Атрибуты);
		Если (НЕ ДопАтрибуты.Свойство("OKUD"))
		 ИЛИ (НЕ ДопАтрибуты.Свойство("code"))
		 ИЛИ (НЕ ЗначениеЗаполнено(ДопАтрибуты.code)) Тогда 
			Возврат;
		КонецЕсли;
		
		ИмяПоследнейФормыОчета = РегламентированнаяОтчетностьВызовСервера.ИмяФормыРеглОтчетаДействующейВОтчетномПериоде(
			РегламентированнаяОтчетностьВызовСервера.ИсточникОтчета(ПараметрыДляПроверки.ИмяФормы),
			ДобавитьМесяц(ТекущаяДатаСеанса(), 24));
		
		ЭтоПоследняяРеализацияОтчета = СтрЗаканчиваетсяНа(ПараметрыДляПроверки.ИмяФормы, ИмяПоследнейФормыОчета);
		
		Если ЭтоПоследняяРеализацияОтчета Тогда
			// В запросе будут отобраны шаблоны с любым "idf" длиной 3 символа в составе кода.
			КодШаблона = Лев(ДопАтрибуты.code, СтрДлина(ДопАтрибуты.code) - 6) + "___" + Прав(ДопАтрибуты.code, 3);
		Иначе
			// В запросе будут отобраны шаблоны с кодом равным коду текущего шаблона формы отчета.
			КодШаблона = ДопАтрибуты.code;
		КонецЕсли;
		
		КодФормыШаблона = Сред(ДопАтрибуты.code, СтрДлина(ДопАтрибуты.code) - 5, 3);
		
		Запрос = Новый Запрос;
		Запрос.Текст = "ВЫБРАТЬ
			|	РАЗНОСТЬДАТ(&ТекущаяДата, ШаблоныЭВФОтчетовСтатистики.ДатаДобавления, СЕКУНДА) КАК РазностьДат,
			|	ШаблоныЭВФОтчетовСтатистики.КодШаблона КАК КодШаблона,
			|	ШаблоныЭВФОтчетовСтатистики.Версия КАК Версия,
			|	ШаблоныЭВФОтчетовСтатистики.Шаблон КАК Шаблон,
			|	ПОДСТРОКА(ШаблоныЭВФОтчетовСтатистики.Версия, 7, 4)
			|	+ ПОДСТРОКА(ШаблоныЭВФОтчетовСтатистики.Версия, 4, 2)
			|	+ ПОДСТРОКА(ШаблоныЭВФОтчетовСтатистики.Версия, 1, 2) КАК ВерсияСортировка
			|ИЗ
			|	РегистрСведений.ШаблоныЭВФОтчетовСтатистики КАК ШаблоныЭВФОтчетовСтатистики
			|ГДЕ
			|	ШаблоныЭВФОтчетовСтатистики.ОКУД = &ОКУД
			|	И ШаблоныЭВФОтчетовСтатистики.КодШаблона ПОДОБНО &КодШаблона
			|УПОРЯДОЧИТЬ ПО
			|	ВерсияСортировка УБЫВ,
			|	КодШаблона УБЫВ,
			|	РазностьДат УБЫВ";
		Запрос.УстановитьПараметр("ОКУД", ДопАтрибуты.OKUD);
		Запрос.УстановитьПараметр("КодШаблона", КодШаблона);
		Запрос.УстановитьПараметр("ТекущаяДата", ТекущаяДатаСеанса());
		
		Выборка = Запрос.Выполнить().Выбрать();
		Если Выборка.Количество() = 0 Тогда 
			Возврат;
		КонецЕсли;
		
		ЗаменитьФормат = Ложь;
		Пока Выборка.Следующий() Цикл
			
			КодФормыШаблонаВыб = Сред(Выборка.КодШаблона, СтрДлина(Выборка.КодШаблона) - 5, 3);
			Если (ДатаВерсии(Выборка.Версия) <= ДатаВерсии(ДопАтрибуты.version)) ИЛИ (КодФормыШаблона > КодФормыШаблонаВыб) Тогда
				Продолжить;
			КонецЕсли;
			
			Если НЕ (Выборка.РазностьДат < 3600 И Выборка.РазностьДат > -3600) Тогда 
				Продолжить;
			КонецЕсли;
			
			ИтоговоеИмяШаблона = ПараметрыДляПроверки.ШаблонРосстата;
			Для Каждого Мкт Из МодульОтчета.Метаданные().Макеты Цикл 
				Если СтрНачинаетсяС(Мкт.Имя, "EF_")
					И СтрЗаканчиваетсяНа(Мкт.Имя, ПараметрыДляПроверки.ШаблонРосстата) Тогда 
					ИтоговоеИмяШаблона = Мкт.Имя;
				КонецЕсли;
			КонецЦикла;
			
			Если НЕ УниверсальныйОтчетСтатистики.СравнитьСтруктурыШаблонов(Выборка.Шаблон,
				МодульОтчета.ПолучитьМакет(ИтоговоеИмяШаблона)) Тогда 
				Продолжить;
			КонецЕсли;
			
			Если НЕ РегламентированнаяОтчетность.ПроверкаФорматаПоПериодуПрименения(ПараметрыДляПроверки, Выборка.Версия) Тогда 
				Продолжить;
			КонецЕсли;
			
			ЗаменитьФормат = Истина;
			Прервать;
		КонецЦикла;
		
		Если НЕ ЗаменитьФормат Тогда
			Возврат;
		КонецЕсли;
		
		ПараметрыДляПроверки.ВерсияШаблона = Выборка.Версия;
		
		УдалитьФайлы(ИтоговыйФайл);
		ИтоговыйФайл = ПолучитьИмяВременногоФайла("xml");
		Выборка.Шаблон.Получить().Записать(ИтоговыйФайл);
	Исключение
	КонецПопытки;
КонецПроцедуры

Процедура ПроверитьСпецификиНаЗаполненность(Контекст, Результат, Инд, ПараметрыДляПроверки)
	Для Каждого МнгСтр Из Контекст.ТаблицаМножественныхСтрок Цикл 
		Контекст.СтруктураПоискаСекцияСтрока.section = МнгСтр.section;
		Контекст.СтруктураПоискаСекцияСтрока.row = МнгСтр.row;
		МнгИнфо = Контекст.ТаблицаТиповСтрок.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока);
		Если МнгИнфо.Количество() <> 1 Тогда 
			ВызватьИсключение "";
		КонецЕсли;
		МнгИнфо = МнгИнфо[0];
		
		ОтбСтроки = Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока);
		ЕстьНезаполненныеСпецифики = Ложь;
		Для Каждого СтрокаДанных Из ОтбСтроки Цикл
			Для Итер = 1 По 3 Цикл
				Если МнгИнфо["s" + Итер + "_isPresent"]
					И (Не СтрокаДанных["s" + Итер + "_isPresent"] Или Не ЗначениеЗаполнено(СтрокаДанных["s" + Итер])) Тогда 
					
					ЕстьНезаполненныеСпецифики = Истина;
					Прервать;
				КонецЕсли;
			КонецЦикла;
			Если ЕстьНезаполненныеСпецифики Тогда 
				Прервать;
			КонецЕсли;
		КонецЦикла;
		
		Если ЕстьНезаполненныеСпецифики Тогда 
			Инд = Инд + 1;
			ИндСтр = Формат(Инд, "ЧГ=");
			Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
			Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
			Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "Вероятная ошибка");
			Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
			Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
			Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, "По строке " + Контекст.ОТЧ.ПривестиЗначение(МнгСтр.row) 
																				+ ", секция " + Контекст.ОТЧ.ПривестиЗначение(МнгСтр.section) 
																				+ " есть незаполненные ключевые поля");
			Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
		КонецЕсли;
	КонецЦикла;
КонецПроцедуры

Процедура ПроверитьСпецификиНаДублирование(Контекст, Результат, Инд, ПараметрыДляПроверки)
	ВариантыСпецифик = Новый Соответствие;
	Для Каждого МнгСтр Из Контекст.ТаблицаМножественныхСтрок Цикл 
		Контекст.СтруктураПоискаСекцияСтрока.section = МнгСтр.section;
		Контекст.СтруктураПоискаСекцияСтрока.row = МнгСтр.row;
		МнгИнфо = Контекст.ТаблицаТиповСтрок.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока);
		Если МнгИнфо.Количество() <> 1 Тогда 
			ВызватьИсключение "";
		КонецЕсли;
		МнгИнфо = МнгИнфо[0];
		КлючПрефикс = МнгСтр.section + "@" + МнгСтр.row + "@";
		
		ОтбСтроки = Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока);
		Для Каждого СтрокаДанных Из ОтбСтроки Цикл
			Ключ = КлючПрефикс;
			Для Итер = 1 По 3 Цикл
				Если МнгИнфо["s" + Итер + "_isPresent"] Тогда 
					Ключ = Ключ + СтрокаДанных["s" + Итер] + "@";
				КонецЕсли;
			КонецЦикла;
			Если ВариантыСпецифик[Ключ] = Неопределено Тогда 
				ВариантыСпецифик.Вставить(Ключ, 1);
			Иначе
				ВариантыСпецифик[Ключ] = 2;
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
	
	Для Каждого КЗ Из ВариантыСпецифик Цикл 
		Если КЗ.Значение = 1 Тогда 
			Продолжить;
		КонецЕсли;
		
		Разложение = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(КЗ.Ключ, "@");
		Пока Разложение.Количество() < 5 Цикл 
			Разложение.Добавить(Неопределено);
		КонецЦикла;
		ОписаниеОшибки = "По строке " + Контекст.ОТЧ.ПривестиЗначение(Разложение[1]) + ", секция " + Контекст.ОТЧ.ПривестиЗначение(Разложение[0])
						+ " в отчете имеются дублирующиеся строки с ключевыми полями "
						+ ?(ЗначениеЗаполнено(Разложение[2]), Разложение[2], "")
						+ ?(ЗначениеЗаполнено(Разложение[3]), "/" + Разложение[3], "")
						+ ?(ЗначениеЗаполнено(Разложение[4]), "/" + Разложение[4], "");
						
		Инд = Инд + 1;
		ИндСтр = Формат(Инд, "ЧГ=");
		Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
		Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
		Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "Вероятная ошибка");
		Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
		Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
		Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, ОписаниеОшибки);
		Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
	КонецЦикла;
КонецПроцедуры

Процедура ПроверитьЗаполненностьПолейТитула(Контекст, Результат, Инд, ПараметрыДляПроверки)
	Для Каждого КЗ Из Контекст.СписокПолейТитула Цикл 
		Если Не ЗначениеЗаполнено(Контекст.ЗначенияПолейТитула[КЗ.Ключ]) Тогда 
			Инд = Инд + 1;
			ИндСтр = Формат(Инд, "ЧГ=");
			Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
			Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
			Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "Обязательно к заполнению");
			Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
			Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
			Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, "Не заполнено поле титула: " + КЗ.Значение);
			Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
			
			Если Контекст.ДополнятьРасшифровкой И ПараметрыДляПроверки.РасшифровкаПолейТитула Тогда 
				ИмяРаздела = "ОбщаяИнформация";
				ИмяКлюча = КЗ.Ключ;
				
				ОтобранныеСтроки = Контекст.ДеревоВыгрузки.Строки.НайтиСтроки(Новый Структура("ЗначениеПоУмолчанию", КЗ.Ключ), Истина);
				Если ОтобранныеСтроки.Количество() = 1 Тогда 
					Если ЗначениеЗаполнено(ОтобранныеСтроки[0].Раздел) И ЗначениеЗаполнено(ОтобранныеСтроки[0].Ключ) Тогда 
						ИмяРаздела = ОтобранныеСтроки[0].Раздел;
						ИмяКлюча = ОтобранныеСтроки[0].Ключ;
					КонецЕсли;
				КонецЕсли;
				
				РасшифровкаПоказателя = Контекст.СписокРасшифровкиПоказателей.Добавить();
				РасшифровкаПоказателя.Номер = Инд;
				РасшифровкаПоказателя.ИмяПоказателя = ИмяКлюча;
				РасшифровкаПоказателя.ИмяСтраницы = ИмяРаздела;
				РасшифровкаПоказателя.ФормулаПоказателя = "Не заполнено обязательное поле: " + КЗ.Значение;
				РасшифровкаПоказателя.СсылкаНаДокументРО = Контекст.СсылкаНаДокументРО;
				РасшифровкаПоказателя.ЗначениеПоказателя = "<Пусто>";
				РасшифровкаПоказателя.ПредставлениеПоказателя = "";
				РасшифровкаПоказателя.Страница = ИмяРаздела;
				
				РасшифровкаФормула = Контекст.СписокРасшифровкиформулы.Добавить();
				РасшифровкаФормула.Номер = Инд;
				РасшифровкаФормула.ТекстовоеПредставлениеФормулы = "Не заполнено обязательное поле: " + КЗ.Значение;
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
КонецПроцедуры

Процедура ПроверитьГодОтчета(Контекст, Результат, Инд, ПараметрыДляПроверки)
	Если ЗначениеЗаполнено(Контекст.РеквизитОтчетаГод) 
		И Контекст.ГодыПрименимости.Количество() > 0
		И Контекст.ГодыПрименимости.НайтиПоЗначению(Контекст.РеквизитОтчетаГод) = Неопределено Тогда
		
		Инд = Инд + 1;
		ИндСтр = Формат(Инд, "ЧГ=");
		Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
		Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
		Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "Шаблон не применим для " + Контекст.РеквизитОтчетаГод + " года");
		Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
		Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
		Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, "Шаблон не применим для " + Контекст.РеквизитОтчетаГод + " года");
		Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
	КонецЕсли;
КонецПроцедуры

Процедура ПроверитьОбязательныеДляЗаполненияГрафы(Контекст, Результат, Инд, ПараметрыДляПроверки)
	Отбор = Новый Структура("section,row");
	ОтборДанных = Новый Структура("section,row,ДобавленоПриПроверке");
	ОтборДанных.ДобавленоПриПроверке = Ложь;
	ОтборСпецифик = Новый Структура("Секция,Колонка");
	
	Для Каждого Стр Из Контекст.ОбязательныеДляВводаГрафы Цикл 
		ФлагОшибки = Ложь;
		Отбор.section = Стр.Секция;
		Отбор.row = Стр.Строка;
		Если Контекст.ТаблицаМножественныхСтрок.НайтиСтроки(Отбор).Количество() > 0 Тогда 
			ЗаполнитьЗначенияСвойств(ОтборДанных, Отбор);
			Если Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(ОтборДанных).Количество() > 0 Тогда 
				ОтборСпецифик.Секция = Стр.Секция;
				ОтборСпецифик.Колонка = Стр.Колонка;
				Специфики = Контекст.КолонкиСпецифик.НайтиСтроки(ОтборСпецифик);
				Если Специфики.Количество() > 0 Тогда 
					ИмяКол = Специфики[0].Специфика + "_isPresent";
				Иначе
					ИмяКол = "col" + Стр.Колонка + "_isPresent";
				КонецЕсли;
				Отбор.Вставить(ИмяКол, Истина);
				ОтобранныеСтроки = Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Отбор);
				Если ОтобранныеСтроки.Количество() = 0 Или Не ЗначениеЗаполнено(ОтобранныеСтроки[0][ИмяКол]) Тогда 
					ФлагОшибки = Истина;
				КонецЕсли;
				Отбор.Удалить(ИмяКол);
			КонецЕсли;
		Иначе
			Если Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Отбор).Количество() = 0 Тогда 
				ФлагОшибки = Истина;
			Иначе
				ИмяКол = "col" + Стр.Колонка + "_isPresent";
				Отбор.Вставить(ИмяКол, Истина);
				Если Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Отбор).Количество() = 0 Тогда 
					ФлагОшибки = Истина;
				КонецЕсли;
				Отбор.Удалить(ИмяКол);
			КонецЕсли;
		КонецЕсли;
		
		Если ФлагОшибки Тогда 
			Инд = Инд + 1;
			ИндСтр = Формат(Инд, "ЧГ=");
			Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
			Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
			Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "Поле необходимо заполнить");
			Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
			Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
			Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, 
				"Не заполнено обязательное поле: " + Контекст.ИменаСекций[Стр.Секция]
				+ " строка:" + Формат(Контекст.ОТЧ.ПривестиЗначение(Отбор.row), "ЧГ=")
				+ " колонка:" + Формат(Контекст.ОТЧ.ПривестиЗначение(Стр.Колонка), "ЧГ="));
			Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
			
			Если Контекст.ДополнятьРасшифровкой Тогда
				Секция = Контекст.ОТЧ.ПривестиЗначение(Стр.Секция);
				Строка = Контекст.ОТЧ.ПривестиЗначение(Стр.Строка);
				Колонка = Контекст.ОТЧ.ПривестиЗначение(Стр.Колонка);
				
				Инфо = ИнформацияПоПолю(Контекст, Секция, Строка, Колонка);
				Если Инфо <> Неопределено Тогда
					РасшифровкаПоказателя = Контекст.СписокРасшифровкиПоказателей.Добавить();
					РасшифровкаПоказателя.Номер = Инд;
					РасшифровкаПоказателя.ИмяПоказателя = Инфо.Ключ;
					РасшифровкаПоказателя.ИмяСтраницы = Инфо.Раздел;
					РасшифровкаПоказателя.ФормулаПоказателя = "Не заполнено обязательное поле";
					РасшифровкаПоказателя.СсылкаНаДокументРО = Контекст.СсылкаНаДокументРО;
					РасшифровкаПоказателя.ЗначениеПоказателя = "";
					РасшифровкаПоказателя.ПредставлениеПоказателя = Инфо.Представление;
					РасшифровкаПоказателя.Страница = Инфо.Раздел;
					
					РасшифровкаФормула = Контекст.СписокРасшифровкиформулы.Добавить();
					РасшифровкаФормула.Номер = Инд;
					РасшифровкаФормула.ТекстовоеПредставлениеФормулы = "Не заполнено обязательное поле";
				КонецЕсли;
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
КонецПроцедуры

Процедура ДобавитьПустыеОбязательныеМножественныеСтроки(Контекст)
	СтруктураСекцияСтрока = Новый Структура("Секция, Строка");
	Для Каждого Стр Из Контекст.ТаблицаМножественныхСтрок Цикл 
		СтруктураСекцияСтрока.Секция = Стр.section;
		СтруктураСекцияСтрока.Строка = Стр.row;
		ОбязательныеДляВводаКолонки = Контекст.ОбязательныеДляВводаГрафы.НайтиСтроки(СтруктураСекцияСтрока);
		Если ОбязательныеДляВводаКолонки.Количество() = 0 Тогда 
			Продолжить;
		КонецЕсли;
		ЗаполнитьЗначенияСвойств(Контекст.СтруктураПоискаСекцияСтрока, Стр);
		Если Контекст.ОсновнойПериод_ДанныеОтчета_МножественныеСтроки.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока).Количество() <> 0
			Или Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока).Количество() <> 0 Тогда 
			Продолжить;
		КонецЕсли;
		
		ТолькоСпецифики = Истина;
		Для Каждого Кол Из ОбязательныеДляВводаКолонки Цикл
			ТолькоСпецифики = ТолькоСпецифики И (Контекст.КолонкиСпецифики.НайтиСтроки(Новый Структура("Секция, Колонка", Кол.Секция, Кол.Колонка)).Количество() > 0);
		КонецЦикла;
		Если ТолькоСпецифики Тогда 
			Продолжить;
		КонецЕсли;
		
		НовСтр = Контекст.ОсновнойПериод_ДанныеОтчета_МножественныеСтроки.Добавить();
		ЗаполнитьЗначенияСвойств(НовСтр, Контекст.СтруктураПоискаСекцияСтрока);
		НовСтр.ДобавленоПриПроверке = Истина;
		НовСтр = Контекст.ОсновнойПериод_ДанныеОтчета.Добавить();
		ЗаполнитьЗначенияСвойств(НовСтр, Контекст.СтруктураПоискаСекцияСтрока);
		НовСтр.ДобавленоПриПроверке = Истина;
	КонецЦикла;
КонецПроцедуры

Процедура ПроверкаДиапазонов(Контекст, Результат, ПараметрыДляПроверки, Инд)
	Для Каждого Стр Из Контекст.ДиапазоныЗначений Цикл 
		СтруктураПоиска = Новый Структура;
		СтруктураПоиска.Вставить("section", Стр.Секция);
		СтруктураПоиска.Вставить("row", Стр.Строка);
		СтруктураПоиска.Вставить("col" + Стр.Колонка + "_isPresent", Истина);
		Для Каждого ОтбСтр Из Контекст.ОсновнойПериод_ДанныеОтчета.НайтиСтроки(СтруктураПоиска) Цикл
			ЗначениеВСтроке = ОтбСтр["col" + Стр.Колонка];
			Если ЗначениеВСтроке <> Неопределено И Не ЧислоВходитВДиапазон(Контекст, ЗначениеВСтроке, Стр.Диапазон) Тогда 
				ОписаниеНарушения = "Значение в разделе " + Контекст.ОТЧ.ПривестиЗначение(Стр.Секция) + " строке " 
					+ Контекст.ОТЧ.ПривестиЗначение(Стр.Строка) + " колонке "
					+ Контекст.ОТЧ.ПривестиЗначение(Стр.Колонка) + " должно входить в диапазон " + Стр.Диапазон;
				
				Инд = Инд + 1;
				ИндСтр = Формат(Инд, "ЧГ=");
				Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
				Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
				Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "Значение " + ЗначениеВСтроке + " не попадает в диапазон");
				Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
				Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
				Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, ОписаниеНарушения);
				Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
				
				Если Контекст.ДополнятьРасшифровкой Тогда
					Секция = Контекст.ОТЧ.ПривестиЗначение(Стр.Секция);
					Строка = Контекст.ОТЧ.ПривестиЗначение(Стр.Строка);
					Колонка = Контекст.ОТЧ.ПривестиЗначение(Стр.Колонка);
					
					Инфо = ИнформацияПоПолю(Контекст, Секция, Строка, Колонка);
					Если Инфо <> Неопределено Тогда
						ОписаниеФормулы = "Значение " + ЗначениеВСтроке + " не входит в диапазон " + Стр.Диапазон;
						РасшифровкаПоказателя = Контекст.СписокРасшифровкиПоказателей.Добавить();
						РасшифровкаПоказателя.Номер = Инд;
						РасшифровкаПоказателя.ИмяПоказателя = Инфо.Ключ;
						РасшифровкаПоказателя.ИмяСтраницы = Инфо.Раздел;
						РасшифровкаПоказателя.ФормулаПоказателя = ОписаниеФормулы;
						РасшифровкаПоказателя.СсылкаНаДокументРО = Контекст.СсылкаНаДокументРО;
						РасшифровкаПоказателя.ЗначениеПоказателя = "";
						РасшифровкаПоказателя.ПредставлениеПоказателя = Инфо.Представление;
						РасшифровкаПоказателя.Страница = Инфо.Раздел;
						
						РасшифровкаФормула = Контекст.СписокРасшифровкиформулы.Добавить();
						РасшифровкаФормула.Номер = Инд;
						РасшифровкаФормула.ТекстовоеПредставлениеФормулы = ОписаниеФормулы;
					КонецЕсли;
				КонецЕсли;
			КонецЕсли;
		КонецЦикла;
	КонецЦикла;
КонецПроцедуры

Процедура СформироватьИнформациюОПоляхОтчета(Контекст)
	ИнформацияОПоляхОтчета = Новый Соответствие;
	ТекущаяСекция = Неопределено;
	ТекущаяСтрока = Неопределено;
	
	Секции = Контекст.ДеревоВыгрузки.Строки.НайтиСтроки(Новый Структура("Код", "section"), Истина);
	Для Каждого Секция Из Секции Цикл 
		Код = Секция.Строки.НайтиСтроки(Новый Структура("Код", "code"));
		Если Код.Количество() <> 1 Тогда 
			Продолжить;
		КонецЕсли;
		
		ТекущаяСекция = Контекст.ОТЧ.ПривестиЗначение(Код[0].ЗначениеПоУмолчанию);
		ИнформацияОПоляхОтчета.Вставить(ТекущаяСекция, Новый Соответствие);
		Строки = Секция.Строки.НайтиСтроки(Новый Структура("Код", "row"));
		Для Каждого Строка Из Строки Цикл
			Код = Строка.Строки.НайтиСтроки(Новый Структура("Код", "code"));
			Если Код.Количество() <> 1 Тогда 
				Продолжить;
			КонецЕсли;
			
			ТекущаяСтрока = Контекст.ОТЧ.ПривестиЗначение(Код[0].ЗначениеПоУмолчанию);
			ИнформацияОПоляхОтчета[ТекущаяСекция].Вставить(ТекущаяСтрока, Новый Соответствие);
			Колонки = Строка.Строки.НайтиСтроки(Новый Структура("Код", "col"));
			Для Каждого Колонка Из Колонки Цикл 
				Если Не ЗначениеЗаполнено(Колонка.Ключ) Тогда 
					Продолжить;
				КонецЕсли;
				Код = Колонка.Строки.НайтиСтроки(Новый Структура("Код", "code"));
				Если Код.Количество() <> 1 Тогда 
					Продолжить;
				КонецЕсли;
				
				ТекущаяКолонка = Контекст.ОТЧ.ПривестиЗначение(Код[0].ЗначениеПоУмолчанию);
				ИнформацияПоКолонке = Новый Структура("Ключ, Раздел, Представление",
					Колонка.Ключ, Колонка.Раздел, СокрЛП("" + Строка.Представление + ", " + Колонка.Представление));
				ИнформацияОПоляхОтчета[ТекущаяСекция][ТекущаяСтрока].Вставить(ТекущаяКолонка, ИнформацияПоКолонке);
			КонецЦикла;
		КонецЦикла;
	КонецЦикла;
	
	Контекст.Вставить("ИнформацияОПоляхОтчета", ИнформацияОПоляхОтчета);
КонецПроцедуры

Функция ИнформацияПоПолю(Контекст, Секция, Строка, Колонка)
	Если Контекст.ИнформацияОПоляхОтчета[Секция] = Неопределено
		Или Контекст.ИнформацияОПоляхОтчета[Секция][Строка] = Неопределено Тогда 
		Возврат Неопределено;
	КонецЕсли;
	Возврат Контекст.ИнформацияОПоляхОтчета[Секция][Строка][Колонка];
КонецФункции

Функция ЧисловойСписокИзОписания(Контекст, Аргумент)
	Результат = Новый Массив;
	ОдноЧисло = Контекст.ОТЧ.ПривестиЗначение(Аргумент);
	Если ЗначениеЗаполнено(ОдноЧисло) И Цел(ОдноЧисло) = ОдноЧисло Тогда 
		Результат.Добавить(ОдноЧисло);
		Возврат Результат;
	КонецЕсли;
	
	Если Аргумент = "*" Тогда 
		Результат.Добавить(Аргумент);
		Возврат Результат;
	КонецЕсли;
	
	Для Каждого Элт Из СтрРазделить(Аргумент, ",") Цикл 
		Если СтрЧислоВхождений(Элт, "-") = 1 Тогда 
			Разделение = СтрРазделить(Элт, "-");
			НачалоДиапазона = Цел(Контекст.ОТЧ.ПривестиЗначение(Разделение[0]));
			КонецДиапазона = Цел(Контекст.ОТЧ.ПривестиЗначение(Разделение[1]));
			Если ЗначениеЗаполнено(НачалоДиапазона) И Цел(НачалоДиапазона) = НачалоДиапазона
				И ЗначениеЗаполнено(КонецДиапазона) И Цел(КонецДиапазона) = КонецДиапазона
				И НачалоДиапазона <= КонецДиапазона Тогда 
				Для Итер = НачалоДиапазона По КонецДиапазона Цикл 
					Результат.Добавить(Итер);
				КонецЦикла;
			КонецЕсли;
		Иначе
			ОдноЧисло = Контекст.ОТЧ.ПривестиЗначение(Элт);
			Если ЗначениеЗаполнено(ОдноЧисло) Тогда 
				Результат.Добавить(ОдноЧисло);
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	
	Возврат Результат;
КонецФункции

Процедура РасшифровкаПоОшибке(Контекст, Условие, ТекстовоеПредставление, Инд)
	Если Контекст.ДополнятьРасшифровкой Тогда 
		ПараметрыПеребора = Новый Массив;
		ПараметрыПеребора.Добавить(ПолучитьВсеАргументыКромеСуммированийВВыражении(Контекст, Условие));
		ПараметрыПеребора.Добавить(ПолучитьВсеАргументыСуммированияВВыражении(Условие));
		Для Каждого Элт Из ПараметрыПеребора Цикл 
			Для Каждого КЗ Из Элт Цикл 
				Секция = Контекст.ОТЧ.ПривестиЗначение(ПолучитьАргументСкобки(КЗ.Ключ, 1));
				Если Не ЗначениеЗаполнено(Секция) Тогда 
					Продолжить;
				КонецЕсли;
				
				Контекст.СтруктураПоискаСекцияСтрока.section = Формат(Секция, "ЧЦ=3; ЧВН=; ЧГ=");
				Если СтрЧислоВхождений(КЗ.Ключ, "[") <> 3 Тогда 
					Строка = Контекст.ОТЧ.ПривестиЗначение(ПолучитьАргументСкобки(КЗ.Ключ, 2));
					Если Не ЗначениеЗаполнено(Строка) Тогда 
						Продолжить;
					КонецЕсли;
					
					Контекст.СтруктураПоискаСекцияСтрока.row = Формат(Строка, "ЧЦ=5; ЧВН=; ЧГ=");
					Если Контекст.ТаблицаМножественныхСтрок.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока).Количество() > 0 Тогда 
						Продолжить;
					КонецЕсли;
				КонецЕсли;
				
				Строки = ЧисловойСписокИзОписания(Контекст, ПолучитьАргументСкобки(КЗ.Ключ, 2));
				Колонки = ЧисловойСписокИзОписания(Контекст, ПолучитьАргументСкобки(КЗ.Ключ, 3));
				Строка = 0;
				Колонка = 0;
				Для Каждого Стр Из Строки Цикл 
					Если Контекст.ОшибочнаяСтрока <> Неопределено И Стр <> Контекст.ОшибочнаяСтрока И Стр <> "*" Тогда 
						Продолжить;
					КонецЕсли;
					
					Для Каждого Кол Из Колонки Цикл
						Если Кол = "*" Тогда 
							Кол = Контекст.ОшибочнаяГрафа;
						КонецЕсли;
						
						Если Контекст.ОшибочнаяГрафа <> Неопределено И Кол <> Контекст.ОшибочнаяГрафа Тогда 
							Продолжить;
						КонецЕсли;
						
						Инфо = ИнформацияПоПолю(Контекст, Секция, Стр, Кол);
						Если Инфо = Неопределено Тогда 
							Продолжить;
						КонецЕсли;
						
						Контекст.СтруктураПоискаСекцияСтрока.row = Формат(Стр, "ЧЦ=5; ЧВН=; ЧГ=");
						Если Контекст.ТаблицаМножественныхСтрок.НайтиСтроки(Контекст.СтруктураПоискаСекцияСтрока).Количество() > 0 Тогда 
							Продолжить;
						КонецЕсли;
						
						Строка = Стр;
						Колонка = Кол;
						Прервать;
					КонецЦикла;
					
					Если ЗначениеЗаполнено(Строка) Тогда 
						Прервать;
					КонецЕсли;
				КонецЦикла;
				
				Если Не ЗначениеЗаполнено(Строка) Или Не ЗначениеЗаполнено(Колонка) Тогда 
					Продолжить;
				КонецЕсли;
				
				РасшифровкаПоказателя = Контекст.СписокРасшифровкиПоказателей.Добавить();
				РасшифровкаПоказателя.Номер = Инд;
				РасшифровкаПоказателя.ИмяПоказателя = Инфо.Ключ;
				РасшифровкаПоказателя.ИмяСтраницы = Инфо.Раздел;
				РасшифровкаПоказателя.ФормулаПоказателя = ТекстовоеПредставление + Символы.ПС + Условие;
				РасшифровкаПоказателя.СсылкаНаДокументРО = Контекст.СсылкаНаДокументРО;
				РасшифровкаПоказателя.ЗначениеПоказателя = "";
				РасшифровкаПоказателя.ПредставлениеПоказателя = Инфо.Представление;
				РасшифровкаПоказателя.Страница = Инфо.Раздел;
				
				РасшифровкаФормула = Контекст.СписокРасшифровкиформулы.Добавить();
				РасшифровкаФормула.Номер = Инд;
				РасшифровкаФормула.ТекстовоеПредставлениеФормулы = ТекстовоеПредставление + Символы.ПС + Условие;
				Возврат;
			КонецЦикла;
		КонецЦикла;
	КонецЕсли;
КонецПроцедуры

Функция ПроверитьКонтрольныеСоотношенияВОтчете(ПараметрыДляПроверки)
	МодульОтчета = РегламентированнаяОтчетностьВызовСервера.ОбъектОтчета(ПараметрыДляПроверки.ИмяФормы);
	Если Не ЗначениеЗаполнено(ПараметрыДляПроверки.ТекстВыгрузки)
		И ПараметрыДляПроверки.ДоступноФормированиеТекстаВыгрузкиБезКонтекстаФормы Тогда
		
		РазложениеИмени = СтроковыеФункцииКлиентСервер.РазложитьСтрокуВМассивПодстрок(ПараметрыДляПроверки.ИмяФормы, ".");
		Если СтрНайти(ПараметрыДляПроверки.ИмяФормы, "Внешний") > 0 Тогда 
			ПараметрыДляПроверки.ТекстВыгрузки = МодульОтчета.ТекстВыгрузкиОтчетаСтатистики(ПараметрыДляПроверки.Ссылка, РазложениеИмени[3]);
		Иначе
			ПараметрыДляПроверки.ТекстВыгрузки = ОбщегоНазначения.ОбщийМодуль(
				"Отчеты." + РазложениеИмени[1]).ТекстВыгрузкиОтчетаСтатистики(ПараметрыДляПроверки.Ссылка, РазложениеИмени[3]);
		КонецЕсли;
	КонецЕсли;
	
	ИмяВременногоКаталога = "";
	Если ТипЗнч(ПараметрыДляПроверки.ШаблонРосстата) = Тип("Строка") Тогда
		ИтоговоеИмяШаблона = ПараметрыДляПроверки.ШаблонРосстата;
		Для Каждого Мкт Из МодульОтчета.Метаданные().Макеты Цикл 
			Если СтрНачинаетсяС(Мкт.Имя, "EF_")
				И СтрЗаканчиваетсяНа(Мкт.Имя, ПараметрыДляПроверки.ШаблонРосстата) Тогда 
				ИтоговоеИмяШаблона = Мкт.Имя;
			КонецЕсли;
		КонецЦикла;
		
		ИмяВременногоКаталога = ПолучитьИмяВременногоФайла() + ПолучитьРазделительПути();
		СоздатьКаталог(ИмяВременногоКаталога);
		ЧтениеZip = Новый ЧтениеZipФайла(Новый ПотокВПамяти(ПолучитьБуферДвоичныхДанныхИзДвоичныхДанных(МодульОтчета.ПолучитьМакет(ИтоговоеИмяШаблона))));
		ЧтениеZip.Извлечь(ЧтениеZip.Элементы[0], ИмяВременногоКаталога, РежимВосстановленияПутейФайловZIP.НеВосстанавливать);
		ИтоговыйФайл = ИмяВременногоКаталога + ЧтениеZip.Элементы[0].Имя;
		ЧтениеZip.Закрыть();
	ИначеЕсли ТипЗнч(ПараметрыДляПроверки.ШаблонРосстата) = Тип("ДвоичныеДанные") Тогда 
		ДанныеШаблона = ПараметрыДляПроверки.ШаблонРосстата;
		ИтоговыйФайл = ПолучитьИмяВременногоФайла("xml");
		ДанныеШаблона.Записать(ИтоговыйФайл);
	КонецЕсли;
	
	ВыгрузкаПредидущегоПериода = Неопределено;
	ОбщегоНазначения.ЗначениеРеквизитаОбъекта(
		ПараметрыДляПроверки.Ссылка, "ДанныеОтчета").Получить().Свойство("ВыгрузкаПредидущегоПериода", ВыгрузкаПредидущегоПериода);
	ИзменитьВерсиюФорматаСПроверкойСовместимостиШаблонов(МодульОтчета, ПараметрыДляПроверки, ИтоговыйФайл);
	Контекст = СформироватьКонтекстДляПроверок(ИтоговыйФайл, ПараметрыДляПроверки.ТекстВыгрузки, ВыгрузкаПредидущегоПериода);
	УдалитьФайлы(ИтоговыйФайл);
	Если ЗначениеЗаполнено(ИмяВременногоКаталога) Тогда 
		УдалитьФайлы(ИмяВременногоКаталога);
	КонецЕсли;
	
	Контекст.Вставить("ДополнятьРасшифровкой", ПараметрыДляПроверки.ДополнятьРасшифровкой);
	Если Контекст.ДополнятьРасшифровкой Тогда 
		Контекст.Вставить("СписокРасшифровкиПоказателей", РегламентированнаяОтчетность.СоздатьСписокРасшифровкиПоказателей());
		Контекст.Вставить("СписокРасшифровкиформулы", РегламентированнаяОтчетность.СоздатьСписокРасшифровкиформулы());
		Контекст.Вставить("ДеревоВыгрузки", ПараметрыДляПроверки.ДеревоВыгрузки);
		Контекст.Вставить("СсылкаНаДокументРО", ПараметрыДляПроверки.СсылкаНаДокументРО);
		СформироватьИнформациюОПоляхОтчета(Контекст);
	КонецЕсли;
	
	Результат = Новый Структура;
	Результат.Вставить("КоличествоНайденныхБезОшибок", 0);
	Результат.Вставить("КоличествоНайденныхОшибок", 0);
	Результат.Вставить("ЭтоАвтопроверкаСтатистики", Истина);
	Результат.Вставить("СтруктураОшибок", Новый Структура);
	
	Инд = 0;
	Если Контекст.ПроверкаОтчетаНаПустоту И Контекст.ОсновнойПериод_ДанныеОтчета.Количество() = 0 Тогда 
		Инд = Инд + 1;
		ИндСтр = Формат(Инд, "ЧГ=");
		Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
		Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
		Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "");
		Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
		Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, "");
		Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, "Отчет не может быть пустым");
		Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
	КонецЕсли;
	
	Попытка
		ПроверитьЗаполненностьПолейТитула(Контекст, Результат, Инд, ПараметрыДляПроверки);
		ПроверитьГодОтчета(Контекст, Результат, Инд, ПараметрыДляПроверки);
		ПроверкаДиапазонов(Контекст, Результат, ПараметрыДляПроверки, Инд);
		Если Контекст.ПроверкаОтчетаНаПустоту Или Контекст.ОсновнойПериод_ДанныеОтчета.Количество() <> 0 Тогда 
			ПроверитьСпецификиНаЗаполненность(Контекст, Результат, Инд, ПараметрыДляПроверки);
			ПроверитьСпецификиНаДублирование(Контекст, Результат, Инд, ПараметрыДляПроверки);
			ДобавитьПустыеОбязательныеМножественныеСтроки(Контекст);
			ПроверитьОбязательныеДляЗаполненияГрафы(Контекст, Результат, Инд, ПараметрыДляПроверки);
		КонецЕсли;
	Исключение
	КонецПопытки;
	
	Для Каждого Стр Из Контекст.ТаблицаКонтрольныхСоотношений Цикл
		Если ЗначениеЗаполнено(Стр.uuid) И Контекст.ОбработанныеКС[Стр.uuid] = Истина Тогда 
			Продолжить;
		КонецЕсли;
		
		ПравилоФормула = Стр.rule;
		Контекст.ПолучитьВсеАргументыВыражения.Очистить();
		Контекст.ПолучитьВсеАргументыКромеСуммированийВВыражении.Очистить();
		Инд = Инд + 1;
		ИндСтр = Формат(Инд, "ЧГ=");
		Результат.СтруктураОшибок.Вставить("Описание" + ИндСтр, "");
		Попытка
			Если ПроверитьКонтрольноеСоотношение(Стр, Контекст) Тогда 
				Результат.КоличествоНайденныхБезОшибок = Результат.КоличествоНайденныхБезОшибок + 1;
				Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, "");
				Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
				Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, ПравилоФормула);
				Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, Стр.name);
				Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Ложь);
			Иначе
				Если ЗначениеЗаполнено(Стр.uuid) Тогда 
					Контекст.ОбработанныеКС.Вставить(Стр.uuid, Истина);
				КонецЕсли;
				Результат.КоличествоНайденныхОшибок = Результат.КоличествоНайденныхОшибок + 1;
				Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, ?(Контекст.ОТЧ.ПривестиЗначение(Стр.tip) = 1, "", "Контролировать необязательно"));
				Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
				Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, ПравилоФормула);
				Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, Стр.name);
				Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Истина);
				РасшифровкаПоОшибке(Контекст, ПравилоФормула, Стр.name, Инд);
			КонецЕсли;
		Исключение
			ОписаниеСтр = "Не удалось вычислить условие" + ?(Контекст.ОТЧ.ПривестиЗначение(Стр.tip) = 1, "", Символы.ПС + "Контролировать необязательно");
			Результат.КоличествоНайденныхБезОшибок = Результат.КоличествоНайденныхБезОшибок + 1;
			Результат.СтруктураОшибок.Вставить("Комментарий" + ИндСтр, ОписаниеСтр);
			Результат.СтруктураОшибок.Вставить("Нарушение" + ИндСтр, "XML шаблон от " + ПараметрыДляПроверки.ВерсияШаблона);
			Результат.СтруктураОшибок.Вставить("Расшифровка" + ИндСтр, ПравилоФормула);
			Результат.СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + ИндСтр, Стр.name);
			Результат.СтруктураОшибок.Вставить("ЭтоОшибка" + ИндСтр, Ложь);
		КонецПопытки;
	КонецЦикла;
	
	Если Контекст.ДополнятьРасшифровкой Тогда 
		Результат.Вставить("СписокРасшифровкиПоказателей", 
			ПоместитьВоВременноеХранилище(Контекст.СписокРасшифровкиПоказателей, ПараметрыДляПроверки.СписокРасшифровкиПоказателей));
		Результат.Вставить("СписокРасшифровкиформулы", 
			ПоместитьВоВременноеХранилище(Контекст.СписокРасшифровкиформулы, ПараметрыДляПроверки.СписокРасшифровкиформулы));
	КонецЕсли;
	
	Возврат Результат;
КонецФункции

Функция ДатаВерсии(СтрДата)
	
	Разделители = "-.,/:";
	
	ДлинаСтроки = СтрДлина(СтрДата);
	
	МассивПолей = Новый Массив;
	МассивПолей.Добавить("");
	
	Для НС = 1 По ДлинаСтроки Цикл
		Сим = Сред(СтрДата, НС, 1);
		Если СтрНайти(Разделители, Сим) > 0 Тогда
			МассивПолей.Добавить("");
		ИначеЕсли СтрНайти("0123456789", Сим) > 0 Тогда
			МассивПолей[МассивПолей.ВГраница()] = МассивПолей[МассивПолей.ВГраница()] + Сим;
		КонецЕсли;
	КонецЦикла;
	
	День  = Макс(1, Число("0" + СокрЛП(МассивПолей[0])));
	Месяц = Макс(1, Число("0" + ?(МассивПолей.ВГраница() < 1, "1", СокрЛП(МассивПолей[1]))));
	Год   = Макс(1, Число("0" + ?(МассивПолей.ВГраница() < 2, "1", СокрЛП(МассивПолей[2]))));
	
	Возврат Дата(Год, Месяц, День);
	
КонецФункции

