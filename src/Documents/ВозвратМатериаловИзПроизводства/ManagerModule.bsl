	//-- НЕ УТ
	|		И (&Подразделение = КорАналитика.МестоХранения)
	|
	|	ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.АналитикаУчетаНоменклатуры КАК КорАналитикаБезНазначения
	|	ПО ТаблицаТовары.Номенклатура = КорАналитикаБезНазначения.Номенклатура
	|		И ТаблицаТовары.Характеристика = КорАналитикаБезНазначения.Характеристика
	|		И ЗНАЧЕНИЕ(Справочник.Назначения.ПустаяСсылка) = КорАналитикаБезНазначения.Назначение
	|		И ВЫБОР
	|			КОГДА ТаблицаТовары.СтатусУказанияСерийОтправитель = 14
	|				ТОГДА ТаблицаТовары.Серия = КорАналитикаБезНазначения.Серия
	|					ИНАЧЕ &ПустаяСерия = КорАналитикаБезНазначения.Серия
	|		КОНЕЦ
	//++ НЕ УТ
	|		И ЗНАЧЕНИЕ(Справочник.СтатьиКалькуляции.ПустаяСсылка) = КорАналитикаБезНазначения.СтатьяКалькуляции
	//-- НЕ УТ
	|		И (&Подразделение = КорАналитикаБезНазначения.МестоХранения)
	|";
	
	ТекстыЗапроса.Добавить(ТекстЗапроса, ИмяРегистра);
	
	Возврат ТекстЗапроса;
	
КонецФункции

Функция ТекстЗапросаВтТаблицаСерииТоваровОтправитель(Запрос, ТекстыЗапроса)
	
	ИмяРегистра = "ВтТаблицаСерииТоваровОтправитель";
	
	ТекстЗапроса =
	"ВЫБРАТЬ
	|	ТаблицаТовары.Номенклатура                      КАК Номенклатура,
	|	ТаблицаТовары.Характеристика                    КАК Характеристика,
