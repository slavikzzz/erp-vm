    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>ВЫБРАТЬ
	ЗаказПереработчику2_5.Ссылка КАК Ссылка
ПОМЕСТИТЬ ТаблицаЗаказов
ИЗ
	Документ.ЗаказПереработчику2_5 КАК ЗаказПереработчику2_5
ГДЕ
	ЗаказПереработчику2_5.Договор = &amp;Договор
	И ЗаказПереработчику2_5.Проведен

ИНДЕКСИРОВАТЬ ПО
	Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗЛИЧНЫЕ
	ДокЗаказПереработчику.Ссылка КАК Ссылка,
	ДокЗаказПереработчику.Организация КАК Организация,
	ДокЗаказПереработчику.Организация КАК ОрганизацияДляОтбора,
	ДокЗаказПереработчику.Партнер КАК Партнер,
	ДокЗаказПереработчику.Номер КАК НомерРаспоряжения,
	ДокЗаказПереработчику.Дата КАК ДатаРаспоряжения,
	ДокЗаказПереработчику.Статус КАК Статус,
	ДокЗаказПереработчику.Приоритет КАК Приоритет,
	ДокЗаказПереработчику.Подразделение КАК Подразделение,
	ДокЗаказПереработчику.ПометкаУдаления КАК ПометкаУдаления,
	ДокЗаказПереработчику.НаправлениеДеятельности КАК НаправлениеДеятельности
ИЗ
	(ВЫБРАТЬ
		ТаблицаЗаказовКОформлению.ЗаказПоставщику КАК Распоряжение
	ИЗ
		(ВЫБРАТЬ
			ЗаказыПоставщикам.ЗаказПоставщику КАК ЗаказПоставщику,
			ЗаказыПоставщикам.Номенклатура КАК Номенклатура,
			ЗаказыПоставщикам.Характеристика КАК Характеристика,
			ЗаказыПоставщикам.Склад КАК Склад,
			ЗаказыПоставщикам.КОформлениюОстаток КАК КОформлению
		ИЗ
			РегистрНакопления.ЗаказыПоставщикам.Остатки(, ЗаказПоставщику В
				(ВЫБРАТЬ
					ТаблицаЗаказов.Ссылка КАК Ссылка
				ИЗ
					ТаблицаЗаказов КАК ТаблицаЗаказов)
			И КодСтроки &lt;> 0) КАК ЗаказыПоставщикам

		ОБЪЕДИНИТЬ ВСЕ

		ВЫБРАТЬ
			ЗаказыПоставщикам.ЗаказПоставщику КАК ЗаказПоставщику,
			ЗаказыПоставщикам.Номенклатура КАК Номенклатура,
			ЗаказыПоставщикам.Характеристика КАК Характеристика,
			ЗаказыПоставщикам.Склад КАК Склад,
			ЗаказыПоставщикам.КОформлению КАК КОформлению
		ИЗ
			РегистрНакопления.ЗаказыПоставщикам КАК ЗаказыПоставщикам
		ГДЕ
			ЗаказыПоставщикам.Активность
			И ЗаказыПоставщикам.ВидДвижения = ЗНАЧЕНИЕ(ВидДВиженияНакопления.Расход)
			И ЗаказыПоставщикам.Регистратор = &amp;Регистратор
			И ЗаказыПоставщикам.ЗаказПоставщику В
				(ВЫБРАТЬ
					ТаблицаЗаказов.Ссылка КАК Ссылка
				ИЗ
					ТаблицаЗаказов КАК ТаблицаЗаказов)
			И ЗаказыПоставщикам.КодСтроки &lt;> 0) КАК ТаблицаЗаказовКОформлению
	СГРУППИРОВАТЬ ПО
		ТаблицаЗаказовКОформлению.ЗаказПоставщику,
		ТаблицаЗаказовКОформлению.Номенклатура,
		ТаблицаЗаказовКОформлению.Характеристика,
		ТаблицаЗаказовКОформлению.Склад
	ИМЕЮЩИЕ
		СУММА(ТаблицаЗаказовКОформлению.КОформлению) > 0

	ОБЪЕДИНИТЬ ВСЕ

	ВЫБРАТЬ
		ТаблицаЗаказовСверхЗаказа.ЗаказПоставщику КАК Распоряжение
	ИЗ
		(ВЫБРАТЬ
			ЗаказыПоставщикам.ЗаказПоставщику КАК ЗаказПоставщику,
			ЗаказыПоставщикам.Номенклатура КАК Номенклатура,
			ЗаказыПоставщикам.Характеристика КАК Характеристика,
			ЗаказыПоставщикам.КодСтроки КАК КодСтроки,
			ЗаказыПоставщикам.КПоступлениюПриход - ЗаказыПоставщикам.КОформлениюРасход КАК КОформлению
		ИЗ
			РегистрНакопления.ЗаказыПоставщикам.Обороты(,,, ЗаказПоставщику В
				(ВЫБРАТЬ
					ТаблицаЗаказов.Ссылка КАК Ссылка
				ИЗ
					ТаблицаЗаказов КАК ТаблицаЗаказов)
			И КодСтроки = 0) КАК ЗаказыПоставщикам

		ОБЪЕДИНИТЬ ВСЕ

		ВЫБРАТЬ
			ЗаказыПоставщикам.ЗаказПоставщику КАК ЗаказПоставщику,
			ЗаказыПоставщикам.Номенклатура КАК Номенклатура,
			ЗаказыПоставщикам.Характеристика КАК Характеристика,
			ЗаказыПоставщикам.КодСтроки КАК КодСтроки,
			ЗаказыПоставщикам.КОформлению КАК КОформлению
		ИЗ
			РегистрНакопления.ЗаказыПоставщикам КАК ЗаказыПоставщикам
		ГДЕ
			Активность
			И Регистратор В (&amp;Регистратор)
			И ВидДвижения = ЗНАЧЕНИЕ(ВидДВиженияНакопления.Расход)
			И ЗаказыПоставщикам.ЗаказПоставщику В
				(ВЫБРАТЬ
					ТаблицаЗаказов.Ссылка КАК Ссылка
				ИЗ
					ТаблицаЗаказов КАК ТаблицаЗаказов)
			И КодСтроки = 0) КАК ТаблицаЗаказовСверхЗаказа
	СГРУППИРОВАТЬ ПО
		ТаблицаЗаказовСверхЗаказа.ЗаказПоставщику,
		ТаблицаЗаказовСверхЗаказа.Номенклатура,
