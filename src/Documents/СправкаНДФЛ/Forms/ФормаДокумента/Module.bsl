&НаСервере
Процедура ОбновитьНалогиНаСервере()
	
	ОчиститьРеквизитыХраненияОшибокДанныхОНалогах();
	
	Объект.СведенияОДоходах.Очистить();
	Объект.СведенияОВычетах.Очистить();
	Объект.УведомленияНОоПравеНаВычеты.Очистить();
	
	ПараметрыСправокНДФЛ = СправкиНДФЛ.ПараметрыСправокНДФЛ();

	ПараметрыСправокНДФЛ.СведенияОДоходах 					= Объект.СведенияОДоходах;
	ПараметрыСправокНДФЛ.СведенияОВычетах 					= Объект.СведенияОВычетах;
	ПараметрыСправокНДФЛ.НалоговыйПериод					= Объект.НалоговыйПериод;
	ПараметрыСправокНДФЛ.ДатаДокумента 						= КонецДня(Объект.Дата);
	ПараметрыСправокНДФЛ.Организация 						= Объект.Организация;
	ПараметрыСправокНДФЛ.РегистрацияВНалоговомОргане		= РегистрацияВНалоговомОргане(); 
	ПараметрыСправокНДФЛ.ИспользоватьНомераСправок 			= Ложь;
	ПараметрыСправокНДФЛ.ЗаполнятьБезДоходовПоЦеннымБумагам = НеВключатьДоходыПоЦеннымБумагам;
	ПараметрыСправокНДФЛ.УведомленияНОоПравеНаВычеты 		= Объект.УведомленияНОоПравеНаВычеты;

	СправкиНДФЛФормы.ОбновитьНалоги(Объект, ПараметрыСправокНДФЛ);	
	
КонецПроцедуры

&НаСервере
Процедура ЗаполнитьДанныеСправкиНаСервере()
	
	ОчиститьРеквизитыХраненияОшибокДанныхОНалогах();
	ОчиститьРеквизитыХраненияОшибокДанныеСотрудника();
	
	Если Не Объект.ФиксНалоги Тогда
		Объект.СведенияОДоходах.Очистить();
		Объект.СведенияОВычетах.Очистить();
		Объект.УведомленияНОоПравеНаВычеты.Очистить();
	КонецЕсли;
	
	ДанныеСправок = Новый Массив;
	
	ДанныеСправок.Добавить(Объект);
	
	УстановитьИнфонадписьОписаниеДоходовОрганизации();

	ПараметрыСправокНДФЛ = СправкиНДФЛ.ПараметрыСправокНДФЛ();
	
	ПараметрыСправокНДФЛ.СведенияОДоходах 					= Объект.СведенияОДоходах;
	ПараметрыСправокНДФЛ.СведенияОВычетах 					= Объект.СведенияОВычетах;
	ПараметрыСправокНДФЛ.НалоговыйПериод 					= Объект.НалоговыйПериод;
	ПараметрыСправокНДФЛ.ДатаДокумента 						= КонецДня(Объект.Дата);
	ПараметрыСправокНДФЛ.Организация 						= Объект.Организация;
	ПараметрыСправокНДФЛ.РегистрацияВНалоговомОргане 		= РегистрацияВНалоговомОргане(); 
	ПараметрыСправокНДФЛ.ИспользоватьНомераСправок 			= Ложь;
	ПараметрыСправокНДФЛ.ЗаполнятьБезДоходовПоЦеннымБумагам = НеВключатьДоходыПоЦеннымБумагам;
	ПараметрыСправокНДФЛ.УведомленияНОоПравеНаВычеты 		= Объект.УведомленияНОоПравеНаВычеты;
	
	СправкиНДФЛ.ПрочитатьДанные(ДанныеСправок, ПараметрыСправокНДФЛ, Истина);
	
	УстановитьПредставлениеАдресов(ЭтотОбъект);
	
	СправкиНДФЛФормыКлиентСервер.УстановитьСвойстваЭлементовСФиксациейДанных(ЭтотОбъект, Объект, ДокументПроведен);
	СправкиНДФЛФормыКлиентСервер.УстановитьИнфонадписьИсправления(ИнфоНадписьИсправления, Объект, ДокументПроведен);
	СправкиНДФЛФормыКлиентСервер.УстановитьПризнакНаличияГражданства(ЭтотОбъект, Объект);
	
КонецПроцедуры

