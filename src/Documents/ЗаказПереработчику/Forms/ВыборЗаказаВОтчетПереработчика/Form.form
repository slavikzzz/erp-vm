    <extInfo xsi:type="form:DynamicListExtInfo">
      <queryText>
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ДокЗаказПереработчику.Ссылка КАК Ссылка,
	ДокЗаказПереработчику.Организация КАК Организация,
	ДокЗаказПереработчику.Партнер КАК Партнер,
	ДокЗаказПереработчику.Номер КАК НомерРаспоряжения,
	ДокЗаказПереработчику.Дата КАК ДатаРаспоряжения,
	ДокЗаказПереработчику.Статус КАК Статус,
	ДокЗаказПереработчику.Приоритет КАК Приоритет,
	ДокЗаказПереработчику.Подразделение КАК Подразделение,
	ДокЗаказПереработчику.ПометкаУдаления КАК ПометкаУдаления,
	ДокЗаказПереработчику.НаправлениеДеятельности КАК НаправлениеДеятельности
{ВЫБРАТЬ
	Ссылка.*,
	Организация.* КАК Организация,
	Партнер.* КАК Партнер,
	НомерРаспоряжения,
	ДатаРаспоряжения,
	Статус.*,
	Приоритет.*,
	Подразделение.*,
	ПометкаУдаления,
	НаправлениеДеятельности.*}
ИЗ
	(ВЫБРАТЬ
		ЗаказыПоставщикам.ЗаказПоставщику КАК Распоряжение,
		0 КАК Поступило,
		ЗаказыПоставщикам.КОформлениюОстаток КАК Заказано
	ИЗ
		РегистрНакопления.ЗаказыПоставщикам.Остатки(, ТИПЗНАЧЕНИЯ(ЗаказПоставщику) = ТИП(Документ.ЗаказПереработчику)) КАК ЗаказыПоставщикам
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ТоварыПолученныеОтПереработчикаОстатки.Распоряжение,
		ВЫБОР
			КОГДА ТоварыПолученныеОтПереработчикаОстатки.КоличествоОстаток > 0
					И ТоварыПолученныеОтПереработчикаОстатки.КодСтроки &lt;> 0
				ТОГДА ТоварыПолученныеОтПереработчикаОстатки.КоличествоОстаток
			ИНАЧЕ 0
		КОНЕЦ,
		ВЫБОР
			КОГДА ТоварыПолученныеОтПереработчикаОстатки.КоличествоОстаток &lt; 0
				ТОГДА ТоварыПолученныеОтПереработчикаОстатки.КоличествоОстаток
			ИНАЧЕ 0
		КОНЕЦ
	ИЗ
		РегистрНакопления.ТоварыПолученныеОтПереработчика.Остатки(, ТИПЗНАЧЕНИЯ(Распоряжение) = ТИП(Документ.ЗаказПереработчику) {(Организация).*}) КАК ТоварыПолученныеОтПереработчикаОстатки
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ТоварыПолученныеОтПереработчика.Распоряжение,
		ВЫБОР
			КОГДА ТоварыПолученныеОтПереработчика.ВидДвижения = ЗНАЧЕНИЕ(ВидДвиженияНакопления.Приход)
				ТОГДА -ТоварыПолученныеОтПереработчика.Количество
			ИНАЧЕ ТоварыПолученныеОтПереработчика.Количество
		КОНЕЦ,
		0
	ИЗ
		РегистрНакопления.ТоварыПолученныеОтПереработчика КАК ТоварыПолученныеОтПереработчика
	ГДЕ
		ТоварыПолученныеОтПереработчика.Регистратор = &amp;Регистратор
		И ТоварыПолученныеОтПереработчика.Активность
	{ГДЕ
		ТоварыПолученныеОтПереработчика.Организация.*}) КАК ВложенныйЗапрос
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.ЗаказПереработчику КАК ДокЗаказПереработчику
		ПО (ДокЗаказПереработчику.Ссылка = ВложенныйЗапрос.Распоряжение)
{ГДЕ
	ДокЗаказПереработчику.Ссылка.*,
	ДокЗаказПереработчику.Организация.*,
	ДокЗаказПереработчику.Партнер.*,
	ДокЗаказПереработчику.Номер,
	ДокЗаказПереработчику.Дата,
	ДокЗаказПереработчику.Статус.*,
	ДокЗаказПереработчику.Приоритет.*,
	ДокЗаказПереработчику.Подразделение.*,
	ДокЗаказПереработчику.ПометкаУдаления,
	ДокЗаказПереработчику.НаправлениеДеятельности.*}

СГРУППИРОВАТЬ ПО
	ВложенныйЗапрос.Распоряжение,
	ДокЗаказПереработчику.Ссылка,
	ДокЗаказПереработчику.Организация,
	ДокЗаказПереработчику.Партнер,
	ДокЗаказПереработчику.Номер,
	ДокЗаказПереработчику.Дата,
	ДокЗаказПереработчику.Статус,
	ДокЗаказПереработчику.Приоритет,
	ДокЗаказПереработчику.Подразделение,
	ДокЗаказПереработчику.ПометкаУдаления,
	ДокЗаказПереработчику.НаправлениеДеятельности

ИМЕЮЩИЕ
	(СУММА(ВложенныйЗапрос.Поступило) &lt;> 0
		ИЛИ СУММА(ВложенныйЗапрос.Заказано) &lt;> 0)</queryText>
