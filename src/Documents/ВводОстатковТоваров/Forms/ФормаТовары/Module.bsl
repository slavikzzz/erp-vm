			КонецЕсли;
			//-- НЕ УТКА
			
			СтруктураДействий.Вставить("ПересчитатьСуммуБезНДС");
			СтруктураДействий.Вставить("ПересчитатьСуммуРегл", КоэффициентПересчетаИзВалютыУпрВРегл);
			Если НЕ (ЭтоОстаткиПринятойТары
