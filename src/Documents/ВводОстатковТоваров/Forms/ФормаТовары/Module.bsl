		ЭлементЗаказ = Элементы.ПолуфабрикатыДавальцаЗаказ;
		
	ИначеЕсли Объект.ХозяйственнаяОперация = ПредопределенноеЗначение("Перечисление.ХозяйственныеОперации.ВводОстатковПродукцииДавальца2_5") Тогда
		
		ЭлементЗаказ = Элементы.ПродукцияДавальцаЗаказ;
		
	Иначе
		Возврат;
	КонецЕсли;
	
	ЭлементЗаказ.Доступность = Форма.ПоЗаказу;
	
	Если Не Форма.ПоЗаказу Тогда
		
		Объект.Заказ = Неопределено;
		
		ЗаполнитьНазначениеКлиентСервер(Форма);
		
	КонецЕсли;
	//-- НЕ УТКА
	
	Возврат;
	
