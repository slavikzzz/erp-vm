	Если Запрос.Параметры.Свойство("ВидЗапасовПоУмолчанию") Тогда
		Возврат;
	КонецЕсли;
	
	ВидЗапасовПоУмолчанию = Справочники.ВидыЗапасов.ВидЗапасовДокумента(Запрос.Параметры.Организация, Неопределено);
	Запрос.УстановитьПараметр("ВидЗапасовПоУмолчанию", ВидЗапасовПоУмолчанию);
	
	//++ НЕ УТКА
	ЗапросВидыЗапасов = Новый Запрос;
	ЗапросВидыЗапасов.Текст =
	"ВЫБРАТЬ
	|	ТаблицаТоваров.Ссылка             КАК Ссылка,
	|	ТаблицаТоваров.ОрганизацияДавалец КАК ОрганизацияДавалец
	|ИЗ
	|	Документ.ПроизводствоБезЗаказа.ВыходныеИзделия КАК ТаблицаТоваров
	|ГДЕ
	|	ТаблицаТоваров.Ссылка = &Ссылка
	|	И ТаблицаТоваров.ВнутренняяПереработка
	|
	|СГРУППИРОВАТЬ ПО
	|	ТаблицаТоваров.Ссылка,
	|	ТаблицаТоваров.ОрганизацияДавалец
	|";
	ЗапросВидыЗапасов.УстановитьПараметр("Ссылка", Запрос.Параметры.Ссылка);
	
	ВидыЗапасовПоУмолчанию = Новый ТаблицаЗначений();
	ВидыЗапасовПоУмолчанию.Колонки.Добавить("Ссылка", Новый ОписаниеТипов("ДокументСсылка.ПроизводствоБезЗаказа"));
