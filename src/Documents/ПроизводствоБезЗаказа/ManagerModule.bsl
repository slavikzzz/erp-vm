Функция ТекстЗапросаВтОснований(Запрос, ТекстыЗапроса)
	
	ИмяРегистра = "ВтОснований";
	
	ТекстЗапроса = 
	"ВЫБРАТЬ
	|	Подразделения.Ссылка							КАК Ссылка,
	|	Подразделения.ХозяйственнаяОперация 			КАК ХозяйственнаяОперация,
	|	Подразделения.Организация						КАК Организация,
	|	Подразделения.Подразделение						КАК Подразделение,
	|	МИНИМУМ(Подразделения.ДополнительнаяЗапись)		КАК ДополнительнаяЗапись
	|ПОМЕСТИТЬ ВтОснований
	|ИЗ
	|	(
	// Выпускающее подразделение
	|	ВЫБРАТЬ
	|		ДанныеДокумента.Ссылка											КАК Ссылка,
	|		ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукции)	КАК ХозяйственнаяОперация,
	|		ДанныеДокумента.Организация										КАК Организация,
	|		&Подразделение													КАК Подразделение,
	|		ЛОЖЬ															КАК ДополнительнаяЗапись
	|	ИЗ
	|		Документ.ПроизводствоБезЗаказа КАК ДанныеДокумента
	|	ГДЕ
	|		ДанныеДокумента.Ссылка = &Ссылка
	//++ НЕ УТКА
	|	
	|	ОБЪЕДИНИТЬ ВСЕ
	|
	// Внутренняя переработка
	|	ВЫБРАТЬ
	|		ДанныеДокумента.Ссылка											КАК Ссылка,
	|		ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукции)	КАК ХозяйственнаяОперация,
	|		ДанныеДокумента.ОрганизацияДавалец								КАК Организация,
	|		&Подразделение													КАК Подразделение,
	|		ИСТИНА															КАК ДополнительнаяЗапись
	|	ИЗ
	|		Документ.ПроизводствоБезЗаказа.ВыходныеИзделия КАК ДанныеДокумента
	|	ГДЕ
	|		ДанныеДокумента.Ссылка = &Ссылка
	|		И ДанныеДокумента.ВнутренняяПереработка
	//-- НЕ УТКА
	|	
	|	ОБЪЕДИНИТЬ ВСЕ
	|
	// Получатель возвратных отходов
	|	ВЫБРАТЬ
	|		ДанныеДокумента.Ссылка											КАК Ссылка,
	|		ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.ВыпускПродукции)	КАК ХозяйственнаяОперация,
	|		ВЫБОР
	//++ НЕ УТКА
	|			КОГДА НЕ ДанныеДокумента.ВладелецИзделия = ЗНАЧЕНИЕ(Справочник.Организации.ПустаяСсылка)
	|				ТОГДА ДанныеДокумента.ВладелецИзделия
	|			КОГДА СпрПартииПроизводства.ВнутренняяПереработка
	|				ТОГДА СпрПартииПроизводства.Организация
	//-- НЕ УТКА
	|			КОГДА ИСТИНА
	|				ТОГДА ДанныеДокумента.Ссылка.Организация
	|		КОНЕЦ															КАК Организация,
	|		ДанныеДокумента.Подразделение									КАК Подразделение,
	|		ИСТИНА															КАК ДополнительнаяЗапись
	|	ИЗ
	|		Документ.ПроизводствоБезЗаказа.ПобочныеИзделия КАК ДанныеДокумента
	|		
	//++ НЕ УТКА
	|		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.ПартииПроизводства КАК СпрПартииПроизводства
	|		ПО СпрПартииПроизводства.Документ = ДанныеДокумента.Ссылка
	|		И СпрПартииПроизводства.Код = ДанныеДокумента.НомерГруппыЗатрат
	|		И НЕ СпрПартииПроизводства.ПометкаУдаления
	|	
	//-- НЕ УТКА
	|	ГДЕ
	|		ДанныеДокумента.Ссылка = &Ссылка
	|		И НЕ ДанныеДокумента.Подразделение = &Подразделение
	|
	|	ОБЪЕДИНИТЬ ВСЕ
	|
	// Подразделение материалов
	|	ВЫБРАТЬ
	|		ДанныеДокумента.Ссылка																КАК Ссылка,
	|		ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.СписаниеРасходовНаПартииПроизводства)	КАК ХозяйственнаяОперация,
	|		ДанныеДокумента.ВидЗапасов.Организация												КАК Организация,
	|		ДанныеДокумента.Подразделение														КАК Подразделение,
	|		ИСТИНА																				КАК ДополнительнаяЗапись
	|	ИЗ
	|		Документ.ПроизводствоБезЗаказа.ВидыЗапасовМатериалыИРаботы КАК ДанныеДокумента
	|	ГДЕ
	|		ДанныеДокумента.Ссылка = &Ссылка
	|		И НЕ ДанныеДокумента.Подразделение = &Подразделение
	|
	|	ОБЪЕДИНИТЬ ВСЕ
	|
	// Подразделение работ
	|	ВЫБРАТЬ
	|		ДанныеДокумента.Ссылка																КАК Ссылка,
	|		ЗНАЧЕНИЕ(Перечисление.ХозяйственныеОперации.СписаниеРасходовНаПартииПроизводства)	КАК ХозяйственнаяОперация,
	|		&Организация																		КАК Организация,
	|		ДанныеДокумента.Подразделение														КАК Подразделение,
	|		ИСТИНА																				КАК ДополнительнаяЗапись
	|	ИЗ
	|		Документ.ПроизводствоБезЗаказа.МатериалыИРаботы КАК ДанныеДокумента
	|	ГДЕ
	|		ДанныеДокумента.Ссылка = &Ссылка
	|		И НЕ ДанныеДокумента.Подразделение = &Подразделение
	|		И ДанныеДокумента.Номенклатура.ТипНоменклатуры = ЗНАЧЕНИЕ(Перечисление.ТипыНоменклатуры.Работа)
	|	) КАК Подразделения
	|
	|СГРУППИРОВАТЬ ПО
	|	Подразделения.Ссылка,
	|	Подразделения.Организация,
	|	Подразделения.Подразделение,
	|	Подразделения.ХозяйственнаяОперация
	|";
	
	ТекстыЗапроса.Добавить(ТекстЗапроса, ИмяРегистра);
	Возврат ТекстЗапроса;
	
