<package xmlns="http://v8.1c.ru/8.1/xdto" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" targetNamespace="http://www.fss.ru/integration/types/organization/v01" elementFormQualified="true" attributeFormQualified="false">
	<valueType name="OgrnMixType" base="xs:string" variety="Atomic" minLength="13" maxLength="15">
		<pattern>([0-9])*</pattern>
	</valueType>
	<valueType name="OgrnType" base="xs:string" variety="Atomic">
		<pattern>([0-9]{15}|[0-9]{13})</pattern>
	</valueType>
	<valueType name="fullName" base="xs:string" variety="Atomic" maxLength="4000"/>
	<valueType name="inn" base="xs:string" variety="Atomic">
		<pattern>\d{10}</pattern>
		<pattern>\d{12}</pattern>
	</valueType>
	<valueType name="kpp" base="xs:string" variety="Atomic">
		<pattern>\d{9}</pattern>
	</valueType>
	<valueType name="kpsNum" base="xs:string" variety="Atomic" length="5">
		<pattern>([0-9])*</pattern>
	</valueType>
	<valueType name="ogrn" base="xs:string" variety="Atomic" length="13">
		<pattern>([0-9])*</pattern>
	</valueType>
	<valueType name="ogrnip" base="xs:string" variety="Atomic" length="15">
		<pattern>([0-9])*</pattern>
	</valueType>
	<valueType name="regNum" base="xs:string" variety="Atomic">
		<pattern>\d{10}</pattern>
	</valueType>
</package>